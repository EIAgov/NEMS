			}
			Set MyConstraints {
				SubsetOf: AllConstraints;
				Definition: {
					Data {SupplyMassBalance,
					SupplyAccounting,
					HubBalance,
					TariffCurveQtyBalance,
					DemandMassBalance,
					StorageInjBalance,
