!  INDEX VARIABLES
      INTEGER I   !  WELL CLASS INDEX VARIABLE
      INTEGER R   !  REGION INDEX VARIABLE
      INTEGER K   !  FUEL TYPE INDEX VARIABLE
      INTEGER T   !  YEAR INDEX VARIABLE
      INTEGER F   !  ALASKAN FIELD VARIABLE
      INTEGER L   !  TEMPORARY INDEX VARIABLE
      INTEGER M   !  TEMPORARY INDEX VARIABLE
      INTEGER N   !  TEMPORARY INDEX VARIABLE
      INTEGER H   !  CANADIAN BORDER INDEX VARIABLE
      INTEGER J   !  MEXICAN BORDER INDEX VARIABLE
      INTEGER PP	  !  Padd counter

      INTEGER*4 IFILE1,IFILE2,IFILE3,IFILE4,IFILE5,IFILE6,IFILE7,IFILE8
      INTEGER*4 OFILE1,OFILE2,OFILE3
      INTEGER*4 SYSOUT      ! SYSTEM OUTPUT UNIT ID
      INTEGER*4 VAROUT      ! SYSTEM OUTPUT UNIT ID
      INTEGER*4 OGBUG1      ! SYSTEM OUTPUT UNIT ID
      INTEGER*4  BASIC                           ! INPUT FILE NUMBER
      INTEGER*4  WLUND                           ! INPUT FILE NUMBER
      INTEGER*4  WLRES                           ! INPUT FILE NUMBER
      INTEGER*4  UNOUT1                          ! OUTPUT FILE NUMBER
      INTEGER*4  DBOUT                           ! OUTPUT FILE NUMBER
      INTEGER*4  IOERR                           ! I/O ERROR NUMBER

      INTEGER OGRUNOP(22)  ! OGSM RUN OPTION FLAG VARIABLE
      INTEGER OGREPORT(30) ! OGSM REPORTING FLAG VARIABLE

!  DECLARATIONS NEEDED TO INVOKE FILE_MGR
      LOGICAL NEW         ! IS FILE NEW (.TRUE.) OR OLD (.FALSE.)
      CHARACTER*18 FNAME  ! 18 CHARS OR MORE
      INTEGER FILE_MGR    ! DECLARATION OF FILE_MGR AS INTEGER FCTN
      EXTERNAL FILE_MGR   ! DESIGNATE THAT FILE_MGR IS A SUBPROGRAM

      INTEGER DATAYR        ! OGSM BASE DATA YEAR

      REAL FEDTXR           ! FEDERAL CORPORATE TAX RATE
      REAL ROYRT            ! ROYALTY RATE
      REAL DISC             ! DISCOUNT RATE
      REAL INFL             ! INFLATION RATE
      REAL DLADJ            ! INFLATION RATE
      REAL DEBTRATIO        ! DEBT RATIO
      REAL OGBETA           ! INDUSTRY BETA
      REAL OGMRP            ! MARKET RISK PREMIUM
      REAL OGPROD(30) ! PROJECT PRIMARY FUEL PRODUCTION VECTOR
      REAL OGCOPROD(30) ! PROJECT COPRODUCT PRODUCTION VECTOR

!  OGSM SYSTEM PRICE VECTORS BY REGION/FUELTYPE/PROJECT LIFE
      REAL OGPRCL48(L48RGN,L48FUEL,30) ! SYSTEM PRICE VECTOR
      REAL OGPRCREF(MNUMPR,MNCRUD,30) ! SYSTEM PRICE VECTOR
      REAL OGPRCAK(AKRGN,AKFUEL,30)    ! SYSTEM PRICE VECTOR
      REAL OGPRCOFF(OFFRGN,OFFFUEL,30) ! SYSTEM PRICE VECTOR
      REAL REPPRCL48(L48RGN,L48FUEL,MNUMYR) ! REPORTING PRICE ARRAY
      REAL REPPRCOFF(OFFRGN,OFFFUEL,MNUMYR) ! REPORTING PRICE ARRAY
      REAL REPPRCAK(AKRGN,AKFUEL,MNUMYR)    ! REPORTING PRICE ARRAY

!  NATIONAL EXPENDITURE VARIABLES
      REAL DCFOFFLAG(OFFWELL,OFFRGN,OFFFUEL)! (T-1) LAG EXP & DEV OFF WT. DCF
      REAL DCFOFFLAG2(OFFWELL,OFFRGN,OFFFUEL)! (T-2) LAG EXP & DEV OFF WT. DCF
      REAL DCFL48LAG(L48WELL,L48RGN,L48FUEL)! (T-1) LAG L48 WT. DCF I,R,K
      REAL DCFL48LAG2(L48WELL,L48RGN,L48FUEL)! (T-2) LAG L48 WT. DCF I,R,K
      REAL DCFL48LAG3(L48WELL,L48RGN,L48FUEL)! (T-3) LAG L48 WT. DCF I,R,K
      REAL SPENDLAGOFF(2)   ! (T-1) LAG EXP & DEV OFFSHORE EXPEND
      REAL SPEXLAGOFF2      ! (T-2) LAG EXPLOR OFFSHORE EXPEND

      REAL SPEND_ON(L48WELL)  ! NATIONAL ONSHORE DRILLING EXPEND.
      REAL SPEND_L48(L48WELL) ! L48 ONSHORE DRILLING EXPENDITURES
      REAL SPEND_OFF(OFFWELL) ! OFFSHORE DRILLING EXPENDITURES
      REAL SPEND_AK(AKWELL)   ! ALASKA DRILLING EXPENDITURES
      REAL SPEND_EOR          ! ENHANCED OIL RECOVERY EXPENDITURES
      REAL SPEND_RGN(L48WELL,L48RGN) ! REGIONAL ONSH DRILLING EXPEND.
      REAL SPENDOFF_RGN(OFFWELL,OFFRGN) ! REGIONAL DRILLING EXPEND.

      INTEGER TECHYR       ! FIRST YEAR OF ADJUSTED TECH FACTOR

!  MDR ADJUSTMENT FACTORS
      REAL COSTADJ         ! DRILLING COST ADJUSTMENT FACTOR
      REAL PRNEWADJ        ! EXTRACTION RATE ADJUSTMENT FACTOR
      REAL DRLADJ          ! DRILLING RESPONSE ADJUSTMENT FACTOR

!  AD GAS ECONOMETRIC PARAMETERS
      REAL ALPHA_AD(9), BETA_AD(9)

!  SHORT RUN SUPPLY CURVES
      REAL OLPELC(12)
      REAL OLEOYRS(12)
      REAL OLPRR(12)
      REAL OLALPHA(12)
      REAL OLBETA(12)
      REAL OLWHP(12)
      REAL EI2PSA(9)

!  FE TECHNOLOGY ADJUSTMENTS
      REAL FETECH_CONV(19,2)
      REAL FEADJ_CONV(2)
      REAL FEIPRD_CONV(L48RGN,3)
      REAL FECOST_CONV(2,4)
      REAL FETECH_UGR(11,6)
      REAL FEDRILL_OIL(2,9,MNUMYR)
      REAL FEOPR_OIL(2,9,MNUMYR)
      REAL FEDRILL_GAS(2,9,MNUMYR)
      REAL FEOPR_GAS(2,9,MNUMYR)
      REAL FEDRILL_CBM(2,9,MNUMYR)
      REAL FEOPR_CBM(2,9,MNUMYR)
      REAL FEDRILL_TGS(2,9,MNUMYR)
      REAL FEOPR_TGS(2,9,MNUMYR)
      REAL FEDRILL_SHL(2,9,MNUMYR)
      REAL FEOPR_SHL(2,9,MNUMYR)
      INTEGER FEYEAR_UGR(4,6)

      COMMON/OGBFW1/OGREPORT,T,DATAYR,FEDTXR,ROYRT,DISC,OGRUNOP, &
