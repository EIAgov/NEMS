            WLRES,UNOUT1,IOERR,VAROUT,DBOUT,OGBUG1

      COMMON/OGBFW10/TECHYR
      COMMON/OGBFW11/COSTADJ,PRNEWADJ,DRLADJ
      COMMON/OGBFW12/ALPHA_AD,BETA_AD
      COMMON/OGBFW13/OLPELC,OLEOYRS,OLPRR,OLALPHA,OLBETA,OLWHP, &
                     EI2PSA
      COMMON/OGBFW14/FETECH_CONV,FEADJ_CONV,FEIPRD_CONV,FECOST_CONV, &
                     FETECH_UGR,FEYEAR_UGR
      COMMON/OGBFW15/FEDRILL_OIL,FEDRILL_GAS,FEDRILL_CBM, &
                     FEDRILL_TGS,FEDRILL_SHL, &
                     FEOPR_OIL,FEOPR_GAS,FEOPR_CBM,FEOPR_TGS,FEOPR_SHL
! STEO BENCHMARKING
      INTEGER STEOYRS
      INTEGER ogSTEO
      REAL*8 sDCRDWHP(MNUMOR,3)
      REAL*8 sRFQTDCRD(MNUMOR+2,3)
      REAL*8 sNGPLPRD(3)
      REAL*8 sNGPLET(3)
      REAL*8 sNGPLPR(3)
      REAL*8 sNGPLBU(3)
      REAL*8 sNGPLIS(3)
      REAL*8 sNGPLPP(3)
      REAL*8 tOGQSHLOIL(15,3)     ! Crude oil production from select shale oil plays
      REAL*8 tOGQSHLGAS(15,3)     ! Natural gas production from select shale gas plays
      REAL*8 tDCRDWHP(MNUMOR,mnumyr)
      REAL*8 tRFQTDCRD(MNUMOR+2,mnumyr)
      REAL*8 sOGENAGPRD(OGDIST,2)
      REAL*8 sOGADGPRD(OGDIST,2)

      COMMON/OGBFW25r/sDCRDWHP,sRFQTDCRD,sNGPLPRD,sNGPLET,sNGPLPR, &
