      COMMON/OGBFW2/ &
         DCFOFFLAG,DCFOFFLAG2, &
         SPENDLAGOFF,SPEXLAGOFF2, &
         SPEND_ON,SPEND_L48,SPEND_OFF,SPEND_AK,SPEND_EOR, &
         SPEND_RGN, &
         DCFL48LAG,DCFL48LAG2,DCFL48LAG3


      COMMON/OGBFW4/OGPRCL48,OGPRCOFF,OGPRCAK,OGPRCREF

      COMMON/OGBFW6/REPPRCL48,REPPRCOFF,REPPRCAK

      COMMON/OGBFW5/IFILE1,IFILE2,IFILE3,IFILE4,IFILE5,IFILE6, &
