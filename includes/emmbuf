! $Header: m:/default/includes/RCS/emmbuf,v 1.4 2014/08/22 13:01:41 xj2 Exp $
!CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
!  PURPOSE: EMMBUF HAS DATA STRUCTURES TO LOAD DATA TABLE IN BUFFER
!  BY RD$TBL FUNCTION AND TO LOAD VARIABLES FROM TABLE IN
!  RD$I*,RD$R* AND RD$C* FUNCTIONS IN EMM
!  AUTHOR: WAYNE COOPER
!  DATA CREATED: 3/3/95
!  MODIFICATIONS:  STDERR AND MSG ADDED - 6/13/95 WCW
!CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
      COMMON /EMMBUF/ INFLL,DBUG,DBUGFIL,TABROWS,COL,MSG,STDERR,  & ! INT
                      BLKLAB,INTAB                               ! CHAR
      INTEGER*4 INFLL   ! INPUT DATA FILE UNIT #
      INTEGER*4 DBUG    ! DEBUG SWITCH
      INTEGER*4 DBUGFIL ! DEBUG FILE UNIT #
      INTEGER*4 TABROWS ! NUMBER OF ROWS IN DATA TABLE
      INTEGER*4 COL     ! CURRENT COLUMN INDEX
      INTEGER*4 NLN     ! MAXIMUM NUMBER OF ROWS IN DATA TABLE
      INTEGER*4 MSG     ! MESSAGE UNIT # FOR NON_CRITICAL WARNINGS AND ERRORS
      INTEGER*4 STDERR  ! UNIT # FOR ALL UNAMBIGUOUSLY CRITICAL ERROR MESSAGES
      PARAMETER (NLN = 1200)
!
      CHARACTER*12  BLKLAB  ! DATA TABLE LABEL IN FILE
      CHARACTER*240  INTAB(NLN) ! DATA BUFFER FOR DATA TABLE
!
