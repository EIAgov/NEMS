!  LNG VARIABLE DECLARTIONS

! USED BY OLD LNG DATA FORMAT READS ONLY
      REAL XFPRDCST(MNUMYR,MAXU)        ! FOREIGN PRODUCTION COSTS
      REAL XLIQCST(STAGE,MNUMYR,MAXU)   ! LIQUEFACTION COSTS
      REAL XREGASCST(STAGE,MNUMYR,MAXW) ! REGASIFICATION COSTS

! USED BY BOTH NEW & OLD DATA FORMAT READS
      REAL FPRDCST(MAXU)                ! FOREIGN PRODUCTION COSTS
      REAL LIQCST(STAGE,MAXU)           ! LIQUEFACTION COSTS
      REAL REGASCST(STAGE,MAXW)         ! REGASIFICATION COSTS

      REAL TRANCST(MAXU,MAXW)           ! TRANSPORATION COSTS
      REAL COSTS(MAXU,MAXW)             ! DELIVERED COSTS
      REAL LIQCAP(STAGE,MAXU)           ! LIQUEFACTION CAPACITY
      REAL REGASCAP(MAXW)               ! INITIAL REGAS CAPACITY
      REAL PROFIT(MAXPAIRS)             ! PROFITABILITIES
      REAL PRICELNG(MNUMYR,MAXW)       ! CITY GATE PRICES
      REAL QLNG(MNUMYR,MAXW)           !FLOW CAPACITY BY DESTINATION
      REAL QLNGMAX(MNUMYR,MAXW)        !MAX. CAPACITY BY DESTINATION
      REAL REGASEXPAN(STAGE,MAXW)       ! REGASIFICATION CAPACITY EXPANSION

      REAL LNGUTZ(MNUMYR,MAXW)         ! UTILIZATION OF REGAS. CAPACITY
      REAL LNGMAXCAP(MAXW)              ! MAX CAPACITY BY DESTINATION

      INTEGER STARTLAG(STAGE,MAXW)! LAGGED TIME UNTIL EXPANSION
      INTEGER REGASSTAGE(MAXW)  ! REGASIFICATION STAGE BY DESTINATION
      INTEGER LIQSTAGE(MAXU)    ! LIQUEFACTION STAGE BY SOURCE
      INTEGER BUILDLAG(MAXW)    ! LAGGED TIME UNTIL BUILD DATE
      INTEGER WMAP(MAXPAIRS)    ! DESTINATION PAIRS
      INTEGER UMAP(MAXPAIRS)    ! SOURCE PAIRS
      INTEGER CURSTAR           ! CURRENT YEAR PLUS THE STARTLAG
      INTEGER S                 ! REGASIFICATION STAGES
      INTEGER X                 !
      INTEGER W                 !
      INTEGER U                 !

      INTEGER L_1STYR(STAGE,MAXU)      ! 1ST YEAR ALLOWED FOR LIQFAC. STAGE
      INTEGER R_1STYR(STAGE,MAXW)      ! 1ST YEAR ALLOWED FOR REGAS. STAGE
      INTEGER LNGHYR                   ! LAST YR OF HIST DATA FOR LNG

      COMMON/LNG1/FPRDCST,LIQCST,TRANCST,REGASCST,COSTS,LIQCAP,REGASCAP, &
                  PROFIT,PRICELNG,QLNG,QLNGMAX,REGASEXPAN,LNGUTZ, &
                  LNGMAXCAP,XFPRDCST,XLIQCST,XREGASCST
      COMMON/LNG2/STARTLAG,REGASSTAGE,LIQSTAGE,BUILDLAG,WMAP,UMAP, &
                  CURSTAR,S,X,W,U,L_1STYR,R_1STYR,LNGHYR

