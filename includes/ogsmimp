!  $Header: m:/default/includes/RCS/ogsmimp,v 1.36 2016/08/22 21:13:28 JBE Exp $

!------------------------------------------------
!  IMPORT PARAMETERS

      INTEGER*4   ICNHYR      ! NUMBER OF HISTORICAL YEARS FOR LNG MODULE
      PARAMETER(ICNHYR=25)

!------------------------------------------------
!  IMPORT BASE YEAR VARIABLE

      COMMON /OGBASVAR/ ICNBYR,RESBASYR,NCNMX
      INTEGER*4   ICNBYR        ! BASE YEAR TO RUN CANADA MODEL
      INTEGER*4   RESBASYR      ! YEAR ASSOC WITH RESOURCE BASE (BEG YR)
      INTEGER*4   NCNMX         ! SWITCH FOR RUNNING NEW CAN OR MEX 0=OLD METHOD, 1=NEW CN, 2=NEW MX, 3=BOTH NEW
!------------------------------------------------
!  CANADIAN IMPORT VARIABLE DECLARATIONS

      COMMON/OGFOR1/SUCWELL,CANPCAP

      REAL*4 SUCWELL                    ! NUMBER OF SUCCESSFUL GAS WELLS
      REAL*4 CANPCAP(MNUMYR)            ! PRODUCTION CAP (BASED ON PREVIOUS YR'S P/R)

!------------------------------------------------
!  SELECT WHICH EQUATION OPTION TO IMPLEMENT FOR CANADA WELLS, FINDING, AND P/R

      COMMON/OGFOR2/WLMETH,FRMETH,PRMETH

      INTEGER*4 WLMETH  ! IDENTIFY WHICH WELLS EQUATION TO USE 
      INTEGER*4 FRMETH  ! IDENTIFY WHICH FINDING RATE EQUATION TO USE
      INTEGER*4 PRMETH  ! IDENTIFY WHICH P/R RATIO EQUATION TO USE
      
!------------------------------------------------
!  CANADIAN RESERVE ADDITION VARIABLES

      COMMON/OGFOR5/FRCAN,REPFRCAN,CUMRCAN,RESTECH,RESBASE, &
        RESADCAN,RESBOYCAN,PRRATCAN,CURRESCAN, &
        CURPRRCAN,REPRSVCAN,REPWELLS,URRCAN,REPADDCAN, &
        REPPRDCAN,CNSTEOPR

      REAL*4 FRCAN               ! FINDING RATE
      REAL*4 REPFRCAN(MNUMYR)    ! CANADA FINDING RATE FOR REPORT
      REAL*4 CUMRCAN             ! CUMULATIVE RESERVE DISCOVERIES
      REAL*4 RESTECH             ! RESERVE TECHOLOGY FACTOR
      REAL*4 RESBASE             ! RECOVERABLE RESOURCES
      REAL*4 RESADCAN            ! RESERVE ADDITIONS
      REAL*4 RESBOYCAN           ! BOY RESERVES -- CANADA
      REAL*4 PRRATCAN            ! P/R RATIOS
      REAL*4 CURRESCAN           ! CURRENT YEAR RESERVES
      REAL*4 CURPRRCAN           ! LAG P/R RATIOS
      REAL*4 REPRSVCAN(MNUMYR)   ! RERSERVES FOR SUMMARY
      REAL*4 REPWELLS(MNUMYR)    ! WELLS FOR SUMMARY
      REAL*4 URRCAN              ! RECOVERABLE RESOURCES
      REAL*4 REPADDCAN(MNUMYR)   ! RESERVE ADDITIONS FOR SUMMARY
      REAL*4 REPPRDCAN(MNUMYR)   ! CANADA PRODUCTION FOR REPORT
      REAL*4 CNSTEOPR(STEO_YEAR) ! FACTORS TO SCALE P/R RATIO TO BENCH TO STEO

!------------------------------------------------
!  HISTORICAL OVERWRITES

      COMMON/OGboy/HISTRESCAN,HISTPRRCAN
      COMMON/OGadd/HISTRESAD
      COMMON/OGwell/HISTFRCAN,HISTWELCAN,HCUMSUCWEL

      REAL*4 HISTRESCAN(ICNHYR+1)         ! BOY RESERVES FOR CAN IN HIST YRS
      REAL*4 HISTPRRCAN(ICNHYR)           ! PR RATIO FOR CAN IN HIST YRS
      REAL*4 HISTRESAD(ICNHYR)            ! RESERVE ADDITIONS FOR CAN IN HIST YRS
      REAL*4 HISTFRCAN(ICNHYR)            ! FINDING RATE FOR CAN WELL IN HIST YRS
      REAL*4 HISTWELCAN(ICNHYR)           ! WELLS DRILLED IN CAN IN HIST YRS
      REAL*4 HCUMSUCWEL(ICNHYR+1)         ! HIST BOY CUM SUC WELLS

