! $Header: m:/default/includes/RCS/dsmdimen,v 1.9 2019/06/27 12:17:40 pkc Exp $
!============== (DSMDIMEN) ===================
! This common block contains all PARAMETER statements of LDSM module
! MAXSEC - NUMBER OF SECTORS
! RES - RESIDENTIAL SECTOR INDEX
! COM - COMMERCIA SECTOR INDEX
! IND - INDUSTRIAL SECTOR INDEX
! TRA - TRANSPORTATION SECTOR INDEX
! MAXCRG - MAXIMUM NUMBER OF CENSUS REGIONS
! MAXREC - MAXIMUM NUMBER OF RECORDS ON DAF-LSR-DB
! MAXMON - MAXIMUM NUMBER OF MONTHS
! MAXSEA - MAXIMUM NUMBER OF SEASONS
! MAXDTP - MAXIMUM NUMBER OF DAY-TYPES
! MAXNRG - MAXIMUM NUMBER OF NERC REGIONS
! MAXHOUR - MAXIMUM NUMBER OF HOURS IN A YEAR +1 = MAXMON*MAXDAY*24+1
! MAXRHOUR - MAXIMUM NUMBER OF HOURS IN A REAL YEAR
! MAXDAY - MAXIMUM NUMBER OF DAYS PER MONTH
! MAXEU  - MAXIMUM NUMBER OF END-USES
! MAXEUGRP  - MAXIMUM NUMBER OF END-USE GROUPS IN EACH SECTOR
! ELINDEX - INDEX FOR ELECTRICITY IN COMMERICAL DEMAND SUBMODULE
! ELHEATX - INDEX FOR ELECTRICITY IN RESIDENTIAL HEATING DEMAN TABEL
! ELWHEAX - INDEX FOR ELECTRICITY IN RESIDENTIAL WATER HEATING DEMAND TABLE
! ELCOOLX - INDEX FOR ELECTRICITY IN RESIDENTIAL COOLING DEMAND TABLE
! ELSTOVX - INDEX FOR ELECTRICITY IN RESIDENTIAL STOVES DEMAND TABLE
! ELDRYEX - INDEX FOR ELECTRICITY IN RESIDENTIAL DRYERS DEMAND TABLE
! ELSECHX - INDEX FOR ELECTRICITY IN RESIDENTIAL SECONDARY HEATING DEMAND TABLE
! ELLDVHX - INDEX FOR ELECTRICITY IN TRANSPORTATION, LIGHT DUTY ELECTR. VEHICLES
! ELRAILX - INDEX FOR ELECTRICITY IN TRANSPORTATION, RAILWAYS
! MAXECPSG - MAXIMUM NUMBER OF SEGMENTS IN ECP LDC DEFINITION
! MAXITV  - MAXIMUM NUMBER OF HOURLY INTERVALS PER SEGMNET IN ECP/EFD LDC DEF.
! MAXECPB - MAXIMUM NUMBER OF BLOCKS IN ONE SEGMENT OF ECP LDC DEF.
! MAXECTB - MAXIMUM TOTAL NUMBER OF BLOCKS IN ECP LDC = MAXECPSG*MAXECPB
! MAXEFDS - MAXIMUM NUMBER OF SEASONS USED BY EFD MODULE
! MAXEFDSS - MAXIMUM NUMBER OF SEGMENTS IN A SEASON
! MAXECPS - MAXIMUM NUMBER OF SEASONS USED BY ECP MODULE
! MAXEFDB - MAXIMUM NUMBER OF BLOCKS IN ONE SEASON LDC FOR EFD
! MAXEFDSG - MAXIMUM OF SEGMENTS IN ALL SEASONES IN EFD LDCS
! DISCFA - DISCOUNT FACTOR USED FOR COMPUTATIONS OF COSTS OF DSM OPTIONS
! UNCONFA - CONVERSION FACTOR FOR UNITS BETWEEN INPUT AND OUTPUT DEMAND DATA
! UNCONFAR - CONVERSION FACTOR FOR UNITS BETWEEN RESIDENTIAL INPUT DATA AND OUT.
! MAXEFTB  - MAXIMUM NUMBER OF BLOCKS IN EFD LDC (MAXEFDSG*MAXEFDB)
! MAXRBT - MAXIMUM NUMBER OF RESIDENTIAL BUILDING TYPES USED IN LDSM
! MAXCBT - MAXIMUM NUMBER OF COMMERCIAL BUILDING TYPES USED IN LDSM
! QREPYRH - NUMBER OF YEARS ON ONE LINE OF SOME LDSM REPORTS
! MAXRDT - MAXIMUM NUMBER OF DECISION TYPES FOR RESIDENTIAL SECTOR
!          USED IN LDSM
! MAXCOP - MAXIMUM NUMBER OF DSM OPTIONS CHOSEN BY ECP FRO COMMERCIALSECTOR
! MAXROP - MAXIMUM NUMBER OF DSM OPTIONS CHOSEN BY ECP FRO RESIDENTILA SECTOR
! MAXDSMP - MAXIMUM NUMBER OF DSM PROGRAMS FOR ONE REGION
! MAXDSMPR - MAXIMUM NUMBER OF DSM PROGRAMS FOR ONE REGION IN RESIDENTIAL SECTOR
! MAXDSMPC - MAXIMUM NUMBER OF DSM PROGRAMS FOR ONE REGION IN COMMERCIAL SECTOR
! MAXOPR - MAXIMUM NUMBER OF DSM OPTIONS PER DSM PROGRAM RESIDENTIAL
! MAXOPC - MAXIMUM NUMBER OF DSM OPTIONS PER DSM PROGRAM COMMERCIAL
! MAXTAF - MAXIMUM NUMBER OF TECHNOLOGIES AFFECTED BY ONE DSM PROGRAM
! MAXRDSMO - MAXMIMUM NUMBER OF RESIDENTIAL DSM OPTIONS IN OPTION DATA BASE
! MAXCDSMO - MAXIMUM NUMBER OF COMMERCIAL DSM OPTIONS IN OPTION DATA BASE
! MAXNLST - MAXIMUM NUMBER OF LISTS OF REGIONS TO DEFINE DSM OPTINS
! MAXRLST - MAXIMUM NUMBER OF REGIONS+1 ON EACH OF THE ABOVE LISTS
! MAXFRT - MAXIMUM NUMBER OF FROM TECHNOLOGIES AFFECTED BY ONE DSM OPTION
! MAXTOT - MAXIMUM NUMBER OF TO TECHNOLOGIES AFFECTED BY ONE DSM OPTION
! MAXRTECH - MAXIMUM NUMBER OF RESIDENTIAL TECHNOLOGIES TO BE AFFECTED BY DSM
! MAXCTECH - MAXIMUM NUMBER OF COMMERCIAL TECHNOLOGIES TO BE AFFECTED BY DSM
! MAXDMT - MAXIMUM NUMBER OF DEMAND MODULE TECHNOLOGIES AFFECTED  BY ONE
!          DSM OPTION
! MAXDECT - MAXIMUM NUMBER OF DECISION TYPES USED IN DSM OPTIONS
! MAXRRST - MAXIMUM NUMBER OF RECORDS ON RESTART FILE FOR ONE SECTOR
! NYRESTR - NUMBER OF YEARS ON RESIDENTIAL RESTART FILE
! NYRESTC - NUMBER OF YEARS ON COMMERCIAL RESTART FILE
! NYRESTR1 - NUMBER OF YEARS WITH DATA ON RESIDENTIAL RESTART FILE
! NYRESTC1 - NUMBER OF YEARS WITH DATA ON COMMERCIAL RESTART FILE
! MAXNTPO - MAXIMUM NUMBER OF DEMAND MODULE TECHNOLOGIES PER OPTION (TO+FROM)
! NUMCTCE - NUMBER OF ELEMENTS IN COMMERCIAL TECHNOLOGY CODE
! TNSEEFD - MAXIMUM TOTAL NUMBER OF SEGMENTS IN ALL SEASONS TOGETHER IN EFD LDC
! MSEGEFD - MAXIMUM NUMBER OF SEGMENTS PER SEASON IN EFD LDC
! MAXREU - MAXIMUM NUMBER OF RESIDENTIAL SECTOR END-USES
! MAXPCPH - MAXIMUM NUMBER OF COINCIDENT PEAK HOURS USED FOR "PCP PURPOUSES"
! MAXPACR - MAXIMUM NUMBER OF DATA POINTS DEFINING PAYBACK-ACCEPTANCE CURVES
! ECPTHR - ECP TIME HORIZON LENGHT IN NUMBER OF YEARS
! MAXCBIL - MAXIMUM NUMBER OF BUILDING TYPES IN COMMERCIAL SECTOR
! MAXCSRV - MAXIMUM NUMBER OF SERVICES IN COMMERCIAL SECTOR
! QDSMRPT - MAX NUMBER OF REPORTS GENERATED BY LDSM
!    NPROB  = NUMBER OF PROBABILITY BINS FOR NEW SMOOTHING
!    NMOM   = NUMBER OF MOMENTS USED IN GRAM-CHARLIER

      INTEGER*2 MAXCSRV
      INTEGER*2 MAXCBIL
      INTEGER*2 ECPTHR
      INTEGER*2 MAXPACR
      INTEGER*2 MAXPCPH
      INTEGER*2 MAXREU
      INTEGER*2 MSEGEFD
      INTEGER*2 TNSEEFD
      INTEGER*2 NUMCTCE
      INTEGER*2 MAXNTPO
      INTEGER*2 NYRESTR
      INTEGER*2 NYRESTC
      INTEGER*2 NYRESTR1
      INTEGER*2 NYRESTC1
      INTEGER*2 MAXRRST
      INTEGER*2 MAXDECT
      INTEGER*2 MAXDMT
      INTEGER*2 MAXRTECH,MAXCTECH
      INTEGER*2 MAXFRT,MAXTOT
      REAL*4    DISCFA
      INTEGER*2 MAXNLST
      INTEGER*2 MAXRLST
      INTEGER*2 MAXRDSMO,MAXCDSMO
      INTEGER*2 MAXTAF
      INTEGER*2 MAXDSMP
      INTEGER*2 MAXDSMPR
      INTEGER*2 MAXDSMPC
      INTEGER*2 MAXOPR
      INTEGER*2 MAXOPC
      REAL*4 UNCONFA
      REAL*4 UNCONFAR
      INTEGER*4 QDSMRPT
      INTEGER*2 MAXCOP
      INTEGER*2 MAXROP
      INTEGER*2 ELINDEX,ELHEATX,ELWHEAX,ELCOOLX,ELSTOVX,ELDRYEX,ELSECHX
      INTEGER*2 MAXECPB,MAXECPSG,MAXECTB
      INTEGER*2 MAXITV
      INTEGER*2 MAXEU
      INTEGER*2 MAXEUGRP
      INTEGER*2 MAXEFDS,MAXEFDB,MAXEFDSG,MAXECPS,MAXEFDSS
      INTEGER*2 MAXEFTB
      INTEGER*2 MAXSEC,RES,COM,IND,TRA
      INTEGER*2 MAXREC
      INTEGER*2 MAXMON,MAXSEA,MAXDTP
      INTEGER*2 MAXNRG
      INTEGER*2 MAXHOUR
      INTEGER*2 MAXRHOUR
      INTEGER*2 MAXDAY
      INTEGER*2 MAXCRG
      INTEGER*2 ELLDVHX
      INTEGER*2 ELRAILX
      INTEGER*2 MAXRBT,MAXRDT
      INTEGER*2 MAXCBT
      INTEGER*2 QREPYRH
      INTEGER   NPROB
      INTEGER   NMOM
      PARAMETER(QREPYRH=3)
      PARAMETER(MAXEFTB=27)
      PARAMETER(MAXEU=140)
      PARAMETER(MAXEUGRP=40)
!* To add more sectors modify also DATA statement in subroutine DSMRDSTR
      PARAMETER(MAXSEC=4,RES=1,COM=2,IND=3,TRA=4)
      PARAMETER(MAXCRG=9)
      PARAMETER(MAXREC=1600)
      PARAMETER(MAXNRG=MNUMNR-3)
      PARAMETER(MAXMON=12,MAXDAY=3,MAXSEA=12,MAXDTP=3)
      PARAMETER(MAXHOUR=MAXMON*MAXDAY*24+1)
      PARAMETER(MAXRHOUR=8784)
      PARAMETER(ELINDEX=1,ELHEATX=2,ELWHEAX=2,ELCOOLX=1)
      PARAMETER(ELSTOVX=3,ELDRYEX=2,ELSECHX=2)
      PARAMETER(MAXECPB=8,MAXECPSG=24,MAXEFDS=4,MAXEFDB=8,MAXEFDSG=24)
      PARAMETER(MAXECPS=4,MAXEFDSS=12)
      PARAMETER(MAXECTB=MAXECPB*MAXECPSG)
      PARAMETER(MAXITV=2)
      PARAMETER(ELLDVHX=6)
      PARAMETER(ELRAILX=1)
      PARAMETER(UNCONFA=292.83) ! currently: 1TBtu =  292.83GWh
      PARAMETER(UNCONFAR=292.83E-06) ! currently: 1MBtu =  292.83E-06 GWh
      PARAMETER(MAXRBT=3,MAXRDT=2)
      PARAMETER(MAXCBT=11)
!     PARAMETER(MAXCOP=25000)  !2090 options
      PARAMETER(MAXCOP=9999)   !280 options
!     PARAMETER(MAXCOP=13   )  !1 option
      PARAMETER(MAXROP=2500)   !212 options
!     PARAMETER(MAXROP=212)    !71 options
!     PARAMETER(MAXROP=13   )  !1 option
      PARAMETER(MAXDSMP=12)
      PARAMETER(MAXDSMPR=6)
      PARAMETER(MAXDSMPC=6)
!     PARAMETER(MAXOPC=70) !2090 options
      PARAMETER(MAXOPC=70) !280  options
!     PARAMETER(MAXOPC=1)  !1 option
!     PARAMETER(MAXOPR=15) !212 options
      PARAMETER(MAXOPR=99) !71  options
!     PARAMETER(MAXOPR=1)  !1 option
!     PARAMETER(MAXTAF=280) ! 2090 options
      PARAMETER(MAXTAF=400) ! 280 options
!     PARAMETER(MAXTAF=4)   ! 1 option
!     PARAMETER(MAXRDSMO=212,MAXCDSMO=2090)
      PARAMETER(MAXRDSMO=212,MAXCDSMO=280)
!     PARAMETER(MAXRDSMO=1 ,MAXCDSMO=1)
      PARAMETER(MAXNLST=3,MAXRLST=MNUMNR-3+1)
      PARAMETER(DISCFA=0.05)
      PARAMETER(MAXFRT=2,MAXTOT=2)
      PARAMETER(MAXRTECH=21,MAXCTECH=70)
      PARAMETER(MAXDMT=4)
      PARAMETER(MAXDECT=3)
      PARAMETER(MAXRRST=9999)
      PARAMETER(NYRESTR=MNUMYR-1)
      PARAMETER(NYRESTC=MNUMYR)
      PARAMETER(NYRESTR1=MNUMYR-1)
      PARAMETER(NYRESTC1=MNUMYR)
      PARAMETER(MAXNTPO=4)
      PARAMETER(NUMCTCE=3)
      PARAMETER(TNSEEFD=24)
      PARAMETER(MSEGEFD=6)
      PARAMETER(MAXREU=11)
      PARAMETER(MAXPCPH=20)
      PARAMETER(MAXPACR=10)
      PARAMETER(ECPTHR=10)
      PARAMETER(MAXCBIL=11,MAXCSRV=10)
      PARAMETER(QDSMRPT=24)
      PARAMETER(NPROB=13)
      PARAMETER(NMOM=4)
