!  $Header: m:/default/includes/RCS/ogsmoff,v 1.10 2018/06/18 12:38:49 dh5 Exp $
!  OFFSHORE BASE YEAR
      INTEGER OFFBYR
      INTEGER OFFNHYR
      INTEGER ASSESS_YR

!  OFFSHORE LOWER 48 CONVENTIONAL VARIABLE DECLARATIONS
      REAL TECHOFF(COSTCAT)                    ! TECH ADJUST FACTOR
      REAL POLCTECHOFF(OFFFUEL)         ! POLICY TECH ADJUST FACTOR
      REAL POLCTECHGOM(OFFFUEL)         ! GOM POLICY TECH ADJUST FACTOR
      REAL TECHADJ_OFF(OFFFUEL,5)         ! HI/LO TECHNOLOGY ADJUST FACTOR

      REAL SROFF(OFFWELL,OFFRGN,OFFFUEL)      ! SUCCESS RATE
      REAL SRADJOFF(OFFWELL,OFFRGN,OFFFUEL) ! SUCCESS RATE ADJ.FACTOR
      REAL DPTHOFF(OFFRGN,OFFFUEL)    ! AVERAGE WELL DEPTH
      REAL DPTHOFFLAG(OFFRGN,OFFFUEL)    ! AVERAGE WELL DEPTH
      REAL DRYDPTHOFF(OFFRGN,OFFFUEL)    ! AVERAGE WELL DEPTH
      REAL DRYDPTHOFFLAG(OFFRGN,OFFFUEL)    ! AVERAGE WELL DEPTH
      REAL FLOWOFF(OFFWELL,OFFRGN,OFFFUEL) ! ANNUAL FLOW RATE PER WELL
      REAL WELLAGOFF(OFFWELL,OFFRGN,OFFFUEL)  ! NUM WELLS IN YEAR T-1
      REAL WELLSOFF(OFFWELL,OFFRGN,OFFFUEL)   ! NUM WELLS
      REAL FR1TECHOFF(OFFRGN,OFFFUEL)  ! TECHNOLOGY ADJUST FACTOR-FIND.RATES
      REAL FR2TECHOFF(OFFRGN,OFFFUEL)  ! TECHNOLOGY ADJUST FACTOR-FIND.RATES
      REAL FR3TECHOFF(OFFRGN,OFFFUEL)  ! TECHNOLOGY ADJUST FACTOR-FIND.RATES

!  INPUTS FOR PRESENT VALUE CALCULATIONS
      INTEGER NFWELLOFF(OFFWELL,OFFRGN,30) ! NW-FLD-WLD DRILL SCHED
      INTEGER EXWELLOFF(OFFWELL,OFFRGN,30) ! EXPL. PRJCT DRILL SCHED
      INTEGER DVWELLOFF(OFFWELL,OFFRGN,30) ! DEV. PRJCT DRILL SCHED
      INTEGER PRJOFF(OFFFUEL) ! EXPECTED LIFE OF PROJECT BY FUELTYPE
      INTEGER RCPRDOFF(2) ! RECOVERY PERIOD INTANG & TANGIBLE COST
      REAL STTXOFF(OFFRGN)   ! WEIGHTED AVERAGE STATE CORP TAX
      REAL CPRDOFF(OFFRGN,OFFFUEL)  ! CO-PRODUCT FACTOR
      REAL SEVTXOFF(OFFRGN,OFFFUEL) ! SEVERANCE TAX RATE
      REAL ADVLTXOFF(OFFRGN)      ! AD VALOREM TAX RATE
      REAL KAPFRCOFF(OFFWELL)     ! DRILL COST TANG & MUST BE DEPREC.
      REAL KAPSPNDOFF(OFFRGN,30)  ! OTHER CAPITIAL EXPEND (BY REGION)
      REAL XDCKAPOFF        ! INTANG DRILL COSTS THAT MUST BE DEPREC.
      REAL DECLOFF(OFFRGN,OFFFUEL)   ! DECLINE RATES
      REAL PROFOFF(OFFRGN,OFFFUEL,30)  ! PRODUCTION PROFILES
      REAL TRANSOFF(OFFRGN,MNUMYR)   ! TRANSPORTATION COSTS
      REAL LAGPRCOFF(OFFRGN,OFFFUEL) ! LAG OFFSHORE PRICE

!  COST VARIABLES
      REAL NFWCOSTOFF(OFFRGN,OFFFUEL)        ! COST NEW FIELD WILDCAT
      REAL DRILLOFF(OFFWELL,OFFRGN,OFFFUEL)  ! COST OF DRILLING WELL
      REAL LEASOFF(OFFWELL,OFFRGN,OFFFUEL)   ! LEASE EQUIPMENT COST
      REAL OPEROFF(OFFWELL,OFFRGN,OFFFUEL)    ! OPERATING COST
      REAL DRYOFF(OFFWELL,OFFRGN,OFFFUEL)     ! DRY HOLE COSTS
      REAL DCFOFF(OFFWELL,OFFRGN,OFFFUEL)     ! DCF DRY WELL
      REAL COSTOFF(OFFWELL,OFFRGN,OFFFUEL)    ! CAPITAL COSTS
      REAL SUCWELLOFF(OFFWELL,OFFRGN,OFFFUEL) ! SUCCESSFUL NEW WELLS
      REAL DRYWELLOFF(OFFWELL,OFFRGN,OFFFUEL) ! DRY NEW WELLS

!  PROFITABILITY INDICES
      REAL PROFIRK_OFF(OFFWELL,OFFRGN,OFFFUEL) ! PROF. WELL/RGN/FUEL

!  LOWER 48 EXPLORATION AND DEVELOPMENT EXPENDITURES
      REAL*8 COMMIT_OFF(OFFWELL) ! OFFSHORE DRILLING COMMITMENTS
      REAL*8 SPENDI_OFF(OFFWELL)  ! L48 ONSHORE EXPL EXP (I)
      REAL*8 SPENDIR_OFF(OFFWELL,OFFRGN)  ! L48 ON EXPL EXP (I,R)
      REAL*8 SPENDIRK_OFF(OFFWELL,OFFRGN,OFFFUEL)  ! L48 ON EXPL EXP

!  LOWER 48 OFFSHORE RESERVE ADDITION VARIABLES
      REAL RESBOYOFF(OFFRGN,OFFFUEL)   ! BOY RESERVES
      REAL HISTPRDOFF(OFFRGN,OFFFUEL,MNUMYR)   ! HISTORICAL PRODUCTION
      REAL HISTRESOFF(OFFRGN,OFFFUEL,MNUMYR)   ! HISTORICAL BOY RESERVES
      REAL HISTPRROFF(OFFRGN,OFFFUEL,MNUMYR)   ! HISTORICAL BOY RESERVES
      REAL HISTNRDOFF(OFFRGN,OFFFUEL,MNUMYR)   ! HISTORICAL NEW FIELD DISC
      REAL HISTEXTOFF(OFFRGN,OFFFUEL,MNUMYR)   ! HISTORICAL EXT.& NEW POOL
      REAL HISTREVOFF(OFFRGN,OFFFUEL,MNUMYR)   ! HISTORICAL REVISIONS
      REAL HISTWELOFF(OFFWELL,OFFRGN,OFFFUEL,-1:MNUMYR) ! HISTORICAL DRILLING
      REAL HISTNFWOFF(OFFRGN,OFFFUEL,-1:MNUMYR) ! HISTORICAL DRILLING
      REAL HISTSROFF(OFFWELL,OFFRGN,OFFFUEL,MNUMYR) ! HISTORICAL SUCCESS RATES
      REAL HISTADOFF(offrgn,MNUMYR)   ! HISTORICAL BOY AD GAS
      REAL HISTTADOFF(MNUMYR)   ! HISTORICAL BOY AD GAS
      REAL PRRATOFF(OFFRGN,OFFFUEL)    ! P/R RATIO
      REAL ELASTOFF(OFFRGN,OFFFUEL)    ! ELASTICITY
      REAL CURRESOFF(OFFRGN,OFFFUEL)   ! LAG (T-1) RESERVES
      REAL CURPRROFF(OFFRGN,OFFFUEL)   ! LAG (T-1) P/R RATIO
      REAL RESADOFF(OFFRGN,OFFFUEL)    ! RES. ADD. BY RGN/FUEL TYPE
      REAL FR1OFF(OFFRGN,OFFFUEL)      ! FINDING RATES NFW
      REAL FR2OFF(OFFRGN,OFFFUEL)      ! FINDING RATES DEV
      REAL FR3OFF(OFFRGN,OFFFUEL)      ! FINDING RATES OEXP
      REAL RSVGROFF(OFFFUEL)           ! RESERVES GROWTH FACTOR
      REAL UNRESOFF(OFFRGN,OFFFUEL)    ! UNDISCOVERED REC. RESOURCES
      real r_unresoff(offrgn,offfuel)  ! Remaining Undiscovered Rec.Rsc
      real urrofftec(offrgn,offfuel)   ! tech und. rec. resources
      real urroffadj_r(offrgn,offfuel)   ! urr adjustment
      REAL INFRSVOFF(OFFRGN,OFFFUEL)   ! INFERRED RESERVES
      real r_infrsvoff(offrgn,offfuel) ! Remaining Inferred Reserves
      REAL INFFACOFF(OFFFUEL)
      REAL URRFACOFF(OFFFUEL)
      REAL RTECHOFF(OFFFUEL)    ! RESERVE ADDITIONS TECHNOLOGY FACTOR
      REAL IRTECHOFF(OFFFUEL)   ! INFERRED RESOURCES TECHNOLOGY FACTORS
      REAL POLRTECHOFF(OFFFUEL) ! POLICY RES ADD TECHNOLOGY FACTOR
      REAL NFWOFF(OFFRGN,OFFFUEL) ! NO. SUC. NEW FIELD WLDCAT WELLS
      REAL OGPRODOFF(OFFRGN,OFFFUEL) ! HOLDS OFFSHORE PRODUCTION
      REAL EXPRDOFF(OFFRGN,OFFFUEL)  ! HOLDS EXPECTED PRODUCTION
      REAL OEXPOFF(OFFRGN,OFFFUEL)   ! NO. OF SUC. OTHER EXPL. WELLS
      REAL DEVOFF(OFFRGN,OFFFUEL)    ! NO. OF SUC. DEV. WELLS
      REAL NRDOFF(OFFRGN,OFFFUEL)    ! NEW RESERVE DISCOVERIES
      REAL NDIROFF(OFFRGN,OFFFUEL)   ! NEW DISCOVERY INFERRED RSVS
      REAL REVOFF(OFFRGN,OFFFUEL)    ! RESERVE REVISIONS
      REAL EXTOFF(OFFRGN,OFFFUEL)    ! RESERVE EXTENSIONS
      REAL FR1MINOFF(OFFRGN,OFFFUEL)  ! MINIMUM ECONOMIC FINDING RATE
      REAL FR2MINOFF(OFFRGN,OFFFUEL)  ! MINIMUM ECONOMIC FINDING RATE
      REAL FR3MINOFF(OFFRGN,OFFFUEL)  ! MINIMUM ECONOMIC FINDING RATE
      REAL CUMR1OFF(OFFRGN,OFFFUEL)  ! CUMULATIVE RESERVE DISCOVERIES
      REAL CUMR2OFF(OFFRGN,OFFFUEL)  ! CUMULATIVE RESERVE DISCOVERIES
      REAL CUMR3OFF(OFFRGN,OFFFUEL)  ! CUMULATIVE RESERVE DISCOVERIES
      REAL DELTA1OFF(OFFRGN,OFFFUEL) ! FINDING RATE DECLINE PARM NFW
      REAL DELTA2OFF(OFFRGN,OFFFUEL) ! FINDING RATE DECLINE PARM DEV
      REAL DELTA3OFF(OFFRGN,OFFFUEL) ! FINDING RATE DECLINE PARM OEXP
      REAL REPPRDOFF(OFFRGN,OFFFUEL,MNUMYR) ! PROD REPORTING VAR
      REAL REPRSVOFF(OFFRGN,OFFFUEL,MNUMYR) ! RSVS REPORTING VAR
      REAL REPRADOFF(OFFRGN,OFFFUEL,MNUMYR) ! RSVADD REPORTING VAR
      REAL DRILLTOTOFF(OFFRGN,OFFFUEL+2,MNUMYR) ! REPORTING VAR

! Offshore Drilling Cost Parameters
      REAL alpha_drl_off(OFFRGN,OFFFUEL)
      REAL b0_drl_off(DEPTH_CAT,OFFFUEL)
      REAL b1_drl_off (OFFRGN,OFFFUEL)
      REAL b2_drl_off(OFFRGN,OFFFUEL)
      REAL rig_drl_off(OFFRGN,OFFFUEL)
      REAL rho_drl_off(OFFRGN,OFFFUEL)
      REAL alpha_dry_off(OFFRGN,OFFFUEL)
      REAL b0_dry_off(DEPTH_CAT,OFFFUEL)
      REAL b1_dry_off (OFFRGN,OFFFUEL)
      REAL b2_dry_off(OFFRGN,OFFFUEL)
      REAL rig_dry_off(OFFRGN,OFFFUEL)
      REAL rho_dry_off(OFFRGN,OFFFUEL)
      REAL curwellsoff
      REAL WELLSOFFLAG
      REAL WELLSOFFLAG2
      REAL rigsoff
      REAL rigsofflag2
      REAL rigsofflag
      REAL revrigoff
      REAL revrigofflag
      REAL revrigofflag2
      REAL expendofflag
      REAL expendofflag2

! FLOW RATE ADJUSTMENT VARIABLES
      REAL FRATOFF(OFFWELL,OFFRGN,OFFFUEL)
! standard error variables for technology cases
      real se_drl_off(offrgn,offfuel)
      real se_dry_off(offrgn,offfuel)
      real se_fr1_off(offrgn,offfuel)
      real se_fr2_off(offrgn,offfuel)
      real se_fr3_off(offrgn,offfuel)

!  FINDING RATE VARIABLES FOR HYPERBOLIC FUNCTION
      REAL BIG_U_OFF(OFFRGN,OFFFUEL)
      REAL INITFR1_OFF(OFFRGN,OFFFUEL)
      REAL OFE2_OFF(OFFRGN,OFFFUEL)
      REAL DELTA_B_OFF(OFFRGN,OFFFUEL)
      REAL POA_OFF(OFFRGN,OFFFUEL)
      REAL ALPHA_OFF(OFFRGN,OFFFUEL)
      REAL BETA1_OFF(OFFRGN,OFFFUEL)
      INTEGER BETA2_OFF(OFFRGN,OFFFUEL)
      REAL PAST1A_OFF(OFFRGN,OFFFUEL)
      REAL PAST1B1_OFF(OFFRGN,OFFFUEL)
      INTEGER PAST1B2_OFF(OFFRGN,OFFFUEL)
      REAL PAST2A_OFF(OFFRGN,OFFFUEL)
      REAL PAST2B1_OFF(OFFRGN,OFFFUEL)
      INTEGER PAST2B2_OFF(OFFRGN,OFFFUEL)
      REAL FUTURA_OFF(OFFRGN,OFFFUEL)
      REAL FUTURB1_OFF(OFFRGN,OFFFUEL)
      INTEGER FUTURB2_OFF(OFFRGN,OFFFUEL)
      REAL FSA_OFF(OFFRGN,OFFFUEL)
      REAL CUM_U_OFF(OFFRGN,OFFFUEL)
      REAL CUM_NFW_OFF(OFFRGN,OFFFUEL)
!  FR2 and FR3 VARIABLES
      REAL CUMOEXPOFF(OFFRGN,OFFFUEL)
      REAL CUMDEVOFF(OFFRGN,OFFFUEL)

      COMMON/OGOFF0/OFFBYR,OFFNHYR,ASSESS_YR

      COMMON/OGOFF1/ &
        DPTHOFF,DPTHOFFLAG,DRYDPTHOFF,DRYDPTHOFFLAG, &
        FLOWOFF,WELLAGOFF, &
        WELLSOFF,SROFF,TECHOFF

      COMMON/OGOFF2/NFWELLOFF,EXWELLOFF,DVWELLOFF, &
         PRJOFF,RCPRDOFF,STTXOFF,CPRDOFF, &
         SEVTXOFF,ADVLTXOFF,KAPFRCOFF,KAPSPNDOFF,XDCKAPOFF,DECLOFF, &
         TRANSOFF,LAGPRCOFF,PROFOFF

      COMMON/OGOFF4/NFWCOSTOFF,DRILLOFF,LEASOFF,OPEROFF,DRYOFF, &
         DCFOFF,COSTOFF,SUCWELLOFF,DRYWELLOFF

      COMMON/OGOFF5/SPENDI_OFF, &
         SPENDIR_OFF,SPENDIRK_OFF

!  HISTORICAL COMMON BLOCK
      COMMON/OGOFF8/HISTRESOFF,HISTPRROFF,HISTADOFF,HISTWELOFF, &
         HISTNFWOFF,HISTSROFF,HISTTADOFF,HISTPRDOFF

      COMMON/OGOFF6/RESADOFF,FR1OFF,FR2OFF,FR3OFF,RSVGROFF,UNRESOFF, &
         INFRSVOFF,RTECHOFF,NFWOFF,OEXPOFF,DEVOFF,NRDOFF,NDIROFF,REVOFF, &
         EXTOFF,FR1MINOFF,FR2MINOFF,FR3MINOFF, &
         CUMR1OFF,CUMR2OFF,CUMR3OFF,DELTA1OFF,DELTA2OFF, &
         DELTA3OFF,CURRESOFF,CURPRROFF,RESBOYOFF,PRRATOFF,ELASTOFF, &
         OGPRODOFF,EXPRDOFF,REPPRDOFF,REPRSVOFF,REPRADOFF,DRILLTOTOFF, &
         IRTECHOFF

! CONTAINS FLOW RATE ADJUSTMENT VARIABLES
      COMMON/OGOFF7/FRATOFF

! POLICY TECHNOLOGY ADJUSTMENT VARIABLES
      COMMON/OGOFFP/POLCTECHOFF,POLRTECHOFF

! GOM POLICY TECHNOLOGY ADJUSTMENT VARIABLES
      COMMON/OGGOMP/POLCTECHGOM
! RESOURCE RECOVERY ADJUSTMENT FACTORS BY FUEL TYPE
      COMMON/OGOFFR/INFFACOFF,URRFACOFF

! HISTORICAL RESERVE ADDITIONS
      COMMON/OGOFFS/HISTNRDOFF,HISTREVOFF,HISTEXTOFF

! PROPORTIONAL SUCCESS RATE ADJUSTMENT FACTORS
      COMMON/OGOFFW/SRADJOFF

! Offshore Drilling Parameters
      common/ogoffd/alpha_drl_off,b0_drl_off,b1_drl_off, &
                    b2_drl_off,rho_drl_off, &
                    alpha_dry_off,b0_dry_off,b1_dry_off, &
                    b2_dry_off,rho_dry_off, &
                    curwellsoff,wellsofflag,wellsofflag2, &
                    rig_drl_off,rig_dry_off, &
                    rigsoff,revrigoff,rigsofflag, &
                    rigsofflag2,revrigofflag,revrigofflag2, &
                    expendofflag,expendofflag2

! Finding Rate Technology Factors
      COMMON/OGOFFFRT/FR1TECHOFF,FR2TECHOFF,FR3TECHOFF

! Remaining Resources
      common/ogoffrr/r_unresoff,r_infrsvoff

! Technically recoverable resources
      common/ogofftec/urrofftec,urroffadj_r

! Standard error variables for technology cases
      common/ogoffse/se_drl_off,se_fr1_off,se_fr2_off,se_fr3_off, &
                     se_dry_off

! Hi/Lo Technology Adjustment
      common/ogofftech/techadj_off

! FINDING RATE VARIABLES FOR HYPERBOLIC FUNCTION
      COMMON/OGOFFFR1A/BIG_U_OFF,INITFR1_OFF,DELTA_B_OFF,POA_OFF, &
                       ALPHA_OFF,BETA1_OFF,PAST1A_OFF, &
                       PAST1B1_OFF,PAST2A_OFF,PAST2B1_OFF, &
                       FUTURA_OFF,FUTURB1_OFF
      COMMON/OGOFFFR1B/BETA2_OFF,PAST1B2_OFF,PAST2B2_OFF,FUTURB2_OFF
      COMMON/OGOFFFR1C/OFE2_OFF,FSA_OFF,CUM_U_OFF,CUM_NFW_OFF

! FR2 AND FR3 VARIABLES
      COMMON/OGOFFFR2/CUMOEXPOFF,cumdevoff
