! $Header: m:/default/includes/RCS/dsmrept,v 1.6 2008/07/30 19:05:05 RSC Exp $
! ===== (DSMREPT) =====
! This INCLUDE FILE provides information about the LDSM reports
! NREPREG - NUMBER OF REGIONS THE LDSM REPORTS ARE TO BE GENERATED
! NODSMRPT(QDSMRPT) - T/F TABLE THAT SHOWS WHICH REPORTS ARE TO BE GENERATED
! NOREGRPT(MNUMNR) -  T/F TABLE WITH REGIONS THE REPORTS ARE TO BE GENERATED FOR
! NOYEARRPT(MNUMYR) - T/F TABLE WITH YEARS THE REPORTS ARE TO BE GENERATED FOR
!                     (SAME INFORMATION AS IN IREPYRV)
! NREPYRV - NUMBER OF YEARS THAT THE DSM REPORTS ARE TO BE GENERATED FOR
! IREPYRV(MNUMYR) - ORDINAL NUMBERS OF NEMS TIME HORIZON YEARS THAT DSM REPORTS
!                   ARE TO BE GENERATED FOR
! NREPYRH - NUMBE OF YEARS THAT ARE TO BE PRESENTED ON ONE LINE IN SOME REPORTS
! IREPYRH(MNYMYR) - ORDINAL NUMBERS OF NEMS TIME HORIZON YEARS THAT ARE TO BE
!                   REPORTED ON ONE IN SOME REPORTS
! SEASONEFD(MAXEFDS) - EFD SEASON NEMAES
! EFDSEGDSC(MAXEFDSS) - VERBAL DESRIPTION OF HOURS IN EFD SEGEMENT
! ECPSEGDSC(MAXECPSG) - VERBAL DESRIPTION OF HOURS IN ECP SEGEMENT
! ECPblSlc(MAXECTB) - number of the slice for the  block belongs to
! ECPLDCSL(MNUMYR+ECP_D_FPH,MAXNRG,MAXECTB) - same as above
! FUECREP6(MAXRDSMO)
! FCSTREP6(MAXRDSMO)
! LIFREP6(MAXRDSMO)
! FPEAREP6(MAXRDSMO,MNUMNR)
! PEAREP6(MAXRDSMO,MNUMNR)
! TUECREP6(MAXRDSMO)
! TCSTREP6(MAXRDSMO)
! TPEAREP6(MAXRDSMO,MNUMNR)
! AACSREP6(MAXRDSMO)
! PLTBREP6(MAXRDSMO)
! PLTCREP6(MAXRDSMO)
! FUECREP0(MAXCDSMO)
! FCSTREP0(MAXCDSMO)
! FCMCSEP0(MAXCDSMO)
! LIFREP0(MAXCDSMO)
! FPEAREP0(MAXCDSMO,MNUMNR)
! PEAREP0(MAXCDSMO,MNUMNR)
! TUECREP0(MAXCDSMO)
! TCSTREP0(MAXCDSMO)
! TMCSREP0(MAXCDSMO)
! TPEAREP0(MAXCDSMO,MNUMNR)
! AACSREP0(MAXCDSMO)
! PLTBREP0(MAXCDSMO)
! PLTCREP0(MAXCDSMO)
! FUNIREP8(MAXRDSMO,QREPYRH) ! DEFINE  ON DSMDIMEN QREPYRH
! TUNIREP8(MAXRDSMO,QREPYRH)
! PUNIREP8(MAXRDSMO,QREPYRH)
! FUNIREP12(MAXCDSMO,QREPYRH) ! DEFINE  ON DSMDIMEN QREPYRH
! TUNIREP12(MAXCDSMO,QREPYRH)
! PUNIREP12(MAXCDSMO,QREPYRH)
! AFUNIREP8(MAXRDSMO) ! DEFINE  ON DSMDIMEN QREPYRH
! ATUNIREP8(MAXRDSMO)
! APUNIREP8(MAXRDSMO)
! AFUNIREP12(MAXCDSMO) ! DEFINE  ON DSMDIMEN QREPYRH
! ATUNIREP12(MAXCDSMO)
! APUNIREP12(MAXCDSMO)
! APUNIREP9(MAXRDSMO)
! AEIMPREP9(MAXRDSMO)
! PUNIREP9(MAXRDSMO,QREPYRH)
! EIMPREP9(MAXRDSMO,QREPYRH)
! APUNIREP13(MAXCDSMO)
! AEIMPREP13(MAXCDSMO)
! PUNIREP13(MAXCDSMO,QREPYRH)
! EIMPREP13(MAXCDSMO,QREPYRH)
! FUNIREP18(MAXRDSMO,MNUMNR)
! TUNIREP18(MAXRDSMO,MNUMNR)
! PUNIREP18(MAXRDSMO,MNUMNR)
! CPUNIREP18(MAXRDSMO,MNUMNR)
! ONEYRSV18(MAXRDSMO,MNUMNR)
! FUNIREP19(MAXCDSMO,MNUMNR)
! TUNIREP19(MAXCDSMO,MNUMNR)
! PUNIREP19(MAXCDSMO,MNUMNR)
! CPUNIREP19(MAXCDSMO,MNUMNR)
! ONEYRSV19(MAXCDSMO,MNUMNR)
! DSMRENIMP(MNUMYR,MNUMNR)
! DSMRPEIMP(MNUMYR,MNUMNR)
! DSMRREBAT(MNUMYR,MNUMNR)
! DSMRADMIN(MNUMYR,MNUMNR)
! DSMRCUSTU(MNUMYR,MNUMNR)
! DSMCENIMP(MNUMYR,MNUMNR)
! DSMCPEIMP(MNUMYR,MNUMNR)
! DSMCREBAT(MNUMYR,MNUMNR)
! DSMCADMIN(MNUMYR,MNUMNR)
! DSMCCUSTU(MNUMYR,MNUMNR)
! REP1SP(MNUMNR,MNUMYR)
! REP1TL(MNUMNR,MNUMYR)
! REP1LF(MNUMNR,MNUMYR)
! REP1SC(MNUMNR,MNUMYR,MAXSEC)
! REP1TD(MNUMNR,MNUMYR)
! REP1MO(MNUMNR,MNUMYR)
! REP1JD(MNUMNR,MNUMYR)
! REP1HO(MNUMNR,MNUMYR)
       REAL*4 REP1SP(MNUMNR,MNUMYR)
       REAL*4 REP1TL(MNUMNR,MNUMYR)
       REAL*4 REP1LF(MNUMNR,MNUMYR)
       REAL*4 REP1SC(MNUMNR,MNUMYR,MAXSEC)
       REAL*4 REP1TD(MNUMNR,MNUMYR)
       INTEGER*2 REP1MO(MNUMNR,MNUMYR)
       INTEGER*2 REP1JD(MNUMNR,MNUMYR)
       INTEGER*2 REP1HO(MNUMNR,MNUMYR)
       REAL*4 DSMRENIMP(MNUMYR,MNUMNR)
       REAL*4 DSMRPEIMP(MNUMYR,MNUMNR)
       REAL*4 DSMRREBAT(MNUMYR,MNUMNR)
       REAL*4 DSMRADMIN(MNUMYR,MNUMNR)
       REAL*4 DSMRCUSTU(MNUMYR,MNUMNR)
       REAL*4 DSMCENIMP(MNUMYR,MNUMNR)
       REAL*4 DSMCPEIMP(MNUMYR,MNUMNR)
       REAL*4 DSMCREBAT(MNUMYR,MNUMNR)
       REAL*4 DSMCADMIN(MNUMYR,MNUMNR)
       REAL*4 DSMCCUSTU(MNUMYR,MNUMNR)
       REAL*4 FUNIREP18(MAXRDSMO,MNUMNR)
       REAL*4 TUNIREP18(MAXRDSMO,MNUMNR)
       REAL*4 PUNIREP18(MAXRDSMO,MNUMNR)
       REAL*4 CPUNIREP18(MAXRDSMO,MNUMNR)
       REAL*4 ONEYRSV18(MAXRDSMO,MNUMNR)
       REAL*4 FUNIREP19(MAXCDSMO,MNUMNR)
       REAL*4 TUNIREP19(MAXCDSMO,MNUMNR)
       REAL*4 PUNIREP19(MAXCDSMO,MNUMNR)
       REAL*4 CPUNIREP19(MAXCDSMO,MNUMNR)
       REAL*4 ONEYRSV19(MAXCDSMO,MNUMNR)
       REAL*4 APUNIREP9(MAXRDSMO)
       REAL*4 AEIMPREP9(MAXRDSMO)
       REAL*4 PUNIREP9(MAXRDSMO,QREPYRH)
       REAL*4 EIMPREP9(MAXRDSMO,QREPYRH)
       REAL*4 APUNIREP13(MAXCDSMO)
       REAL*4 AEIMPREP13(MAXCDSMO)
       REAL*4 PUNIREP13(MAXCDSMO,QREPYRH)
       REAL*4 EIMPREP13(MAXCDSMO,QREPYRH)
       REAL*4 FUNIREP8(MAXRDSMO,QREPYRH) ! DEFINE  ON DSMDIMEN QREPYRH
       REAL*4 TUNIREP8(MAXRDSMO,QREPYRH)
       REAL*4 PUNIREP8(MAXRDSMO,QREPYRH)
       REAL*4 FUNIREP12(MAXCDSMO,QREPYRH) ! DEFINE  ON DSMDIMEN QREPYRH
       REAL*4 TUNIREP12(MAXCDSMO,QREPYRH)
       REAL*4 PUNIREP12(MAXCDSMO,QREPYRH)
       REAL*4 AFUNIREP8(MAXRDSMO) ! DEFINE  ON DSMDIMEN QREPYRH
       REAL*4 ATUNIREP8(MAXRDSMO)
       REAL*4 APUNIREP8(MAXRDSMO)
       REAL*4 AFUNIREP12(MAXCDSMO) ! DEFINE  ON DSMDIMEN QREPYRH
       REAL*4 ATUNIREP12(MAXCDSMO)
       REAL*4 APUNIREP12(MAXCDSMO)
       REAL*4 FUECREP6(MAXRDSMO)
       REAL*4 FCSTREP6(MAXRDSMO)
       INTEGER*2 LIFREP6(MAXRDSMO)
       REAL*4 FPEAREP6(MAXRDSMO,MNUMNR)
       REAL*4 PEAREP6(MAXRDSMO,MNUMNR)
       REAL*4 TUECREP6(MAXRDSMO)
       REAL*4 TCSTREP6(MAXRDSMO)
       REAL*4 TPEAREP6(MAXRDSMO,MNUMNR)
       REAL*4 AACSREP6(MAXRDSMO)
       REAL*4 PLTBREP6(MAXRDSMO)
       REAL*4 PLTCREP6(MAXRDSMO)
       REAL*4 FUECREP0(MAXCDSMO)
       REAL*4 FCSTREP0(MAXCDSMO)
       REAL*4 FMCSREP0(MAXCDSMO)
       INTEGER*2 LIFREP0(MAXCDSMO)
       REAL*4 FPEAREP0(MAXCDSMO,MNUMNR)
       REAL*4 PEAREP0(MAXCDSMO,MNUMNR)
       REAL*4 TUECREP0(MAXCDSMO)
       REAL*4 TCSTREP0(MAXCDSMO)
       REAL*4 TMCSREP0(MAXCDSMO)
       REAL*4 TPEAREP0(MAXCDSMO,MNUMNR)
       REAL*4 AACSREP0(MAXCDSMO)
       REAL*4 PLTBREP0(MAXCDSMO)
       REAL*4 PLTCREP0(MAXCDSMO)
       INTEGER*4 NREPREG
       CHARACTER*14 SEASONEFD(MAXEFDS)
       CHARACTER*12 EFDSEGDSC(MAXEFDSG)
       CHARACTER*12 ECPSEGDSC(MAXECPSG)
       LOGICAL NODSMRPT(QDSMRPT)
       LOGICAL NOREGRPT(MNUMNR)
       LOGICAL NOYEARRPT(MNUMYR)
       INTEGER*4 NREPYRV,IREPYRV(MNUMYR),NREPYRH,IREPYRH(MNUMYR)
       REAL*4 ECPblSlc(MAXECTB)
       REAL*4 ECPLDCSL(MNUMYR+ECP_D_FPH,MAXNRG,MAXECTB)
       COMMON/DSMREPL/NODSMRPT,NOREGRPT,NOYEARRPT
       COMMON/DSMREPN/NREPYRV,IREPYRV,NREPYRH,IREPYRH,NREPREG,LIFREP6, &
       LIFREP0,REP1MO,REP1JD,REP1HO
       COMMON/DSMREPR/ECPblSlc,ECPLDCSL,FUECREP6, &
       FCSTREP6,FPEAREP6,PEAREP6,TUECREP6,TCSTREP6,TPEAREP6, &
