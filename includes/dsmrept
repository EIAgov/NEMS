       ,ATUNIREP12,APUNIREP12,APUNIREP9,AEIMPREP9,PUNIREP9,EIMPREP9 &
       ,APUNIREP13,AEIMPREP13,PUNIREP13,EIMPREP13 &
       ,FUNIREP18,TUNIREP18,PUNIREP18,CPUNIREP18,ONEYRSV18 &
       ,FUNIREP19,TUNIREP19,PUNIREP19,CPUNIREP19,ONEYRSV19 &
       ,DSMRENIMP,DSMRPEIMP,DSMRREBAT,DSMRADMIN,DSMRCUSTU &
       ,DSMCENIMP,DSMCPEIMP,DSMCREBAT,DSMCADMIN,DSMCCUSTU &
       ,REP1SP,REP1TL,REP1LF,REP1SC,REP1TD
       COMMON/DSMREPC/SEASONEFD,EFDSEGDSC,ECPSEGDSC
