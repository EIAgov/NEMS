!  the following should be included before dispinyr:
!     include 'parametr'
!     include 'emmparm'
!     include 'cdsparms'
      INTEGER HSTYEAR
      INTEGER NHIST
      INTEGER NSTEO
      INTEGER NYEAR
      PARAMETER(HSTYEAR=2010)         ! Start Year for Historical Overwrites
      PARAMETER(NHIST=12)
      PARAMETER(NSTEO=2)
      PARAMETER(NYEAR=NHIST + NSTEO + (HSTYEAR-1990))
!
!   BENCHMARK DATA FOR GENERATION AND FUEL CONSUMPTION
!
!        Nerc Region
!  Ownership type--1=Utility, 2=Nonutility
      REAL HGNCLNR(2,MNUMNR,NYEAR)    !  1 COAL GEN BY OWNER TYPE/NERC
      REAL HGNNGNR(2,MNUMNR,NYEAR)    !  2 GAS  GEN BY OWNER TYPE/NERC
      REAL HGNOGNR(2,MNUMNR,NYEAR)    !  3 OTH GAS FUELS GEN BY OWNER TYPE/NERC
      REAL HGNOLNR(2,MNUMNR,NYEAR)    !  4 DS   GEN BY OWNER TYPE/NERC
      REAL HGNURNR(2,MNUMNR,NYEAR)    !  5 NUC  GEN BY OWNER TYPE/NERC
      REAL HGNPSNR(2,MNUMNR,NYEAR)    !  6 PS   GEN BY OWNER TYPE/NERC
      REAL HGNHYNR(2,MNUMNR,NYEAR)    !  7 HYD (NOT PS) GEN BY OWNER TYPE/NERC
      REAL HGNGENR(2,MNUMNR,NYEAR)    !  8 GEOTHERMAL GEN BY OWNER TYPE/NERC
      REAL HGNMSNR(2,MNUMNR,NYEAR)    !  9 MSW GEN BY OWNER TYPE/NERC
      REAL HGNWDNR(2,MNUMNR,NYEAR)    ! 10 WOOD GEN BY OWNER TYPE/NERC
      REAL HGNSONR(2,MNUMNR,NYEAR)    ! 11 Solar Gen by Ownership type/NERC
      REAL HGNPVNR(2,MNUMNR,NYEAR)    ! 12 PHOTOVOLTAIC GEN BY OWNER TYPE/NERC
      REAL HGNPTNR(2,MNUMNR,NYEAR)    ! 13 PHOTOVOLTAIC TILT GEN BY OWNER TYPE/NERC
      REAL HGNWNNR(2,MNUMNR,NYEAR)    ! 14 WIND GEN BY OWNER TYPE/NERC
      REAL HGNWLNR(2,MNUMNR,NYEAR)    ! 15 WIND LOW SPEED GEN BY OWNER TYPE/NERC
      REAL HGNWFNR(2,MNUMNR,NYEAR)    ! 16 OFS WIND GEN BY OWNER TYPE/NERC
      REAL HGNOTNR(2,MNUMNR,NYEAR)    ! 17 OTH  GEN BY OWNER TYPE/NERC
      REAL HGNTLNR(2,MNUMNR,NYEAR)    ! 18 TOT. GEN BY OWNER TYPE/NERC
      REAL HFLCLNR(2,MNUMNR,NYEAR)    ! 19 COAL CONS BY OWNER TYPE/NERC
      REAL HFLNGNR(2,MNUMNR,NYEAR)    ! 20 GAS  CONS BY OWNER TYPE/NERC
      REAL HFLOLNR(2,MNUMNR,NYEAR)    ! 21 OIL  CONS BY OWNER TYPE/NERC
      REAL HFLDSNR(2,MNUMNR,NYEAR)    ! 22 DIST  CONS BY OWNER TYPE/NERC
      REAL HFLRSNR(2,MNUMNR,NYEAR)    ! 23 RESID  CONS BY OWNER TYPE/NERC
! Generation by plant type for ftab Table 59 Own, 1 - utility, 2 = nonutility, 3 = NTCogen, 4 = Total
      REAL HGENPC(4,MNUMNR,NYEAR)    !  1 Total Pulv Coal generation by NERC
      REAL HGENIG(4,MNUMNR,NYEAR)    !  2 Total IGCC w/o seq coal generation by NERC
      REAL HGENIS(4,MNUMNR,NYEAR)    !  3 Total IGCC w/seq generation by NERC
      REAL HGENST(4,MNUMNR,NYEAR)    !  4 Total Oil/Gas steam generation by NERC
      REAL HGENCC(4,MNUMNR,NYEAR)    !  5 Total Conv. CC generation by NERC
      REAL HGENAC(4,MNUMNR,NYEAR)    !  6 Total Adv. CC w/o seq generation by NERC
      REAL HGENAS(4,MNUMNR,NYEAR)    !  7 Total Adv. CC w/seq generation by NERC
      REAL HGENCT(4,MNUMNR,NYEAR)    !  8 Total Conv. Turb. generation by NERC
      REAL HGENAT(4,MNUMNR,NYEAR)    !  9 Total Adv. Turb. generation by NERC
      REAL HGENIC(4,MNUMNR,NYEAR)    ! 10 Total Internal Combustion Engine generation by NERC
      REAL HGENFC(4,MNUMNR,NYEAR)    ! 11 Total Fuel Cell generation by NERC
      REAL HGENDG(4,MNUMNR,NYEAR)    ! 12 Total Dist. Gen. generation by NERC
      REAL HGENNU(4,MNUMNR,NYEAR)    ! 13 Total Nuclear generation by NERC
      REAL HGENRN(4,MNUMNR,NYEAR)    ! 14 Total Renew.  generation by NERC
      REAL HGENPS(4,MNUMNR,NYEAR)    ! 15 Total Pumped St. generation by NERC
      REAL HGENSQ(4,MNUMNR,NYEAR)    ! 16 Total Pulv Coal generation w/Seq by NERC
      REAL HGENDS(4,MNUMNR,NYEAR)    ! 17 Total Diurnal Storage generation by NERC

!   CONSUMPTION BY CENSUS REGION
      REAL HFLCLCR(MNUMCR,NYEAR)    !  1 COAL  CONS BY CENSUS REGION    
      REAL HFLNGCR(MNUMCR,NYEAR)    !  2 GAS   CONS BY CENSUS REGION   
      REAL HFLOLCR(MNUMCR,NYEAR)    !  3 OIL   CONS BY CENSUS REGION    
      REAL HFLDSCR(MNUMCR,NYEAR)    !  4 DIST  CONS BY CENSUS REGION    
      REAL HFLRSCR(MNUMCR,NYEAR)    !  5 RESID CONS BY CENSUS REGION    
!   CONSUMPTION BY GAS REGIONS
      REAL HFLNGGR(21,NYEAR)    !  1 GAS  CONS BY GAS REGION               
      REAL HFLCLGR(21,NYEAR)    !  2 COAL CONS BY GAS REGION
      REAL HFLOLGR(21,NYEAR)    !  3 OIL  CONS BY GAS REGION               
      REAL HSEAGASPER(21,EFD_D_MSP,NYEAR) !  percentage of gas consumption by season and gas region
!   EMM REGION SALES BY SECTOR
      REAL HELRSNR(MNUMNR,NYEAR)       !  1 RESIDENTIAL
      REAL HELCMNR(MNUMNR,NYEAR)       !  2 COMMERCIAL
      REAL HELINNR(MNUMNR,NYEAR)       !  3 INDUSTRIAL
      REAL HELTRNR(MNUMNR,NYEAR)       !  4 TRANSPORTATION
      REAL HELOTNR(MNUMNR,NYEAR)       !  5 OTHER
      REAL HELASNR(MNUMNR,NYEAR)       !  6 ALL SECTORS
      REAL HELNUNR(MNUMNR,NYEAR)       !  7 PURCHASES FROM NONUTILITIES
!   CENSUS DIVISION SALES BY SECTOR
      REAL HELRSCR(MNUMCR,NYEAR)       !  1 RESIDENTIAL
      REAL HELCMCR(MNUMCR,NYEAR)       !  2 COMMERCIAL
      REAL HELINCR(MNUMCR,NYEAR)       !  3 INDUSTRIAL
      REAL HELTRCR(MNUMCR,NYEAR)       !  4 TRANSPORTATION
      REAL HELOTCR(MNUMCR,NYEAR)       !  5 OTHER
      REAL HELASCR(MNUMCR,NYEAR)       !  6 ALL SECTORS
!   CENSUS DIVISION ELECTRICITY PRICES BY SECTOR
      REAL HPELRS(MNUMCR,NYEAR)       !  1 RESIDENTIAL
      REAL HPELCM(MNUMCR,NYEAR)       !  2 COMMERCIAL
      REAL HPELIN(MNUMCR,NYEAR)       !  3 INDUSTRIAL
      REAL HPELTR(MNUMCR,NYEAR)       !  4 TRANSPORTATION
      REAL HPELOT(MNUMCR,NYEAR)       !  5 OTHER
      REAL HPELAS(MNUMCR,NYEAR)       !  6 ALL SECTORS
!   EMM REGION ELECTRICITY PRICES BY SECTOR
      REAL HPELRSNR(MNUMNR,NYEAR)       !  1 RESIDENTIAL
      REAL HPELCMNR(MNUMNR,NYEAR)       !  2 COMMERCIAL
      REAL HPELINNR(MNUMNR,NYEAR)       !  3 INDUSTRIAL
      REAL HPELTRNR(MNUMNR,NYEAR)       !  4 TRANSPORTATION
      REAL HPELOTNR(MNUMNR,NYEAR)       !  5 OTHER
      REAL HPELASNR(MNUMNR,NYEAR)       !  6 ALL SECTORS
!   CENSUS DIVISION REVENUE BY SECTOR
      REAL HREVRSCR(MNUMCR,NYEAR)       !  1 RESIDENTIAL
      REAL HREVCMCR(MNUMCR,NYEAR)       !  2 COMMERCIAL
      REAL HREVINCR(MNUMCR,NYEAR)       !  3 INDUSTRIAL
      REAL HREVTRCR(MNUMCR,NYEAR)       !  4 TRANSPORTATION
      REAL HREVOTCR(MNUMCR,NYEAR)       !  5 OTHER
      REAL HREVASCR(MNUMCR,NYEAR)       !  6 ALL SECTORS
!   EMM REGION REVENUE BY SECTOR
      REAL HREVRSNR(MNUMNR,NYEAR)       !  1 RESIDENTIAL
      REAL HREVCMNR(MNUMNR,NYEAR)       !  2 COMMERCIAL
      REAL HREVINNR(MNUMNR,NYEAR)       !  3 INDUSTRIAL
      REAL HREVTRNR(MNUMNR,NYEAR)       !  4 TRANSPORTATION
      REAL HREVOTNR(MNUMNR,NYEAR)       !  5 OTHER
      REAL HREVASNR(MNUMNR,NYEAR)       !  6 ALL SECTORS
!   INTERNATIONAL TRADE
      REAL HTIMPF(MNUMNR,NYEAR)        !  1 FIRM IMPORTS
      REAL HTIMPE(MNUMNR,NYEAR)        !  2 ECONOMY IMPORTS
      REAL HTEXPF(MNUMNR,NYEAR)        !  3 FIRM EXPORTS
      REAL HTEXPE(MNUMNR,NYEAR)        !  4 ECONOMY EXPORTS
      REAL NIMPCRSH(MNUMCR,NYEAR)      !  5 CENSUS REGION SHARE OF NET IMPORTS
!    IPP              
      REAL HCIPGEN(MNUMNR,NYEAR,17,2)  !  1 IPP Generation by grid/ownuse
!    COGENERATION
      REAL HCNTGEN(MNUMNR,NYEAR,MNUMCGF,2)    !  1 NON-TRADITIONAL COGENERATION
      REAL HCNTQNR(MNUMNR,NYEAR,MNUMCGF)      !  2 NON-TRADITIONAL FUEL CONSUMPTION BY NERC REG
      REAL HCNTQCR(MNUMCR,NYEAR,MNUMCGF)      !  3 NON-TRADITIONAL FUEL CONSUMPTION BY CENSUS REG
      REAL HCREGEN(MNUMCR,NYEAR,MNUMCGF,2)    !  4 REFINERY COGENERATION
      REAL HCOGGEN(MNUMCR,NYEAR,4,2)          !  5 EOR COGENERATION
      REAL HCINDGEN(MNUMCR,NYEAR,MNUMCGF,2)   !  6 INDUSTRIAL COGENERATION
      REAL HCCOMGEN(MNUMCR,NYEAR,MNUMCGF,2)   !  7 COMMERCIAL COGENERATION
!    HISTORICAL AER HEAT RATES FOR RENEWABLES AND NUCLEAR
      REAL HNUCHR(NYEAR)               ! AER Historical Nuclear Heat Rate
      REAL HGEOHR(NYEAR)               ! AER Historical Geothermal Heat Rate
      REAL HWODHR(NYEAR)               ! AER Historical Wood Heat Rate        
      REAL HMSWHR(NYEAR)               ! AER Historical MSW Heat Rate           
      REAL HFOSHR(NYEAR)               ! AER Historical Fossil Fuel Equivalent Heat rate
      REAL HFOSHRCR(MNUMCR,NYEAR)      ! AER Historical Fossil Fuel Equivalent Heat rate
                                       !   used for hydro, solar and wind.
!  LAST HISTORICAL YEAR AND LAST STEO YEAR
      INTEGER UYR_EMIS                 ! LAST YEAR FOR HIST. EMIS. DATA
      INTEGER UYR_HIST
      INTEGER UYR_STEO
      INTEGER UYR_OVER
!  DOLLAR YEARS FOR INPUT ELECTRICITY PRICES
      INTEGER PRCDYR(NYEAR)
!  CENSUS TO NERC MAPPING FOR COGEN, BY FUEL
      REAL MPCGCtoN(MNUMNR,MNUMCR,TC_FUELS) !  FUEL INDICE MATCHES TC_FUEL IN UNUGS
!
!   Common block for Historical generation/consumption/trade
!
      COMMON/HISDATA/HGNCLNR,HGNNGNR,HGNOGNR,HGNOLNR, &
                     HGNURNR,HGNPSNR,HGNHYNR,HGNGENR,HGNMSNR, &
                     HGNWDNR,HGNSONR,HGNPVNR,HGNPTNR,HGNWNNR,HGNWLNR,HGNWFNR,HGNOTNR,HGNTLNR, &
                     HFLCLNR,HFLNGNR,HFLOLNR,HFLDSNR,HFLRSNR, &
                     HGENPC,HGENSQ,HGENIG,HGENIS,HGENST,HGENCC,HGENAC,HGENAS,HGENCT, & 
                     HGENAT,HGENIC,HGENNU,HGENPS,HGENFC,HGENRN,HGENDG,HGENDS,         &
                     HELRSNR,HELCMNR,HELINNR,HELTRNR,HELOTNR,HELASNR,HELNUNR, &
                     HELRSCR,HELCMCR,HELINCR,HELTRCR,HELOTCR,HELASCR, &
                     HPELRS,HPELCM,HPELIN,HPELTR,HPELOT,HPELAS, &
                     HPELRSNR,HPELCMNR,HPELINNR,HPELTRNR,HPELOTNR,HPELASNR, &
                     HREVRSCR,HREVCMCR,HREVINCR,HREVTRCR,HREVOTCR,HREVASCR, &
                     HREVRSNR,HREVCMNR,HREVINNR,HREVTRNR,HREVOTNR,HREVASNR, &
                     HTIMPF,HTIMPE,HTEXPF,HTEXPE,NIMPCRSH,HCIPGEN,  &
                     HCNTGEN,HCNTQNR,HCNTQCR,HCREGEN,HCOGGEN,HCINDGEN,HCCOMGEN, &
                     HNUCHR,HGEOHR,HFOSHR,HFOSHRCR,HWODHR,HMSWHR,  &
                     HFLOLCR,HFLCLCR,HFLNGCR,HFLDSCR,HFLRSCR,HFLNGGR,HFLCLGR,HFLOLGR,HSEAGASPER,   &
                     UYR_EMIS,UYR_HIST,UYR_STEO,UYR_OVER,PRCDYR,MPCGCtoN
!   ANNUAL CAPACITY FACTORS AND FUEL COSTS FOR NUCLEAR PLANTS
!
!        Nerc Region
      REAL UNUCFNR(MNUMNR,MNUMYR)    !  1 CAPACITY FACTORS
      REAL UPURELN(MNUMNR,MNUMYR)    !  1 FUEL PRICES
      COMMON/NUCDAT/UNUCFNR,UPURELN
