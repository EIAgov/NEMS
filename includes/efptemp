! UEFP TEMPORARY DATA COMMON BLOCK
      COMMON/EFPTEMP/URTUP,URBCWP,URABDE,URPRDF,URDITC,UDISNT,UPIDEF, &
                     USLNDG,URBDE,USLPRC,URRCWP,URCNBV,URCNAD,URAFDL, &
                     URDLRB,URAITC,URFITC,URAFDC,URFFDC,UROFFS,VREDTF, &
                     UPIND,USLLP,USLAGN,URPRTX,URSLTX,URTDWO,URTDRG, &
                     URCIDC,UDISYR,UPIRET,URFDC,URATSF,URTOMD,URTOMT, &
                     URFDCE,URNITC,URATSD,URXFDC,URFDCD,URPSDV,URBL, &
                     URYCWP,UPGW,UPCGW,UPYCWP,UPAFDC,UPONLN,URQAMT, &
                     UPISOC,UPADOC
      REAL*4 URTUP(2,3,MNUMNR-3) !UTILITY PLANT IN SERVICE
      REAL*4 URBCWP(2,3,MNUMNR-3) !BOOK VALUE OF CWIP
      REAL*4 URABDE(2,3,MNUMNR-3) !ACCUMULATED BOOK DEPRECIATION
      REAL*4 URPRDF(2,3,MNUMNR-3) !DEFERRED INCOME TAXES
      REAL*4 URDITC(2,3,MNUMNR-3) !DEFERRED INVESTMENT TAX CREDITS
      REAL*4 UDISNT(2,3,MNUMNR-3) !NET BOOK VALUE OF DISALLOWED PLANT
      REAL*4 UPIDEF(2,3,MNUMNR-3) !DEFERRED PHASE-IN REVENUES
      REAL*4 USLNDG(2,3,MNUMNR-3) !NET DEFERRED GAIN ON SALE LEASEBACK TRANSACTIONS
      REAL*4 URBDE(2,3,MNUMNR-3)  !BOOK DEPRECIATION EXPENSE
      REAL*4 USLPRC(2,3,MNUMNR-3) !PROCEEDS FROM SALE LEASEBACK TRANSACTION
      REAL*4 URRCWP(2,3,MNUMNR-3) !CWIP IN RATE BASE
      REAL*4 URCNBV(2,3,MNUMNR-3) !BOOK VALUE OF CANCELLED PLANT
      REAL*4 URCNAD(2,3,MNUMNR-3) !ACCUMULATED DEPRECIATION OF CANCELLED PLANT
      REAL*4 URAFDL(2,3,MNUMNR-3) !RATE BASE ADJUSTMENT FOR PLANTS COMING ON MID YEAR
      REAL*4 URDLRB(2,3,MNUMNR-3) !CHANGE IN RATE BASE BETWEEN END YEAR AND BEG YEAR
      REAL*4 URAITC(2,3,MNUMNR-3) !AMORTIZATION OF INVESTMENT TAX CREDIT
      REAL*4 URFITC(2,3,MNUMNR-3) !INVEST TAX CREDIT GENERATED AND FLOWED THROUGH
      REAL*4 URAFDC(2,3,MNUMNR-3) !AMORTIZATION OF DEFERRED DEBT AFUDC TAX SAVINGS
      REAL*4 URFFDC(2,3,MNUMNR-3) !DEBT AFUDC TAX SAVINGS GENERATED AND FLOWED THRU
      REAL*4 UROFFS(2,3,MNUMNR-3) !AFUDC OFFSET
      REAL*4 VREDTF(2,3,MNUMNR-3) !EXCESS DEFERRED TAXES FLOWED THRU - CURRENT YEAR
      REAL*4 UPIND(2,3,MNUMNR-3) !NET DEFERRED REVENUES FROM PHASE IN PLANS
      REAL*4 USLLP(2,3,MNUMNR-3) !LEASE PAYMENTS FOR SALE/LEASEBACKS
      REAL*4 USLAGN(2,3,MNUMNR-3) !AMORTIZATION OF BOOK GAIN FROM SALE/LEASEBACK
      REAL*4 URPRTX(2,3,MNUMNR-3) !PROPERTY TAXES
      REAL*4 URSLTX(2,3,MNUMNR-3) !SALES TAXES
      REAL*4 URTDWO(2,3,MNUMNR-3) !STRAIGHT LINE TAX DEPRECIATION
      REAL*4 URTDRG(2,3,MNUMNR-3) !TAX DEPRECIATION FOR REGULATORY PURPOSES
      REAL*4 URCIDC(2,3,MNUMNR-3) !CAPITALIZED INTEREST DURING CONSTRUCTION
      REAL*4 UDISYR(2,3,MNUMNR-3) !BOOK VALUE OF PLANT DISALLOWED IN THIS YEAR
      REAL*4 UPIRET(2,3,MNUMNR-3) !CAPITALIZED RETURN ON DEFERRED PHASE-IN COSTS
      REAL*4 URFDC(2,3,MNUMNR-3) !TOTAL BOOKED AFUDC - CURRENT YEAR
      REAL*4 URATSF(2,3,MNUMNR-3) !ACCEL TAX DEPRECIATION TAX SAVINGS FLOWED THRU
      REAL*4 URTOMD(3,MNUMNR-3) !TOTAL DISTRIBUTION RELATED O&M EXPENSES - REAL $
      REAL*4 URTOMT(3,MNUMNR-3) !TOTAL TRANSMISSION RELATED O&M EXPENSES - REAL $
      REAL*4 URFDCE(2,3,MNUMNR-3) !TOTAL BOOKED EQUITY AFUDC
      REAL*4 URNITC(2,3,MNUMNR-3) !NET CHANGE IN DEFERRED ITC
      REAL*4 URATSD(2,3,MNUMNR-3) !DEFERRED ACCEL TAX DEPRECIATION TAX SAVINGS
      REAL*4 URXFDC(2,3,MNUMNR-3) !DEFERRED DEBT AFUDC TAX SAVINGS
      REAL*4 URFDCD(2,3,MNUMNR-3) !BOOKED DEBT AFUDC
      REAL*4 URPSDV(2,3,MNUMNR-3) !PREFERRED STOCK DIVIDENDS
      REAL*4 URBL(4,2,3,MNUMNR-3) !UTILITY PLANT IN SERVICE BY FUNCTION (P,T,D,O)
      REAL*4 URYCWP(2,3,MNUMNR-3) !DIRECT CONSTRUCTION EXPENDITURES
      REAL*4 UPGW(16,2,3,MNUMNR-3) !GW OF CAPACITY UNDER CONSTRUCTION - CURRENT YEAR
      REAL*4 UPCGW(16,2,3,MNUMNR-3) !GW OF CAPACITY COMPLETED - CURRENT YEAR
      REAL*4 UPYCWP(16,2,2,3,MNUMNR-3) !DIRECT CONSTRUCTION EXPENDITURES
      REAL*4 UPAFDC(16,2,3,MNUMNR-3) !BOOKED AFUDC
      REAL*4 UPONLN(16,2,3,MNUMNR-3) !TOTAL BOOK VALUE OF CAPACITY COMPLETED - CURRENT
      REAL*4 UPISOC(16,2,3,MNUMNR-3) !TOTAL PLANT IN SERVICE - ORIGINAL COST
      REAL*4 UPADOC(16,2,3,MNUMNR-3) !TOTAL ACCUM DEPRECIATION - ORIGINAL COST
      REAL*4 URQAMT(2,3,MNUMNR-3) !CONTRIBUTION TO NUCLEAR DECOMMISSIONING FUND
