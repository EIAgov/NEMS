! $Header: N:/default/includes/RCS/lsrmgpar,v 1.2 1999/12/19 20:16:20 DSA Exp $
! MAXDAY - MAXIMUM NUMBER OF DAYS IN A MONTH
! MAXMON - MAXIMUM NUMBER OF MONTHS IN A YEAR
! MAXSEA - MAXIMUM NUMBER OF SEASONS IN A YEAR
! MAXDTP - MAXIMUM NUMBER OF DAY-TYPES
! MAXNP  - MAXIMUM OF PROFILES IN LSR
! MAXSDT - MAXIMUM NUMBER OF SEASON/DAY-TYPE COMBINATIONS IN A PROFILE
! IMSG - I/O UNIT TO WRITE MASSEAGES
! IOCN - I/O UNIT TO READ CONTROL FILE
! IOLSR - I/O UNIT TO READ LSRS
! IOCAL - I/O UNIT TO READ CALENDAR FILE
! IODAF - I/O UNIT FOR DAF-LSR-DB
      INTEGER*2 MAXDAY
      INTEGER*2 MAXMON
      INTEGER*2 MAXSEA
      INTEGER*2 MAXDTP
      INTEGER*2 MAXNP
      INTEGER*2 MAXSDT
      INTEGER*4 IOCN
      INTEGER*4 IOCAL
      INTEGER*4 IMSG
      INTEGER*4 IOLSR
      INTEGER*4 IODAF
      PARAMETER(IOCN=8,IMSG=7,IOLSR=10,IODAF=11,IOCAL=9)
      PARAMETER(MAXNP=36,MAXSDT=1)
      PARAMETER(MAXDAY=3,MAXMON=12,MAXSEA=12,MAXDTP=3)
