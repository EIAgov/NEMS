!  $Header: N:/default/includes/RCS/efprcy,v 1.2 1999/12/19 20:13:51 DSA Exp $
! EFP REGION, COMPONENT, AND YEAR COMMON BLOCK
      COMMON/EFPRCY/ESCWPP,ESPRLT,ESRTLT,ESPRST,ESPRCE,ESRTST, &
                    ESPRPS,ESRTCE,ESRTPS,ESTSHR,EDIVRT,EILAG, &
                    ESFTXR,ESERCE,ESPERDA,ESPEREX
      CHARACTER*144 OUTRCY
      EQUIVALENCE (ESCWPP,OUTRCY)
      REAL*4 ESCWPP(2)     !PERCENT OF CWIP IN RATE BASE
      REAL*4 ESPRLT(2)     !LONG TERM DEBT PERCENT CAPITALIZATION - CURRENT YEAR
      REAL*4 ESPRST(2)     !SHORT TERM DEBT PERCENT CAPITALIZATION -CURRENT YEAR
      REAL*4 ESPRCE(2)     !COMMON EQUITY PERCENT CAPITALIZATION - CURRENT YEAR
      REAL*4 ESPRPS(2)     !PREFERRED STOCK PERCENT CAPITALIZATION -CURRENT YEAR
      REAL*4 ESRTLT(2)     !COST OF NEW LONG TERM DEBT - CURRENT YEAR
      REAL*4 ESRTST(2)     !COST OF NEW SHORT TERM DEBT - CURRENT YEAR
      REAL*4 ESRTPS(2)     !COST OF NEW PREFERRED STOCK - CURRENT YEAR
      REAL*4 ESRTCE(2)     !ALLOWED RETURN ON COMMON EQUITY - CURRENT YEAR
      REAL*4 ESTSHR(2)     !OWNERSHIP FRACTION (1=IOU, 2=PUBLIC)
      REAL*4 EDIVRT(2)     !DIVIDEND PAYOUT RATIO
      REAL*4 EILAG(2)      !REGULATORY LAG ASSUMPTION
      REAL*4 ESFTXR(2)     !FEDERAL INCOME TAX RATE
      REAL*4 ESERCE(2)     !INPUT EARNED RETURN ON COMMON EQUITY - CURRENT YEAR
!     STRANDED ASSETS INPUT DATA
      REAL*4 ESPERDA(4)    !% GETTING DIRECT ACCESS BY CUSTOMER CLASS
      REAL*4 ESPEREX(4)    !% OF CLASS SI RECOVERED BY EXIT FEES
