!  $Header: N:/default/includes/RCS/efpr,v 1.2 1999/12/19 20:13:44 DSA Exp $
!
!     EFP REGION ONLY COMMON BLOCK
!
      COMMON/EFPR/UOBKVL,UOASVL,UOAITC,UOAFDC,UOABDE,ERCCA, &
                  CPDCON, CPDSLP, &
                  CPTCON, CPTSLP, &
                  EXTRA
!
      INTEGER ESIZ$EFP,EBLK$EFP,ELFT$EFP
      PARAMETER (ESIZ$EFP =  4 *  4 + &
                             5 *  4 * EFP_D_VIN * EFP_D_CAP * EFP_D_OWN + &
                             1 *  4 * EFP_D_CAP + &
                             2 *  4 * EFD_D_OWN )
      PARAMETER (EBLK$EFP = ( (ESIZ$EFP - 1) / 8192) + 1)
      PARAMETER (ELFT$EFP = EBLK$EFP * 8192 - ESIZ$EFP)
!
      REAL*4 UOBKVL(EFP_D_VIN,EFP_D_CAP,EFP_D_OWN) !BOOK VALUE OF EXISTING ASSETS
      REAL*4 UOASVL(EFP_D_VIN,EFP_D_CAP,EFP_D_OWN) !TAX BASIS OF EXISTING ASSETS
      REAL*4 UOAITC(EFP_D_VIN,EFP_D_CAP,EFP_D_OWN) !AMORTIZATION OF ITC FOR EXISTING ASSETS
      REAL*4 UOAFDC(EFP_D_VIN,EFP_D_CAP,EFP_D_OWN) !AMORTIZATION OF AFDC TAX SAVINGS FOR EXIST ASSETS
      REAL*4 UOABDE(EFP_D_VIN,EFP_D_CAP,EFP_D_OWN) !ACCUMULATED BOOK DEPRECIATION FOR EXISTING ASSETS
      REAL*4 ERCCA(EFP_D_CAP)                  !COST OF POST SERVICE CAPITAL EXPENDITURES ($/KW)
      REAL*4 CPDCON                          !CONSTANT FOR MODELING DISTRIBUTION CAPITAL EXPEND
      REAL*4 CPDSLP                          !SLOPE FOR MODELING DISTRIBUTION CAPITAL EXPEND
      REAL*4 CPTCON                          !CONSTANT FOR MODELING TRANSMISSION CAPITAL EXPEND
      REAL*4 CPTSLP                          !SLOPE FOR MODELING TRANSMISSION CAPITAL EXPEND
      CHARACTER*1 EXTRA(ELFT$EFP+1)          ! Leftover Bytes (ELFT$EFP may be 0)
      CHARACTER*8192 OUTR(EBLK$EFP)
      EQUIVALENCE (UOBKVL,OUTR)
!
      COMMON/EFPTMP/EOBKVL,EOASVL,EOAITC,EOAFDC,EOABDE
      REAL*4 EOBKVL(EFP_D_VIN,EFP_D_CAP,EFP_D_OWN) !BOOK VALUE OF EXISTING ASSETS
      REAL*4 EOASVL(EFP_D_VIN,EFP_D_CAP,EFP_D_OWN) !TAX BASIS OF EXISTING ASSETS
      REAL*4 EOAITC(EFP_D_VIN,EFP_D_CAP,EFP_D_OWN) !AMORTIZATION OF ITC FOR EXISTING ASSETS
      REAL*4 EOAFDC(EFP_D_VIN,EFP_D_CAP,EFP_D_OWN) !AMORTIZATION OF AFDC TAX SAVINGS FOR EXIST ASSETS
      REAL*4 EOABDE(EFP_D_VIN,EFP_D_CAP,EFP_D_OWN) !ACCUMULATED BOOK DEPRECIATION FOR EXISTING ASSETS
