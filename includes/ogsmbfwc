!  INDEX VARIABLES
      INTEGER I   !  WELL CLASS INDEX VARIABLE
      INTEGER R   !  REGION INDEX VARIABLE
      INTEGER K   !  FUEL TYPE INDEX VARIABLE
      INTEGER T   !  YEAR INDEX VARIABLE
      INTEGER F   !  ALASKAN FIELD VARIABLE
      INTEGER L   !  TEMPORARY INDEX VARIABLE
      INTEGER M   !  TEMPORARY INDEX VARIABLE
      INTEGER N   !  TEMPORARY INDEX VARIABLE

      INTEGER*4 IFILE1,IFILE2,IFILE3,IFILE4,IFILE5,IFILE6,IFILE7,IFILE8
      INTEGER*4 OFILE1,OFILE2

      INTEGER OGRUNOP(20)  ! OGSM RUN OPTION FLAG VARIABLE
      INTEGER OGREPORT(30) ! OGSM REPORTING FLAG VARIABLE

!  DECLARATIONS NEEDED TO INVOKE FILE_MGR
      LOGICAL NEW         ! IS FILE NEW (.TRUE.) OR OLD (.FALSE.)
      CHARACTER*18 FNAME  ! 18 CHARS OR MORE
      INTEGER FILE_MGR    ! DECLARATION OF FILE_MGR AS INTEGER FCTN
      EXTERNAL FILE_MGR   ! DESIGNATE THAT FILE_MGR IS A SUBPROGRAM

      INTEGER DATAYR        ! OGSM BASE DATA YEAR
      INTEGER*4 SYSOUT      ! SYSTEM OUTPUT UNIT ID
      REAL FEDTXR           ! FEDERAL CORPORATE TAX RATE
      REAL ROYRT            ! ROYALTY RATE
      REAL DISC             ! DISCOUNT RATE
      REAL INFL             ! INFLATION RATE
      REAL OGPROD(30) ! PROJECT PRIMARY FUEL PRODUCTION VECTOR
      REAL OGCOPROD(30) ! PROJECT COPRODUCT PRODUCTION VECTOR

!  OGSM SYSTEM PRICE VECTORS BY REGION/FUELTYPE/PROJECT LIFE
      REAL OGPRCCAN(MCANRG,L48FUEL,30) ! SYSTEM PRICE VECTOR
      REAL OGPRCCANL(MCANRG,L48FUEL) ! LAG SYSTEM PRICE 
      REAL OGPRCCANL2(MCANRG,L48FUEL) ! LAG-2 SYSTEM PRICE 
      REAL REPPRCCAN(MCANRG-1,L48FUEL,MNUMYR) ! REPORTING PRICE ARRAY

!  NATIONAL EXPENDITURE VARIABLES
!     REAL DCFOFFLAG(OFFWELL,OFFRGN,OFFFUEL)! (T-1) LAG EXP & DEV OFF WT. DCF
!     REAL DCFOFFLAG2(OFFWELL,OFFRGN,OFFFUEL)! (T-2) LAG EXP & DEV OFF WT. DCF
!     REAL DCFL48LAG(L48WELL,MCANRG-1,L48FUEL)! (T-1) LAG L48 WT. DCF I,R,KG
!     REAL DCFL48LAG2(L48WELL,MCANRG-1,L48FUEL)! (T-2) LAG L48 WT. DCF I,R,K
!     REAL DCFL48LAG3(L48WELL,MCANRG-1,L48FUEL)! (T-3) LAG L48 WT. DCF I,R,K
!     REAL SPENDLAGOFF(2)   ! (T-1) LAG EXP & DEV OFFSHORE EXPEND
!     REAL SPEXLAGOFF2      ! (T-2) LAG EXPLOR OFFSHORE EXPEND

!     REAL SPEND_ON(L48WELL)  ! NATIONAL ONSHORE DRILLING EXPEND.
!     REAL SPEND_L48(L48WELL) ! L48 ONSHORE DRILLING EXPENDITURES
!     REAL SPEND_OFF(OFFWELL) ! OFFSHORE DRILLING EXPENDITURES
!     REAL SPEND_EOR          ! ENHANCED OIL RECOVERY EXPENDITURES
!     REAL SPEND_RGN(L48WELL,MCANRG-1) ! REGIONAL ONSH DRILLING EXPEND.
!     REAL SPENDOFF_RGN(OFFWELL,OFFRGN) ! REGIONAL DRILLING EXPEND.

!  MDR ADJUSTMENT FACTORS
      INTEGER TECHYR       ! FIRST YEAR OF ADJUSTED TECH FACTOR
      REAL COSTADJ         ! DRILLING COST ADJUSTMENT FACTOR
      REAL PRNEWADJ        ! EXTRACTION RATE ADJUSTMENT FACTOR
      REAL DRLADJ          ! DRILLING RESPONSE ADJUSTMENT FACTOR

!  AD GAS ECONOMETRIC PARAMETERS
      REAL ALPHA_AD(9), BETA_AD(9)


      COMMON/OGBFW1c/OGREPORT,T,DATAYR,FEDTXR,ROYRT,DISC,OGRUNOP, &
         INFL,OGPROD,OGCOPROD



      COMMON/OGBFW4c/OGPRCCAN,OGPRCCANL,OGPRCCANL2

      COMMON/OGBFW6c/REPPRCCAN

      COMMON/OGBFW5c/IFILE1,IFILE2,IFILE3,IFILE4,IFILE5,IFILE6, &
            IFILE7,IFILE8,OFILE1,OFILE2,SYSOUT

      COMMON/OGBFW11c/TECHYR,COSTADJ,PRNEWADJ,DRLADJ
      COMMON/OGBFW12c/ALPHA_AD,BETA_AD
