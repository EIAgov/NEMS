! $Header: m:/default/includes/RCS/edbdef,v 1.13 2017/02/24 21:51:40 CGA Exp $
!CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
!     EDBDEF:
!     CONTAINS EMM DATABASE DEFINITION TABLE VARIABLES TO BE USED FOR
!     WRITING OUT DBASE TABLES TO THE @.EMMDBASE FILE AND TO THE SERVER.
!CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
! Requires EMMPARM first
! Requires EUSPRC first

       COMMON /EDBDEF/     COLVALS,  COLV,                         & !REAL * 4
       ECPLSI,   EFDLGSI, EFDPVINI, EFDEFPT, EFDIEAMAP, EOWNFTAB,  & !INTEGER
       ORACLESW, ORCLEFD,  ORCLECP, ORCLEFP, ORCLCL,FNRUN,   &
       TNUM,     LOOPING,  NUMCOLS,                                 & 
       ECPLSD,   ECPPYD,   ECPLGSD,                                & !CHARACTER
       ECPLGMD,  ECPLGHD,  OWNNAME,  FLNAME,   FLRGNAME, FLRGCODE,  PTYPE, &
       EFDLSLD,  EFDOWND,  EFDPOWND, EFDFUD, &
       EFDSEAD,  EFDSEAN,  EFDLGD,   EFDLSGD,  EFDVIND,  EFDPVIND, &
       ENITRD,   EFDITRD,  ECREGD,   ECPSO2D,  EFDNAME, &
       EFPEUSD,  EFPDSTD,  EFPSPRD,  EFPOWND,  EFPPTYD, ECAPFTABD,  FUELFTABD,  &
       FTABOWND, FTABOWNS, IEACATD, SCEN1,    &
       CHCOLVALS, CHCOLV, DYNSTM, WRTSTM, SQLSTMTOLD
!
       INTEGER MAXTABS,MAXCOLS,MAXRECS
       PARAMETER(MAXTABS=40)             ! Maximum DB Tables written in a subroutine
       PARAMETER(MAXCOLS=90)             ! Maximum Columns written in a DB record
       PARAMETER(MAXRECS=100)           ! Maximum DB records written in one call to Load_data
!
!     ECP DATABASE DEFINITIONS
!
      INTEGER*4    ECPLSI(ECP_D_VLS+1)            ! ECP LOAD SLICE INDEX
      CHARACTER*40 ECPLSD(ECP_D_VLS+1)            ! ECP LOAD SLICE DEFINITION
      CHARACTER*40 ECPPYD(ECP_D_XPH)              ! ECP PLANNING YEAR DEFINITION
      CHARACTER*25 ECPLGSD(ECP_D_STP)             ! ECP LOAD GROUP DEFINITION - SEASON/TIME OF DAY
      CHARACTER*25 ECPLGMD(ECP_D_STP)             ! ECP LOAD GROUP DEFINITION - MONTHS
      CHARACTER*25 ECPLGHD(ECP_D_STP)             ! ECP LOAD GROUP DEFINITION - HOURS
      CHARACTER*25 ECAPFTABD(9)                 ! ECP FTAB Capacity Index              
      CHARACTER*20 OWNNAME(ECP_D_OWN)             ! ECP OWNERSHIP NAMES
      CHARACTER*30 FLNAME(ECP_D_NFL)              ! ECP FUEL NAMES
      CHARACTER*25 FLRGNAME(MAXNFR)             ! ECP FUEL REGION STATES/NAME
      CHARACTER*2  FLRGCODE(MAXNFR)             ! ECP FUEL TWO LETTER CODE
      CHARACTER*2  PTYPE(ECP_D_CAP)               ! ECP PLANT TYPES - D=DISPATCHABE
!                                                                   R=RENEWABLE
!                                                                   I=INTERMITTENT
!     EFD DATABASE DEFINITIONS
!
      CHARACTER*40 EFDLSLD(EFD_D_MVS)             ! EFD LOAD SLICE DEFINITION
      CHARACTER*25 EFDOWND(EFD_D_OWN)             ! EFD OWNER DEFINITION
      INTEGER*4    EOWNFTAB(EFD_D_OWN)           ! MAP EFD OWNER TO FTAB OWNER
      CHARACTER*25 EFDPOWND(EFD_D_OWN)            ! EFD PLANT OWNER DEFINITION
      CHARACTER*40 FTABOWND(EFD_D_OWN)            ! EFD FTAB OWNER DEFINITION
      CHARACTER*25 FTABOWNS(EFD_D_OWN)            ! EFD FTAB OWNER SHORT NAME
      CHARACTER*25 EFDFUD(EFD_D_FPP)              ! EFD FUEL USE DEFINITION
      CHARACTER*25 EFDSEAD(EFD_D_MSP)             ! EFD SEASON DEFINITION
      CHARACTER*25 EFDSEAN(EFD_D_MSP)             ! EFD SEASON Short Name
      CHARACTER*25 FUELFTABD(7)                 ! EFD FTAB Capacity Index              
      INTEGER*4    EFDLGSI(EFD_D_MVS)             ! EFD LOAD GROUP SEASON INDEX
      CHARACTER*40 EFDLGD(EFD_D_MVS)              ! EFD LOAD GROUP DEFINITION
      CHARACTER*40 EFDLSGD(EFD_D_MVS,EFD_D_MVS)     ! EFD LOAD SEGMENT DEFINITION
      CHARACTER*25 EFDVIND(EFD_D_VIN)             ! EFD VINTAGE DEFINITION
      INTEGER*4    EFDPVINI(12)                 ! EFD PLANT FILE VINTAGE INDEX
      CHARACTER*25 EFDPVIND(12)                 ! EFD PLANT FILE VINTAGE DEFINITION
      CHARACTER*25 ENITRD(10)                   ! NEMS ITERATION DEFINITION
      CHARACTER*25 EFDITRD(10)                  ! EFD ITERATION DEFINITION
      CHARACTER*25 ECREGD(MNUMCR)               ! CENSUS REGION DEFINITION
      CHARACTER*40 ECPSO2D(EFD_D_SO2)             ! ECP SO2 COMPLIANCE GROUP DEFINITION
      CHARACTER*40 EFDNAME(EFD_D_CAP)             ! EFD PLANT TYPE DESCRIPTION
      INTEGER*4    EFDEFPT(EFD_D_CAP)             ! EFD TO EFP PLANT TYPE MAPPING
      INTEGER*4    EFDIEAMAP(EFD_D_CAP)           ! EFD TO IEA PLANT TYPE MAPPING
      CHARACTER*40 IEACATD(8)                   ! IEA CAPACITY CATEGORY
!
!     EFP DATABASE DEFINITIONS
!
      CHARACTER*40 EFPEUSD(MNEUGRP)             ! EFP END USE DEFINITIONS
      CHARACTER*25 EFPDSTD(5)                   ! EFP DEMAND SECTOR DEFINITIONS
      CHARACTER*25 EFPSPRD(4)                   ! EFP STAGE OF PRODUCTION DEFINITIONS
      CHARACTER*25 EFPOWND(EFP_D_OWN+1)           ! EFP OWNER DEFINITION
      CHARACTER*40 EFPPTYD(EFP_D_CAP)             ! EFP PLANT TYPE DEFINITION
!
!     STORE ORACLE SWITCH VALUES
!
      INTEGER*4    ORACLESW                     ! ORACLE MAIN ON/OFF SWITCH
!                                               ! 0=OFF,1=ON-PRODUCTION,2=ON-TESTING
      INTEGER*4    ORCLEFD                      ! WRITE EFD DATABASE INFO, 0=NO,1=YES
      INTEGER*4    ORCLECP                      ! WRITE ECP DATABASE INFO, 0=NO,1=YES
      INTEGER*4    ORCLEFP                      ! WRITE EFP DATABASE INFO, 0=NO,1=YES
      INTEGER*4    ORCLCL                       ! WRITE COAL DATABASE INFO, 0=NO,1=YES
!
!     STORE FINAL RUN SWITCH
!
      INTEGER*4    FNRUN                        ! FINAL RUN SWITCH, 1 = final run
!                                                !                   0 = not final run
!     STORE ORACLE RECORD WRITING VARIABLES
!
      INTEGER TNUM,LOOPING(MAXTABS),NUMCOLS(MAXTABS)
      REAL*8 COLVALS(MAXCOLS,MAXRECS),COLV(MAXTABS,MAXCOLS,MAXRECS)
      CHARACTER*25 SCEN1
      CHARACTER*40 CHCOLVALS(MAXCOLS,MAXRECS),CHCOLV(MAXTABS,MAXCOLS,MAXRECS)
      CHARACTER*220 DYNSTM(MAXTABS)
      CHARACTER*35  WRTSTM(MAXTABS)
      CHARACTER*220 SQLSTMTOLD
!
