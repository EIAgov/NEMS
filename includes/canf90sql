!CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
!     CANF90SQL:
!     CONTAINS F90SQL HANDLE VARIABLES
!CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC

      COMMON /CANF90SQL/CONNHNDL, ENVHNDL, STMTHNDL, CONNHNDLPMM, STMTHNDLPMM,  & !INTEGER
                        CONNHNDLOGSM, STMTHNDLOGSM, ORAERR
!
!     STORE F90SQL HANDLE VARIABLES FOR EMMDB
!
      INTEGER(SQLHDBC_KIND):: ConnHndl
      INTEGER(SQLHENV_KIND):: EnvHndl
      INTEGER(SQLHSTMT_KIND):: StmtHndl
!
!     STORE F90SQL HANDLE VARIABLES FOR PMMDB
!
      INTEGER(SQLHDBC_KIND):: ConnHndlpmm
      INTEGER(SQLHSTMT_KIND):: StmtHndlpmm
!
!     STORE F90SQL HANDLE VARIABLES FOR OGSMDB
!
      INTEGER(SQLHDBC_KIND):: ConnHndlogsm
      INTEGER(SQLHSTMT_KIND):: StmtHndlogsm
!
!     STORE ORACLE SWITCH VALUES
!
      INTEGER*4    ORAERR                       ! ORACLE ERROR HAS OCCURRED, 0=NO,1=YES
!
