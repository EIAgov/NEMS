      logical eoctc           !Environmental Operating Cost Tax Credit?
      real eoctcr             !Environmental Operating Cost Tax Credit Rate
      real eoctcab            !Environmental Operating Cost Tax Credit Rate addback %
      logical tcoti           !Tax Credit On Tangible Investments
      integer yr1             !number of years for tcoti
      logical tcoii           !Tax Credit On Intangible Investments
      integer yr2             !number of years for tcoii

