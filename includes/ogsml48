        real eor_gas_prod(max_reg,max_src,max_yr)    ! Gas Production from EOR project by region and CO2 source (MBCF) ?
        real co2avail(max_reg,max_yr)                ! total CO2 available in a region across all sources (BCF)
        real basavail(max_reg,max_yr)                ! base annual CO2 available in a region across all sources (BCF)
        real useavail(max_reg,max_yr)                ! used annual CO2 volume by region across all sources (BCF)        

