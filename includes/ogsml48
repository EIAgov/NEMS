        REAL INVCAP(MAX_YR)              ! Annual Total Capital Investment Constraint - used for constraining projects (MM$)
        real usecap(max_yr)              ! annual total capital investment constraint - used by projects (MM$)
        real bascap(max_yr)              ! annual total capital investment constraint - initial capital available (MM$)

        REAL DRCAP_O(MAX_REG,MAX_YR)     ! REGIONAL Oil Well Drilling Footage Constraint (DEV) (Feet*1000)
        REAL DRCAP_G(MAX_REG,MAX_YR)     ! REGIONAL Gas Well Drilling Footage Constraint (DEV) (Feet*1000)
        REAL NAT_DRCAP_O(MAX_YR)         ! NATIONAL Oil Well Drilling Footage Constraint (DEV) (Feet*1000)
        REAL NAT_DRCAP_G(MAX_YR)         ! NATIONAL Gas Well Drilling Footage Constraint (DEV) (Feet*1000)


        REAL EXPCDRCAP(MAX_REG,MAX_YR)   ! REGIONAL CONVENTIONAL Expl Drilling Footage Constraint (Feet*1000)
        REAL EXPUDRCAP(MAX_REG,MAX_YR)   ! REGIONAL UNCONVENTIONAL Expl Drilling Footage Constraint (Feet*1000)
        REAL NAT_EXPCDRCAP(MAX_YR)       ! NATIONAL CONVENTIONAL Expl Drilling Footage Constraint (Feet*1000)
        REAL NAT_EXPUDRCAP(MAX_YR)       ! NATIONAL UNCONVENTIONAL Expl Drilling Footage Constraint (Feet*1000)

        real expcdrcapg(max_reg,max_yr)  ! regional conventional gas exploration drilling footage constraint (Feet*1000) !mc change ver 8
        real expudrcapg(max_reg,max_yr)  ! regional unconventional gas exploration drilling footage constraint (Feet*1000) !mc change ver 8
        real nat_expcdrcapg(max_yr)      ! national conventional gas exploration drilling footage constraint (Feet*1000) !mc change ver 8
        real nat_expudrcapg(max_yr)      ! national unconventional gas exploration drilling footage constraint (Feet*1000) !mc change ver 8

        REAL SUCCDEVO(MAX_REG)           ! Development Oil Well Success Rate BY REGION (Fraction)
        REAL SUCCDEVG(MAX_REG)           ! Development Gas Well Success Rate BY REGION (Fraction)
        REAL SUCCEXP(MAX_REG)            ! Expl Well Success Rate by REGION (FRACTION)
        REAL TOTHWCAP(MAX_YR)            ! Total Horizontal Drilling Footage Constraint (Feet*1000)
        REAL SUCCHDEV                    ! Horizontal Development Well Success Rate by Region (FRACTION)

        COMMON /CONSTR/ INVCAP, DRCAP_O, DRCAP_G, EXPCDRCAP, EXPUDRCAP, &
                        NAT_DRCAP_O, NAT_DRCAP_G, NAT_EXPCDRCAP, &
