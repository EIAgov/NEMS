!********************************************************
! OIL SHALE (OGFOR_OS) VARIABLE DECLARATIONS
! Moved from wellogs.f 07/17/14

	REAL OS_GALLON_TON                        ! Oil Shale Per Ton
  	REAL OS_PRJ_SIZE                          ! Oil Shale Plant Size (barrels/day)
  	REAL OS_CONV_EFF                          ! Oil Shale Conversion Efficiency (Percent of oil recovered from rock)
  	REAL OS_MINE_CST_TON                      ! Oil Shale underground mining costs, 2004 dollars per ton of Rock
  	REAL OS_PLANT_INVEST                      ! Oil Shale Retorting and Upgrading Plant Costs, 2004 dollars
  	REAL OS_PLANT_OPER_CST                    ! Oil Shale Plant Operating Costs, 2004 dollars
  	REAL OS_CAP_FACTOR                        ! Oil Shale Plant Capacity Factor (Percent Operating Per Year)
  	REAL OS_TECH_RATE                         ! Oil Shale Project Technological Progress Rate
  	REAL OS_ROYAL_RATE                        ! Oil Shale Royalty Rate (Percent of revenues per year)
  	REAL OS_CORP_TAX_RATE                     ! Oil Shale Corp. Tax Rate, 35% Federal, 3% State
  	REAL OS_EQUITY_SHARE                      ! Oil Shale Equity Percent of Total Capital Structure, Debt Share = 1 - Equity
  	REAL OS_EQUITY_VOL                        ! Oil Shale Equity Volatility Beta
  	REAL OS_EQUITY_PREMIUM                    ! Oil Shale Equity Market Risk Premium (percent)
  	REAL OS_DEBT_PREMIUM                      ! Oil Shale Debt Risk Premium (percent)
  	REAL OS_DISCOUNT_RATE                     ! Oil Shale Discount Rate (Real)
  	REAL OS_MAX_PROD                          ! Oil Shale maximum production (bbls per day)
  	REAL OS_GAS_PRICE                         ! Oil Shale Natural Gas Price for Production, used in DCF Calc
  	REAL OS_GAS_PROD                          ! Oil Shale Gas Production (Mcf per Year at 50,000 bpd)
  	REAL OS_ELEC_PRICE                        ! Oil Shale Electricity Price for Consumption, used in DCF Calc
  	REAL OS_ELEC_CONSUMP                      ! Oil Shale Electricity Consumption Per Year, used in DCF Calc (Kwh per Year at 50,000 bpd)
  	REAL OS_EMETAX                            ! Oil Shale CO2 emissions tax in 2004 dollars per metric ton of CO2, used in DCF Calc
  	REAL OS_CO2EMISS                          ! Oil Shale CO2 emissions (Metric tonnes per day at 50,000 bpd)
  	REAL OS_PENETRATE_YR                      ! Minimum number of years to reach full penetration, used in penetration algorithm (July 2010)
  	REAL OS_EXTRARISK                         ! Risk Premium due to possible carbon legislation

  	INTEGER OS_PRJ_LIFE                       ! Oil Shale Project Life
  	INTEGER OS_PRJ_CONST                      ! Oil Shale Project Construction Period
  	INTEGER OS_START_YR                       ! Earliest calendar year oil shale project can be built
  	INTEGER OS_PLANTS                         ! Number of Oil Shale Plants in Operation and under Construction (July 2010)
  	INTEGER OS_PLANTS_NEW                     ! Number of New Plants being built in single year
  	INTEGER OS_PLANTS_CONST                   ! Number of Oil Shale Plants That Have Gone Into Construction
  	INTEGER OS_MAX_BLD                        ! Maximum number of Oil Shale Plants That can go Into Construction

  	REAL OS_MINE_CST                 	  ! Initial annual mining cost per barrel (2004 $), unaffected by tech progress
  	REAL OS_MINE_CST_BBL(MNUMYR+5)         	  ! Oil shale mining cost per bbl, with technological progress, (2004 $/bbl)
  	REAL OS_INVEST(MNUMYR+5)               	  ! Oil Shale Retorting and Upgrading Investment Cost For Plant Initiated in Year "T", with tech progress
  	REAL OS_OPER_CST(MNUMYR+5)             	  ! Oil Shale Plant Operating Costs for Plant Initiated in Year "T", with tech progress
  	REAL OS_PROJ_OPROD(MNUMYR-10,MNUMYR+5)    ! Oil Shale project production with 25 rows for each year from 2005 through 2030
  	                                                       !             66 columns (to 2055) to permit examination of production past 2030
  	REAL OS_PROJ_GPROD(MNUMYR-10,MNUMYR+5)    ! Oil Shale project natural gas production with 25 rows for each year from 2005 through 2030
  	                                                       !             66 columns (to 2055) to permit examination of production past 2030
  	REAL OS_PROJ_ELCONS(MNUMYR-10,MNUMYR+5)   ! Oil Shale project electricity consumption with 25 rows for each year from 2005 through 2030
  	                                                       !             66 columns (to 2055) to permit examination of production past 2030
  	REAL DCF                         	  ! Net discounted cash flow of project considered in year "TM1"
  	REAL OS_OPROD                    	  ! Total projected Oil Shale production in the current model year
  	REAL OS_GPROD                    	  ! Total projected Natural Gas production in the current model year
  	REAL OS_ELCONS                   	  ! Total projected Electricity consumption in the current model year (kwh per year)
  	REAL OS_DCF                      	  ! Discounted cash flow
  	REAL OS_PROFIT                   	  ! Relative Profitability = OS_DCF/OS_PLANT_INVEST; determines rate of market penetration (July 2010)
  	REAL MAX_PROD                    	  ! Temporary variable based on OS_MAX_PROD and modulated by OS_PROFIT (July 2010)
  	REAL MAX_PROD1                   	  ! Temporary market penetration variable 
	
  	INTEGER OS_YEARS(MNUMYR+5)          	  ! Matrix with years running from 1990 to 2055
  	INTEGER TM1                         	  ! Local year counter set to CURIYR (current year being run), Values = 1,2,3,etc.
  	INTEGER TM2                         	  ! Local year counter which runs through some or all years.
  	INTEGER ROW                         	  ! Number of matrix rows
  	INTEGER COL                         	  ! Number of matrix columns
  	INTEGER OS_WRITE                    	  ! Report writer variable, ON=1, OFF=0
  	
  	COMMON/os1/OS_GALLON_TON,OS_PRJ_SIZE,OS_CONV_EFF,OS_MINE_CST_TON,OS_PLANT_INVEST,OS_PLANT_OPER_CST,OS_CAP_FACTOR, &
  			OS_TECH_RATE,OS_ROYAL_RATE,OS_CORP_TAX_RATE,OS_EQUITY_SHARE,OS_EQUITY_VOL,OS_EQUITY_PREMIUM, &
  			OS_DEBT_PREMIUM,OS_DISCOUNT_RATE,OS_MAX_PROD,OS_GAS_PRICE,OS_GAS_PROD,OS_ELEC_PRICE,OS_ELEC_CONSUMP
  	
  	COMMON/os2/OS_EMETAX,OS_CO2EMISS,OS_PENETRATE_YR,OS_EXTRARISK
  			
  	COMMON/os3/OS_PRJ_LIFE,OS_PRJ_CONST,OS_START_YR,OS_PLANTS,OS_PLANTS_NEW,OS_PLANTS_CONST,OS_MAX_BLD
  	
  	COMMON/os4/OS_MINE_CST,OS_MINE_CST_BBL,OS_INVEST,OS_OPER_CST,OS_PROJ_OPROD,OS_PROJ_GPROD, &
  			OS_PROJ_ELCONS,DCF,OS_OPROD,OS_GPROD,OS_ELCONS,OS_PROFIT,MAX_PROD,MAX_PROD1
  			
  	COMMON/os5/OS_YEARS,TM1,TM2,ROW,COL,OS_WRITE
