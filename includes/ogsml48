!from taxes.h
!  taxes header file
      real xroy               !federal royalty rate

      real fedrate            !federal income tax rate
      real ipdr               !Independent Producer Depletion Rate
      logical cidc            !Capitalize Intangible Drilling Cost?
      logical coi             !Capitalize Other Intangibles?
      logical envscn          !flag for environmental scenario
      logical ce              !Capitalize Environmentals?
      logical amt             !Alternative Minimum Tax?
      logical eortc           !EOR TAX CREDIT ?
      logical credamt         !Flag that allows AMT to be credited in future years
      real smar               !Six Month Amortization Rate (%)
      real ipd                !Intangible drilling cost Preference Deduction
      real acer               !ACE Rate
      real ira                !max alt. min. tax reduction for independents
      real amtrate            !Altermative Minimum Tax RATE
      logical eec             !Expense Environomental Costs?
      logical nil             !Net Income Limitations?
      real nill               !Net Income Limitation Limit
      real pdr                !Percent Depletion Rate (%)
      real piic               !Percent of Intan. Inv. to Capitalize
      real eortcr             !EOR Tax Credit Rate
      real eortcab            !EOR Tax Credit Rate addback %
      real eortcrp(MAX_YR)    !EOR Tax Credit Applied by yr%
      real eortcp             !EOR Tax Credit phase out oil price
      logical ggctc           !G&G Tangible Tax Credit (Depleted)
      real ggctcr             !G&G Tangible Tax Credit Rate (Depleted)
      real ggctcab            !G&G Tangible Tax Credit Rate addback %
      logical ggetc           !G&G Intangible Tax Credit (Expensed)
      real ggetcr             !G&G Intangible Tax Credit Rate (Expensed)
      real ggetcab            !G&G Intangible Tax Credit Rate addback %
      logical lactc           !Lease Acq. Tangible Tax Credit (Depleted)
      real lactcr             !Lease Acq. Tangible Tax Credit Rate (Depleted)
      real lactcab            !Lease Acq. Tangible Tax Credit Rate addback %
      logical laetc           !Lease Acq. Intangible Tax Credit (Expensed)
      real laetcr             !Lease Acq. Intangible Tax Credit Rate (Expensed)
      real laetcab            !Lease Acq. Intangible Tax Credit Rate addback %
      logical tdtc            !Tangible Development Tax Credit?
      real tdtcr              !Tangible Development Tax Credit Rate
      real tdtcab             !Tangible Development Tax Credit Rate addback%
      logical idctc           !Intangible Drilling Cost Tax Credit
      real idctcr             !Intangible Drilling Cost Tax Credit Rate
      real idctcab            !Intangible Drilling Cost Tax Credit Rate addback %
      logical oitc            !Other Intangible Tax Credit?
      real oitcr              !Other Intangible Tax Credit Rate
      real oitcab             !Other Intangible Tax Credit Rate addback %
      logical ettc            !Environmental Tangible Tax Credit?
      real ettcr              !Environmental Tangible Tax Credit Rate
      real ettcab             !Environmental Tangible Tax Credit Rate addback %
      logical eitc            !Environmental Intangible Tax Credit?
      real eitcr              !Environmental Intangible Tax Credit Rate
