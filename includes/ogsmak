!  ALASKA INPUT DIMENSIONS - last CHANGED 7-20-11

      INTEGER KNOFLD(AKRGN)              ! NUMBER OF KNOWN FIELDS NOT DEVELOPED

!  ALASKA INPUT VARIABLES

      INTEGER*4  AKBYR                   ! START YEAR FOR AK MODULE

      REAL FEDTXR_AK                     ! FEDERAL CORP TAX
      REAL STTXAK                        ! STATE CORP TAX
      REAL FSZCOAK(AKSIZE,AKRGN)         ! CRUDE OIL UNDISC. FIELD SIZES
      REAL FSZNGAK(AKSIZE,AKRGN)         ! NATURAL GAS UNDISC. FIELD SIZES
      REAL DRILLAK(AKWELL,AKRGN,AKFUEL)  ! BASELINE DRILLING COST
      REAL DRLNFWAK(AKRGN,AKFUEL)        ! BASELINE NFW DRILLING COST
      REAL FACILAK(9)                    ! BASELINE FACILITY COST
      REAL EQUIPAK(AKWELL,AKRGN,AKFUEL)  ! BASELINE LEASE EQUIPMENT COST
      REAL OPERAK(4,2)                   ! BASELINE OPERATING COST
      REAL DRYAK(AKWELL,AKRGN,AKFUEL)    ! DRY HOLE COST
      INTEGER PRJAK(AKFUEL)              ! EXPECTED LIFE OF PROJECT BY FUEL TYPE
      INTEGER NFW_AK(AKRGN,AKFLD)        ! DRILLING SCHEDULE FOR NFW'S
      INTEGER EXP_AK(AKRGN,AKFLD)        ! DRILLING SCHEDULE FOR EXPL WELLS
      INTEGER DEV_AK(AKRGN,AKFLD)        ! DRILLING SCHEDULE FOR DEVEL WELLS
      INTEGER RCPRDAK(2)                 ! RECOVERY PERIOD FOR IDC'S & TDC'S
      REAL SRAK(AKWELL,AKRGN,AKFUEL)     ! SUCCESS RATE
      REAL TECHAK(COSTCAT)               ! TECHNOLOGY ADJUSTMENT FACTORS
      REAL POLCTECHAK(AKFUEL)            ! POLICY TECHNOLOGY ADJUSTMENT FACTORS
      REAL WELLSAK(AKWELL,AKRGN,AKFUEL)  ! NUM WELLS DRILLED
      REAL NS_OFF_NFW(41)                ! NORTH ALASKA FEDERAL OFFSHORE NEW FIELD WILDCAT
                                         !    PERCENTAGE AS A FUNCTION OF TOTAL NORTH ALASKA NEW FIELD WILDCATS

!  INPUTS FOR KNOWN FIELDS

      REAL RECRES(AKFLD)                 ! RECOVERABLE RESERVES
      REAL MAXPRO(AKFLD)                 ! MAXIMUM PRODUCTION PER DAY
      REAL DECLPRO(AKFLD)                ! DECLINE RATES
      REAL AKREGPER(AKFLD,AKRESRGN)      ! PERCENTAGE OF THE OIL/GAS FIELD THAT IS ONSHORE, STATE OFFSHORE, AND FEDERAL OFFSHORE (9-1-09)

      REAL HISTPRDCO(AKFLD,AKHYR)        ! HISTORICAL PRODUCTION
      REAL PROWELLS(AKFLD,AKHYR)         ! HIST. NO. OF PRODUCING WELLS
      REAL STEO(AKFLD,6)                 ! STEO PRODUCTION PROJECTION
      INTEGER PROYR(AKFLD)               ! START YEAR OF KNOWN FIELDS

!  INPUTS/OUTPUTS FOR PRODUCTION, RESERVE AND RESOURCE ACCOUNTING (8-19-09)

      REAL AKRESOURCES(MNUMYR,AKRESTYPE,AKRESRGN,AKFUEL)   ! RESOURCE TYPES (AKRESTYPE): 1=RESERVES, 2=INFERRED RESOURCES, 3=UNDISCOVERED RESOURCES
                                                           ! RESOURCE REGIONS (AKRESRGN): 1=ONSHORE, 2=OFFSHORE STATE, 3=FEDERAL OFFSHORE
                                                           ! AT THE BEGINNING OF YEAR T
      REAL AKHISTRES((AKHYR-1),AKRESTYPE,AKRESRGN,AKFUEL)  ! HISTORICAL RESOURCE PROFILE, Reserves history 1 less year than current history
                                                           !
      REAL TOTCOPRODREG(MNUMYR,AKRESRGN)                   ! AK OIL PRODUCTION, TOTAL FOR THE RESOURCE REGION AT TIME=CURIYR
      REAL TOTNGPRODREG(MNUMYR,AKRESRGN)                   ! AK GAS PRODUCTION, TOTAL FOR THE RESOURCE REGION AT TIME=CURIYR 

!  INPUTS FOR PRESENT VALUE CALCULATIONS

      REAL COPRDAK(AKWELL,AKRGN,AKFUEL)   ! CO-PRODUCT FACTOR
      REAL SEVTXAK(2)                     ! PRODUCTION TAXE RATE
      REAL ADVTXAK(AKRGN)                 !
      REAL XDCKAPAK                       !
      REAL KAPFRCAK(AKWELL)               ! PERCENT DEPRECIABLE DRILLING COSTS
      REAL KAPSPNDAK(AKRGN)               ! OTHER CAPITAL COSTS

      REAL TRANSAK(AKRGN,AKWELL)          ! TRANSPORTATION COSTS

!  COST VARIABLES
      REAL OPCST_AK(4,2)                  ! OPERATING COST

!  INDEX VARIABLES
      INTEGER LAGIYR                      !  YEAR INDEX VARIABLE

!  COST VARIABLES
      REAL DRLCSTAK(AKWELL,AKRGN,AKFUEL)  ! COST OF DRILLING WELL
      REAL NFWCSTAK(AKRGN,AKFUEL)         ! COST OF DRILLING WELL
      REAL LEQCSTAK(AKWELL,AKRGN,AKFUEL)  ! LEASE EQUIPMENT COST

!  AGGREGATED PROFITABILITY INDICIES
      REAL PROFAK(AKWELL)                 ! ALASKA PROF. FOR ALL RGN & FUEL

!  PRESENT VALUE COST VARIABLES
      REAL REVAK(MNUMYR)            ! PV EXPECTED REVENUES
      REAL ROYAK(MNUMYR)            ! PV ROYALTY
      REAL OPERCST(MNUMYR)          ! PV OPERATING COST

!  EXPLORATION AND DEVELOPMENT EXPENDITURES
      REAL SPENDNF       ! NEW FIELD WILDCAT EXPENDITURES
      REAL SPENDDV       ! DEVELOPMENTAL EXPENDITURES
      REAL SPENDEX       ! EXPLORATORY EXPENDITURES

!  PRODUCTION VARIABLES
      REAL PRODD_AK(AKFUEL,AKRGN)      ! PROD FROM DEVELOPMENT PROJECTS
      REAL PRODC_AK(AKFUEL,AKRGN)      !  PROD FROM CURRENT PRODUCING FLDS
      REAL PRODFAC(6,MNUMYR)          ! ALASKA PRODUCTION FACTORS
      REAL OGPRNGAK(AKRGN,MNUMYR)     ! ALASKA PRODUCTION BY REGION
      REAL TOTPRCOAK(MNUMYR)          ! ALASKA PRODUCTION BY REGION
      REAL TOTPRNGAK(MNUMYR)          ! ALASKA PRODUCTION BY REGION
      real akcoprd(MNUMYR,akfld)      ! alaska production by field
      real akngprd(MNUMYR,akfld)      ! alaska production by field

!  ALASKA RESOURCES
      REAL BOYRESNGAK(AKRGN,MNUMYR)   ! AK NG RESOURCES
      REAL BOYRESCOAK(AKRGN,MNUMYR)   ! AK CO RESOURCES
      REAL RESADNGAK(AKRGN,MNUMYR)    ! RESERVE ADDITIONS AK NG
      REAL RESADCOAK(AKRGN,MNUMYR)    ! RESERVE ADDITIONS AK CO
      REAL RTECHAK(AKFUEL)            ! RESERVE TECHNOLOGY FACTORS
      REAL POLRTECHAK(AKFUEL)         ! POLICY TECHNOLOGY ADJUSTMENT FACTORS
      REAL RECADJAK                   ! MDR HI/LO RECOVERY CASE ADJ FACTOR
      REAL HISTRESCOAK(AKRGN,AKHYR)   ! AK HIST CO BOY RESERVES BY REGION
      REAL HISTADCOAK(AKRGN,AKHYR)    ! AK HIST CO RESERVES ADD BY REGION
      REAL HISTADNGAK(AKRGN,AKHYR)    ! AK HIST NG RESERVES ADD BY REGION
      REAL AKNGRES                    ! INTERNAL ALASKA PIPELINE

!  OTHER ALASKA VARIABLES
      REAL WOP(MNUMYR)                  ! WORLD OIL PRICE FOR NEW EXPLORATORY WELL CALCULATIONS
      REAL DEVFLD(AKFUEL,AKFLD,AKRGN)  ! DEVELOPMENT PROJECTS
      REAL DEVBGN(AKFUEL,AKFLD,AKRGN)  ! START YEAR OF DEV PROJECT
      REAL DEVMAX(AKFUEL,AKFLD,AKRGN)  ! MAX PROD OF DEV PROJECT
      INTEGER NDP(AKFUEL,AKRGN)         ! NUMBER OF DEVELOPMENT PROJECTS
      INTEGER NPF(AKFUEL,AKRGN)         ! NUMBER OF PRODUCING FIELDS

!  ALASKA OIL PIPELINE (TAPS) VARIABLES THAT DETERMINE WHETHER THE OIL PIPELINE IS OPERATING OR NOT OPERATING
      REAL AKWOP(MNUMYR)                ! WORLD OIL PRICE FOR TAPS CALCULATION
      REAL TAPSMINIMUM                  ! MINIMUM THROUGHPUT BELOW WHICH OIL PIPELINE IS SHUTDOWN IN THOUSAND BBLS PER DAY
      REAL TAPSMINOILREV                ! MINIMUM NORTH SLOPE WELLHEAD OIL PRODUCTION REVENUES NECESSARY TO CONTINUE OPERATION (2010 $)
      REAL NSOILREVENUES(MNUMYR)        ! ALASKA NORTH SLOPE OIL REVENUES (2010 $)
      INTEGER TAPSFLAG                  ! LOGICAL VARIABLE AS TO WHETHER OIL PIPELINE IS OPERATING 1=OPERATING, 0=NOT OPERATING

!  ANGTS VARIABLES
      INTEGER ANGTSYR                   !
      REAL ANGTSPRC                     !
      REAL ANGTSRES                     !
      REAL ANGTSMAX                     !

!  MISC VARIABLES
      INTEGER FLDREG(AKFLD)             ! MAPPING OF FIELD TO REGION

      COMMON/OGAK1/STTXAK,FSZCOAK,FSZNGAK,DRILLAK,DRLNFWAK, &
         FACILAK,EQUIPAK,OPERAK,DRYAK,NFW_AK,EXP_AK,DEV_AK, &
         SRAK,TECHAK,WELLSAK,KAPFRCAK,FEDTXR_AK,OPCST_AK, &
         COPRDAK,SEVTXAK,ADVTXAK,KAPSPNDAK,XDCKAPAK, &
         TRANSAK,PRJAK, &
         RECRES,MAXPRO,DECLPRO,HISTPRDCO,PROYR,PROWELLS, &
         AKRESOURCES,AKHISTRES,TOTCOPRODREG,TOTNGPRODREG,AKREGPER, &
         TAPSMINIMUM,AKWOP,WOP,NSOILREVENUES,TAPSMINOILREV

      COMMON/OGAK2/KNOFLD,RCPRDAK

      COMMON/AOGSSC1/LAGIYR

      COMMON/AOGSSC2/DRLCSTAK,LEQCSTAK, &
                     PROFAK,NFWCSTAK

      COMMON/AOGSSC3/REVAK,ROYAK,OPERCST

      COMMON/AOGSSC4/PRODFAC

      COMMON/AOGSSC5/RESADNGAK,RESADCOAK,BOYRESNGAK,BOYRESCOAK, &
                     HISTRESCOAK,HISTADCOAK,HISTADNGAK,AKNGRES

      COMMON/AOGSSC6/DEVFLD,DEVBGN,NDP,NPF,SPENDNF,SPENDDV,SPENDEX, &
                     DEVMAX,PRODD_AK,PRODC_AK,TOTPRCOAK, &
                     OGPRNGAK,TOTPRNGAK

      COMMON/AOGSSC7/ANGTSYR,ANGTSPRC,ANGTSRES,ANGTSMAX
      COMMON/AOGSSC8/POLCTECHAK
      COMMON/AOGSSC8B/RTECHAK,POLRTECHAK
      COMMON/AOGSSC10/STEO
      COMMON/AOGSSC11/RECADJAK
      common/aogssc12/akcoprd,akngprd,NS_OFF_NFW
      common/aogssc13/fldreg,TAPSFLAG

      COMMON/AOGSMBYR/AKBYR
