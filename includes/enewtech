! $Header: m:/default/includes/RCS/enewtech,v 1.5 2017/05/19 19:29:00 rsc Exp $
!CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
!     ENEWTECH CONTAINS TECHNOLOGICAL OPTIMISM AND LEARNING FACTORS
!     DETERMINED IN THE NEW TECHNOLOGIES SUBMODULE
!CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
      COMMON /ENEWTECH/ UPLROPT,UPLRLC,UPLROPTC,UPLRLCC,B_CCST_ADJ    ! REAL
!
      INTEGER ESIZ$NEW
      PARAMETER (ESIZ$NEW = 3 * 4 * ECP_D_CAP + 2 * 4 * ECP$COMP)
!CC
!CC  VARIABLE DECLARATION AND DEFINITIONS
!CC
!     DISPATCHABLE TECHNOLOGIES
!
      REAL*4    UPLROPT(ECP_D_CAP)       ! OPTIMISM FACTOR - avg for plt type
      REAL*4    UPLRLC(ECP_D_CAP)        ! LEARNING FACTOR - avg for plt type
      REAL*4    UPLROPTC(ECP$COMP)       ! OPTIMISM FACTOR BY COMPONENT
      REAL*4    UPLRLCC(ECP$COMP)        ! LEARNING FACTOR BY COMPONENT
      REAL*4    B_CCST_ADJ(ECP_D_CAP)       ! ADJUSTMENT FACTOR CALCULATED FROM PROGRAM GOALS
