! EFP GENERAL COMMON BLOCK
      INTEGER EIDIST       !PLANT TYPE NUMBER FOR DISTRIBUTION PLANT
      INTEGER EIMISC       !PLANT TYPE NUMBER FOR MISCELLANEOUS PLANT
      INTEGER EITRAN       !PLANT TYPE NUMBER FOR TRANSMISSION PLANT
      INTEGER EIPROD       !HIGHEST PLANT TYPE FOR PRODUCTION PLANT
      INTEGER EICOAL       !PLANT TYPE NUMBER FOR COAL PLANT
      INTEGER EIOG         !PLANT TYPE NUMBER FOR OIL/GAS STEAM PLANT
      INTEGER NNONG        !NUMBER OF NON-GENERATION PLANT TYPES
      INTEGER NPTYP        !NUMBER OF PLANT TYPES
      INTEGER EINUC        !PLANT TYPE NUMBER FOR NUCLEAR PLANT
      INTEGER EICAN        !PLANT TYPE NUMBER FOR CANCELLED PLANT
      INTEGER UF_R         !UNIT NUMBER FOR REGIONAL DAF
      INTEGER UF_RC        !UNIT NUMBER FOR REGIONAL/COMPONENT DAF
      INTEGER UF_RCY       !UNIT NUMBER FOR REGIONAL/COMPONENT/YEAR DAF
      INTEGER UF_STMT      !UNIT NUMBER FOR EFP OUTPUT DAF #1
      INTEGER UF_STM2      !UNIT NUMBER FOR EFP OUTPUT DAF #2
      INTEGER UF_EFPRPT    !UNIT NUMBER FOR EFP OUTPUT REPORT
      INTEGER IOUT         !UNIT NUMBER FOR EFP OUTPUT REPORT
      INTEGER UF_MCS       !UNIT NUMBER FOR VALCAP EFPMCS FILE
      INTEGER UF_NCS       !UNIT NUMBER FOR VALCAP EFPNCS FILE
      INTEGER UF_RGC       !UNIT NUMBER FOR VALCAP REGCOMP FILE
      PARAMETER (EIDIST=14,EITRAN=13,EIPROD=12,NNONG=2, &
                 NPTYP=EIPROD+NNONG,EINUC=10,EICAN=6,EIMISC=7, &
                 EICOAL=1,EIOG=3)
      COMMON/EFPGEN/EOVYRS,EOBKLF,EOTXLF,EODEPR,ESBKLF,ESDEPR, &
                    ESTXLF,ESTXRC,ESLCP,ESCPRF,UF_STM2, &
                    UF_R,UF_RC,UF_RCY,UF_STMT,UF_EFPRPT,IOUT, &
                    UF_MCS,UF_NCS,UF_RGC
      INTEGER EOVYRS       !NUMBER OF VINTAGE-YEAR CLASSES IN EFP
      REAL*4 EOBKLF(14,2)  !BOOK LIFE FOR EXISTING ASSETS
      REAL*4 EOTXLF(14,2)  !TAX LIFE FOR EXISTING ASSETS
      REAL*4 EODEPR(14,2)  !BOOK DEPRECIATION RATES FOR EXISTING ASSETS
      REAL*4 ESBKLF(14,2)  !BOOK LIFE FOR NEW ASSETS
      REAL*4 ESDEPR(14,2)  !BOOK DEPRECIATION RATES FOR NEW ASSETS
      REAL*4 ESTXLF(14,2)  !TAX LIFE FOR NEW ASSETS (ACR)
      REAL*4 ESTXRC(14,2)  !TAX RECOVERY CLASS FOR NEW ASSETS (ACRS)
      REAL*4 ESLCP(14)     !LENGTH OF CONSTRUCTION PROFILES
      REAL*4 ESCPRF(14,15) !CONSTRUCTION PROFILES (YEAR 1=ON LINE YEAR)
