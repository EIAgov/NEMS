!
!     DISPCRV STORES EMM LOAD AND RENEWABLE DISPATCH DATA 
!
      COMMON /DISPCRV/ UTWDTH,UTHGHT,UTPEAK,UTXMKT,UCRNW,UTNMRG, &
        UCRPS,UTHTP2,UTHTS2,UTSEAS,UTNGRP,UTNSEG
!
!     DISPCRV - VARIABLE DECLARATION AND DEFINITIONS
!
      INTEGER*4 UTSEAS(ELD_D_DAY),UTNGRP,UTNSEG
      REAL*8 UTWDTH(EFD_D_SSZ,ELD_D_DAY)
      REAL*8 UTHGHT(EFD_D_SSZ,ELD_D_DAY,MNUMNR)
      REAL*8 UTPEAK(EFD_D_MSP,MNUMNR)
      REAL*8 UTXMKT(EFD_D_SSZ,ELD_D_DAY,MNUMNR)
      REAL*8 UCRNW(EFD_D_SSZ,ELD_D_DAY,MNUMNR)
      REAL*8 UTNMRG(EFD_D_SSZ,ELD_D_DAY,MNUMNR)
      REAL*8 UCRPS(EFD_D_SSZ,ELD_D_DAY,MNUMNR)
      REAL*8 UTHTP2(EFD_D_SSZ,ELD_D_DAY,MNUMNR)
      REAL*8 UTHTS2(EFD_D_SSZ,ELD_D_DAY,MNUMNR)
