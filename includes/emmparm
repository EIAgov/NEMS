!CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
!  EMMPARM DEFINES THE PARAMETERS WHICH DETERMINE THE SIZE OF THE EMM   
!CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
!
      INTEGER*4 ECP_D_CAP    ! ECP TOTAL ALL TYPES DSP, INT, RNW, AND DGN
      INTEGER*4 ECP_D_CIS    ! ECP NUMBER OF CANADIAN IMPORT SUPPLY CURVE STE
      INTEGER*4 ECP_D_CSC    ! ECP MAXIMUM # OF SCRUBBER TYPE CONVERSIONS    
      INTEGER*4 ECP_D_DGN    ! ECP NUMBER OF DISTRIBUTED GENERATION TYPES     
      INTEGER*4 ECP_D_DGS    ! ECP NUMBER OF DIST GEN AVOIDED TRANS. STEPS
      INTEGER*4 ECP_D_DSM    ! ECP NUMBER OF DSM PROGRAM TYPES               
      INTEGER*4 ECP_D_DSP    ! ECP NUMBER OF DISPATCHABLE CAPACITY TYPES     
      INTEGER*4 ECP_D_FPH    ! LENGTH OF FULL PLANNING HORIZON               
      INTEGER*4 ECP_D_FPP    ! ECP FUELS PER PLANT                           
      INTEGER*4 ECP_D_INT    ! ECP NUMBER OF INTERMITTENT CAPACITY TYPES     
      INTEGER*4 ECP_D_I_R    ! ECP NUMBER OF INTERMITTENT CAPACITY TYPES     
      INTEGER*4 ECP_D_LCP    ! ECP LONGEST CONSTRUCTION PROFILE              
      INTEGER*4 ECP_D_LDG    ! ECP NUMBER OF BUILDING BLOCKS PER SEASON      
      INTEGER*4 ECP_D_LINV   ! ECP LENGTH OF DSM INVESTMENT PROFILE          
      INTEGER*4 ECP_D_MSP    ! ECP SEASONAL PERIOD                           
      INTEGER*4 ECP_D_MXP    ! MAXIMUM NUMBER OF IMPORT/EXPORT REGIONS       
      INTEGER*4 ECP_D_NCC    ! # COEFFICIENTS FOR COAL PRICE EXPECTATIONS EQUATIONS
      INTEGER*4 ECP_D_NFL    ! ECP NUMBER OF FUEL TYPES                      
      INTEGER*4 ECP_D_OWN    ! ECP OWNERSHIP TYPE 1 = UTILITY 2 = NON-UTILITY
      INTEGER*4 ECP_D_RNW    ! ECP NUMBER OF OTHER RENEWABLE CAPACITY TYPES  
      INTEGER*4 ECP_D_SCR    ! ECP NUMBER OF CLUSTERS--RETROFIT CANDIDATES   
      INTEGER*4 ECP_D_SGP    ! ECP NUMBER OF PLANT GROUPS / RETROFIT GROUP   
      INTEGER*4 ECP_D_SSZ    ! ECP MAXIMUM NUMBER OF STEPS PER GROUP         
      INTEGER*4 ECP_D_STO    ! ECP NUMBER OF STORAGE CAPACITY TYPES          
      INTEGER*4 ECP_D_STP    ! ECP NUMBER OF LOAD GROUPS (IE. SUMMER/DAYTIME)
      INTEGER*4 ECP_D_VLS    ! ECP NUMBER OF VERTICAL LOAD SEGMENTS          
      INTEGER*4 ECP_D_XPH    ! LENGTH OF EXPLICIT PLANNING HORIZON           
      INTEGER*4 ECP_D_RET    ! MAXIMUM NUMBER OF ECP RETIREMENT GROUPS
      INTEGER*4 ECP_D_RCF    ! MAXIMUM NUMBER OF ECP BIOMASS COFIRING RETROFIT CATEGORIES
      INTEGER*4 ECP_D_CFS    ! MAXIMUM NUMBER OF ECP BIOMASS COFIRING Utilization Options
      INTEGER*4 ECP_D_SSTP  ! NUMBER OF SHORT-TERM ELASTICITY STEPS         
      INTEGER*4 NOX_D_MST    ! MAXIMUM NUMBER OF STATES WITH NOX CAP
      INTEGER*4 NOX_D_GRP    ! MAXIMUM NUMBER OF NOX CAP GROUPS
      INTEGER*4 NOX_D_CTL    ! MAXIMUM NUMBER OF NOX CONTROL OPPORTUNITIES
      INTEGER*4 CO2_D_MST    ! MAXIMUM NUMBER OF STATES WITH CO2 CAP
      INTEGER*4 CO2_D_GRP    ! MAXIMUM NUMBER OF CO2 CAP GROUPS
!
      INTEGER*4 EFD_D_CSS    ! maximum number of Canadian Supply steps
      INTEGER*4 EFD_D_BTM    ! MAX NUMBER OF BOILER BOTTOM TYPES  WAYNE
      INTEGER*4 EFD_D_BTP    ! MAX NUMBER OF BOILER TYPES  WAYNE
      INTEGER*4 EMM_D_BTP    ! MAX NUMBER OF BOILER TYPES INCLUDES BOTTOM TYPE
      INTEGER*4 EFD_D_CAP    ! TOTAL PLANT TYPES DSP AND RNW AND DGN        
      INTEGER*4 EFD_D_CGP    ! COMPLIANCE GROUPS PER PLANT GROUP             
      INTEGER*4 EFD_D_DGN    ! DISTRIBUTED GENERATION CAPACITY TYPES          
      INTEGER*4 EFD_D_DSP    ! DISPATCHABLE CAPACITY TYPES                   
      INTEGER*4 EFD_D_FPP    ! ECP FUELS PER PLANT                           
      INTEGER*4 EFD_D_FRG    ! SUPPLY/REPORTING REGIONS PER FUEL TYPE        
      INTEGER*4 EFD_D_LDG    ! NUMBER OF BUILDING BLOCKS PER SEASON          
      INTEGER*4 EFD_D_MFRG   ! MAXIMUM NUMBER OF REGIONS FOR ANY FUEL TYPE   
      INTEGER*4 EFD_D_MHG    ! RENEWABLE EFD PLANT GROUPS                           
      INTEGER*4 EFD_D_MDG    ! DISTRIBUTED GENERATION EFD PLANT GROUPS                           
      INTEGER*4 EFD_D_MHS    ! HORIZONTAL SLICES                             
      INTEGER*4 EFD_D_MPG    ! DISPATCHABLE EFD PLANT GROUPS                         
      INTEGER*4 EFD_D_MSP    ! SEASONAL PERIODS                              
      INTEGER*4 EFD_D_PROV   ! NUMBER OF INTERNATIONAL PROVINCES             
      INTEGER*4 EFD_D_MVS    ! VERTICAL SLICES (EFD)                         
      INTEGER*4 EMM_D_NCT    ! MAX NUMBER OF NOX CONTROL TECHNOLOGIES
      INTEGER*4 EMS_D_NFL    ! NUMBER FUEL TYPES FOR EMISSION CALCULATIONS
      INTEGER*4 ECP_D_BTP    ! MAXIMUM NUMBER OF BOILER TYPES PER PLANT TYPE
      INTEGER*4 EFD_D_NFL    ! NUMBER FUEL TYPES                             
      INTEGER*4 EFD_D_OWN    ! OWNERSHIP TYPE 1=PRIVATE 2=PUBLIC 3=EWG 4=NT CGN 5=T CGN
      INTEGER*4 EFD_D_RNW    ! RENEWABLE TYPES                               
      INTEGER*4 EFD_D_SO2    ! SO2 COMPLIANCE GROUPS                         
      INTEGER*4 EFD_D_SSZ    ! MAXIMUM NUMBER OF STEPS PER GROUP (EFD)       
      INTEGER*4 EFD_D_STP    ! NUMBER OF LOAD GROUPS PER SEASON (EFD)        
      INTEGER*4 EFD_D_VIN    ! VINTAGE 1=EXISTING, 2=PIPELINE, 3=NEW         
!
      INTEGER*4 EFP_D_CAP    ! FINANCIAL PLANT TYPES DSP, INT, AND RNW       
      INTEGER*4 EFP_D_OWN    ! OWNERSHIP TYPE 1=PRIVATE 2=PUBLIC             
      INTEGER*4 EFP_D_BLD    ! NEW/PIPELINE BUILDS MODELED IN EFP
      INTEGER*4 EFP_D_VIN    ! VINTAGE 1 = 1990, 2 = 1989 ... 40 <= 1949     
!
      INTEGER*4 ELD_D_DAY    ! SEASON / TIME OF DAY GROUPS                   
      INTEGER*4 ELD_D_HRS    ! SEGMENTS IN EACH SEASON/TIME OF DAY GROUPS    
!
      INTEGER*4 ETT_D_MTG    ! MAXIMUM NUMBER OF TRADE GROUPS                
      INTEGER*4 ETT_D_CAN    ! MAXIMUM NUMBER OF CANADIAN TRADE GROUPS
      INTEGER*4 ETT_D_RECS   ! MAXIMUM NUMBER OF RECORDS IN CONSTRAINT FILE  
!
      INTEGER*4 WPLT_D_GRP   ! MAXIMUM NUMBER OF PLANT GROUPS PER REGION     
      INTEGER*4 WPLT_D_REC   ! MAXIMUM NUMBER OF PLANT RECORDS               
      INTEGER*4 WPLT_D_RGN   ! MAXIMUM NUMBER OF PLANT REGIONS               
      INTEGER*4 WPLT_D_BBN   ! MAX # BOILER/BOTTOM/NOX RED COMB PER PLANT RECORD    ! WAYNE
!
      INTEGER*4 MAX_UROWS  ! MAXIMUM NUMBER OF FTAB ROWS USING EMM OUTPUT
      INTEGER*4 DTYP_ROW   ! MAXIMUM NUMBER OF DATA TYPES PER FTAB ROW
      INTEGER*4 KEYS_ROW   ! MAXIMUM NUMBER OF CONDITIONAL KEYS ASSOCIATED WITH AN FTAB ROW
      INTEGER*4 MAX_NDX    ! MAXIMUM NUMBER OF INDEXS NEEDED TO TEST FOR CONDITIONAL KEYS
!
      INTEGER*4 EMM_D_GRP    ! MAXIMUM NUMBER OF PLANT GROUPS
      INTEGER*4 EMM_D_REC    ! MAXIMUM NUMBER OF EMM DATA BASE RECORDS
      INTEGER*4 EMM_D_CMB    ! MAXIMUM NUMBER OF EMM DATA BASE COMBINATIONS
      INTEGER*4 EMM_D_ST     ! MAXIMUM NUMBER OF STATES (LOWER 48 + DC)
      INTEGER*4 MAX_DTYP   ! NUMBER OF DATA TYPES FOR EMM OUTPUT DATABASE 
      INTEGER*4 MAX_KEYS   ! NUMBER OF CONDITIONAL KEYS
      INTEGER*4 MAX_FTAB   ! MAXIMUM NUMBER OF FTAB ROWS PER EMM DATA BASE RECORD
!
      INTEGER*4 MX_RANK    ! Maximum Number of Coal Ranks = 3 (Bit., Sub., Lig.)
      INTEGER*4 MX_SO2T    ! Maximum Number of ECP Coals 1->Bit-Low 2->Bit-Med 3->Bit-Hi 4->Sub-Low 5->Sub-Med 6->Lig
      INTEGER*4 MX_PCTYP   ! Maximum Number of Particulate Control Types = 3 (BagHouse,CSE,Other/None)
      INTEGER*4 MX_SCTYP   ! Maximum Number of SO2 Control Types = 2 (Wet, Dry)
      INTEGER*4 MX_ACI     ! Maximum Number of Active Carbon Injection Options
      INTEGER*4 MX_ACSS    ! Maximum Number of Activated Carbon Supply Steps = 5
      INTEGER*4 MAX_CL     ! Maximum Number of Existing Coal Units = 1800
      INTEGER*4 MX_CNFG    ! Maximum Number of Coal Configurations
      INTEGER*4 MX_ROPT    ! Maximum Number of Retrofit Combinations 
      INTEGER*4 MX_RCMB    ! Maximum Number of Retrofit Components
      INTEGER*4 MX_CL_UNIT ! Maximum Number of Coal Units

      INTEGER*4 MX_STO_INC ! Maximum Number of Storage Increments Allowed
!                                                                       
!  PARAMETERS                                                           
!                                                                       
      PARAMETER(ECP_D_CIS =   2)         ! ECP NUMBER OF CANADIAN IMPORT SUPPLY CURVE STE
      PARAMETER(ECP_D_CSC =   3)         ! ECP MAXIMUM # OF SCRUBBER TYPE CONVERSIONS
      PARAMETER(ECP_D_DGN =   2)         ! ECP NUMBER OF DISTRIBUTED GENERATION TYPES
      PARAMETER(ECP_D_DGS =   6)         ! ECP NUMBER OF DIST GEN AVOIDED TRANS. STEPS
      PARAMETER(ECP_D_DSM =  12)         ! ECP NUMBER OF DSM PROGRAM TYPES
      PARAMETER(ECP_D_DSP =  55)         ! ECP NUMBER OF DISPATCHABLE CAPACITY TYPES
      PARAMETER(ECP_D_FPH =  30)         ! LENGTH OF FULL PLANNING HORIZON
      PARAMETER(ECP_D_FPP =   6)         ! ECP FUELS PER PLANT
      PARAMETER(ECP_D_INT =   9)         ! ECP NUMBER OF INTERMITTENT CAPACITY TYPES
      PARAMETER(ECP_D_LCP =  10)         ! ECP LONGEST CONSTRUCTION PROFILE
      PARAMETER(ECP_D_LDG =   6)         ! ECP NUMBER OF BUILDING BLOCKS PER SEASON
      PARAMETER(ECP_D_LINV = 10)         ! ECP LENGTH OF DSM INVESTMENT PROFILE
      PARAMETER(ECP_D_MSP =   4)         ! ECP SEASONAL PERIOD
      PARAMETER(ECP_D_MXP =   8)         ! MAXIMUM NUMBER OF IMPORT/EXPORT REGIONS
      PARAMETER(ECP_D_NCC =   5)         ! # COEFFICIENTS FOR COAL PRICE EXPECTATIONS EQUATIONS
      PARAMETER(ECP_D_NFL =  15)         ! ECP NUMBER OF FUEL TYPES
      PARAMETER(ECP_D_OWN =   2)         ! ECP OWNERSHIP TYPE 1 = UTILITY 2 = NON-UTILITY
      PARAMETER(ECP_D_RNW =  11)         ! ECP NUMBER OF OTHER RENEWABLE CAPACITY TYPES
      PARAMETER(ECP_D_STO =   3)         ! ECP NUMBER OF STORAGE CAPACITY TYPES
      PARAMETER(ECP_D_SCR =   3)         ! ECP NUMBER OF CLUSTERS--RETROFIT CANDIDATES
      PARAMETER(ECP_D_SGP = 200)         ! ECP NUMBER OF PLANT GROUPS / RETROFIT GROUP
      PARAMETER(ECP_D_SSZ =   4)         ! ECP MAXIMUM NUMBER OF STEPS PER GROUP
      PARAMETER(ECP_D_STP =  12)         ! ECP NUMBER OF LOAD GROUPS (IE. SUMMER/DAYTIME)
      PARAMETER(ECP_D_VLS =  24)         ! ECP NUMBER OF VERTICAL LOAD SEGMENTS
      PARAMETER(ECP_D_XPH =  10)         ! LENGTH OF EXPLICIT PLANNING HORIZON
      PARAMETER(ECP_D_RET =   4)         ! MAXIMUM NUMBER OF ECP RETIREMENT GROUPS
      PARAMETER(ECP_D_RCF =   5)         ! MAXIMUM NUMBER OF ECP BIOMASS COFIRING RETROFIT CATEGORIES
      PARAMETER(ECP_D_CFS =   6)         ! MAXIMUM NUMBER OF ECP BIOMASS COFIRING Utilization Options
      PARAMETER(ECP_D_SSTP =  3)         ! NUMBER OF SHORT-TERM ELASTICITY STEPS
      PARAMETER(ECP_D_CAP =  ECP_D_DSP + ECP_D_INT + ECP_D_RNW + ECP_D_STO + ECP_D_DGN)    ! ECP TOTAL ALL TYPES DSP, INT, RNW, AND DGN
      PARAMETER(ECP_D_I_R =  ECP_D_INT + ECP_D_RNW + ECP_D_STO)       ! ECP NUMBER OF INTERMITTENT CAPACITY TYPES
      PARAMETER(NOX_D_MST = 50)          ! MAXIMUM NUMBER OF STATES WITH NOX CAP
      PARAMETER(NOX_D_GRP = 5)           ! MAXIMUM NUMBER OF NOX CAP GROUPS
      PARAMETER(NOX_D_CTL = 2250)        ! MAXIMUM NUMBER OF NOX CONTROL OPPORTUNITIES
      PARAMETER(CO2_D_MST = 51)          ! MAXIMUM NUMBER OF STATES WITH CO2 CAP
      PARAMETER(CO2_D_GRP = 5)           ! MAXIMUM NUMBER OF CO2 CAP GROUPS
!                                                                       
      PARAMETER(EFD_D_CSS = 5)           ! maximum number of Canadian Supply steps
      PARAMETER(EFD_D_BTM =   3)         ! MAX NUMBER OF BOILER BOTTOM TYPES  WAYNE
      PARAMETER(EFD_D_BTP =   13)        ! MAX NUMBER OF BOILER TYPES  WAYNE
      PARAMETER(EMM_D_BTP =   31)        ! MAX NUMBER OF BOILER TYPES INCLUDES BOTTOM TYPE
      PARAMETER(EFD_D_CGP =   1)         ! COMPLIANCE GROUPS PER PLANT GROUP
      PARAMETER(EFD_D_DGN =   2)         ! DISTRIBUTED GENERATION CAPACITY TYPES
      PARAMETER(EFD_D_DSP =  34)         ! DISPATCHABLE CAPACITY TYPES
      PARAMETER(EFD_D_FPP =   3)         ! ECP FUELS PER PLANT
      PARAMETER(EFD_D_FRG =   3)         ! SUPPLY/REPORTING REGIONS PER FUEL TYPE
      PARAMETER(EFD_D_LDG =   6)         ! NUMBER OF BUILDING BLOCKS PER SEASON
      PARAMETER(EFD_D_MFRG = 25)         ! MAXIMUM NUMBER OF REGIONS FOR ANY FUEL TYPE
      PARAMETER(EFD_D_MHG =1300)         ! RENEWABLE EFD PLANT GROUPS
      PARAMETER(EFD_D_MDG = 600)         ! DISTRIBUTED GENERATION EFD PLANT GROUPS
      PARAMETER(EFD_D_MPG =1200)         ! DISPATCHABLE EFD PLANT GROUPS
      PARAMETER(EFD_D_MSP =   4)         ! SEASONAL PERIODS
      PARAMETER(EFD_D_PROV =  8)         ! NUMBER OF INTERNATIONAL PROVINCES
      PARAMETER(EMM_D_NCT =  20)         ! MAX NUMBER OF NOX CONTROL TECHNOLOGIES
      PARAMETER(EMS_D_NFL =   6)         ! NUMBER FUEL TYPES FOR EMISSION CALCULATIONS
      PARAMETER(ECP_D_BTP =   3)         ! MAXIMUM NUMBER OF BOILER TYPES PER PLANT TYPE
      PARAMETER(EFD_D_NFL =  60)         ! NUMBER FUEL TYPES
      PARAMETER(EFD_D_OWN =   5)         ! OWNERSHIP TYPE 1=PRIVATE 2=PUBLIC 3=EWG 4=NT CGN 5=T CGN
      PARAMETER(EFD_D_RNW =  18)         ! RENEWABLE TYPES
      PARAMETER(EFD_D_SO2 =   2)         ! SO2 COMPLIANCE GROUPS
      PARAMETER(EFD_D_SSZ =   4)         ! MAXIMUM NUMBER OF STEPS PER GROUP (EFD)
      PARAMETER(EFD_D_STP =   3)         ! NUMBER OF LOAD GROUPS PER SEASON (EFD)
      PARAMETER(EFD_D_VIN =   3)         ! VINTAGE 1=EXISTING, 2=PIPELINE, 3=NEW
      PARAMETER(EFD_D_CAP =  EFD_D_DSP + EFD_D_RNW + EFD_D_DGN)     ! TOTAL PLANT TYPES DSP AND RNW AND DGN
      PARAMETER(EFD_D_MHS =   EFD_D_MPG)   ! HORIZONTAL SLICES
!                                                                       
      PARAMETER(EFP_D_CAP =  14)         ! FINANCIAL PLANT TYPES DSP, INT, AND RNW
      PARAMETER(EFP_D_OWN =   2)         ! OWNERSHIP TYPE 1=PRIVATE 2=PUBLIC
      PARAMETER(EFP_D_BLD = 1500)        ! NEW/PIPELINE BUILDS MODELED IN EFP
      PARAMETER(EFP_D_VIN =  40)         ! VINTAGE 1 = 1990, 2 = 1989 ... 40 <= 1949
!                                                                       
      PARAMETER(ELD_D_DAY =   9)         ! SEASON / TIME OF DAY GROUPS
      PARAMETER(ELD_D_HRS =  64)         ! SEGMENTS IN EACH SEASON/TIME OF DAY GROUPS
!
      PARAMETER(EFD_D_MVS =  ELD_D_DAY * EFD_D_SSZ)    ! VERTICAL SLICES (EFD)
!                                                                       
      PARAMETER(ETT_D_MTG = 150)         ! MAXIMUM NUMBER OF TRADE GROUPS
      PARAMETER(ETT_D_CAN = 150)         ! MAXIMUM NUMBER OF CANADIAN TRADE GROUPS
      PARAMETER(ETT_D_RECS =  200)       ! MAXIMUM NUMBER OF RECORDS IN CONSTRAINT FILE
!                                                                       
      PARAMETER(WPLT_D_GRP = 3100)       ! MAXIMUM NUMBER OF PLANT GROUPS PER REGION
      PARAMETER(WPLT_D_REC = 27000)      ! MAXIMUM NUMBER OF PLANT RECORDS
      PARAMETER(WPLT_D_RGN = 28)         ! MAXIMUM NUMBER OF PLANT REGIONS
!
      PARAMETER(MAX_UROWS = 1800)      ! MAXIMUM NUMBER OF FTAB ROWS USING EMM OUTPUT
      PARAMETER(DTYP_ROW = 4)          ! MAXIMUM NUMBER OF DATA TYPES PER FTAB ROW
      PARAMETER(KEYS_ROW = 4)          ! MAXIMUM NUMBER OF CONDITIONAL KEYS ASSOCIATED WITH AN FTAB ROW
      PARAMETER(MAX_NDX =  56)         ! MAXIMUM NUMBER OF INDEXS NEEDED TO TEST FOR CONDITIONAL KEYS
!
      PARAMETER(EMM_D_GRP = 20000)       ! MAXIMUM NUMBER OF PLANT GROUPS
      PARAMETER(EMM_D_REC = 60000)       ! MAXIMUM NUMBER OF EMM DATA BASE RECORDS
      PARAMETER(EMM_D_CMB = 10000)       ! MAXIMUM NUMBER OF EMM DATA BASE COMBINATIONS
      PARAMETER(EMM_D_ST  = 49)          ! MAXIMUM NUMBER OF STATES (LOWER 48 + DC)
      PARAMETER(MAX_DTYP = 42)         ! NUMBER OF DATA TYPES FOR EMM OUTPUT DATABASE
      PARAMETER(MAX_KEYS = 10)         ! NUMBER OF CONDITIONAL KEYS
      PARAMETER(MAX_FTAB = 26)         ! MAXIMUM NUMBER OF FTAB ROWS PER EMM DATA BASE RECORD
!
      PARAMETER(MX_RANK   = 3)         ! Maximum Number of Coal Ranks = 3 (Bit., Sub., Lig.)
      PARAMETER(MX_SO2T   = 6)         ! Maximum Number of ECP Coals 1->Bit-Low 2->Bit-Med 3->Bit-Hi 4->Sub-Low 5->Sub-Med 6->Lig
      PARAMETER(MX_PCTYP  = 3)         ! Maximum Number of Particulate Control Types = 3 (BagHouse,CSE,Other/None)
      PARAMETER(MX_SCTYP  = 2)         ! Maximum Number of SO2 Control Types = 2 (Wet, Dry)
      PARAMETER(MX_ACI    = 7)         ! Maximum Number of Active Carbon Injection Options
      PARAMETER(MX_ACSS   = 5)         ! Maximum Number of Activated Carbon Supply Steps = 5
      PARAMETER(MAX_CL    = 1800)      ! Maximum Number of Existing Coal Units = 1800
      PARAMETER(MX_CNFG   = 144)       ! Maximum Number of Coal Configurations
      PARAMETER(MX_ROPT   = 47)        ! Maximum Number of Retrofit Combinations
      PARAMETER(MX_RCMB   = 6)         ! Maximum Number of Retrofit Components
      PARAMETER(MX_CL_UNIT = 2000)     ! Maximum Number of Coal Units
!
      INTEGER ETT_D_MSP                  ! Maximum Number Seasons for Trade and Transportation Data
      INTEGER ETT_D_GRP                  ! Maximum Number Groups for Trade and Transportation Data
      INTEGER ETT_D_SEG                  ! Maximum Number Segments for Trade and Transportation Data
      PARAMETER(ETT_D_MSP=2)             ! Maximum Number Seasons for Trade and Transportation Data
      PARAMETER(ETT_D_GRP=2)             ! Maximum Number Groups for Trade and Transportation Data
      PARAMETER (ETT_D_SEG = 1)          ! Maximum Number Segments for Trade and Transportation Data
!
      INTEGER ECI_D_MSP                  ! Maximum Number Seasons for Economy Canadian Input Data
      INTEGER ECI_D_GRP                  ! Maximum Number Groups for Economy Canadian Input Data
      INTEGER ECI_D_SEG                  ! Maximum Number Segments for Economy Canadian Input Data
      PARAMETER(ECI_D_GRP=18)            ! Maximum Number Groups for Economy Canadian Input Data
      PARAMETER (ECI_D_SEG = 6)          ! Maximum Number Segments for Economy Canadian Input Data
      PARAMETER(ECI_D_MSP=6)             ! Maximum Number Seasons for Economy Canadian Input Data
!
      INTEGER*4 TC_FUELS               ! Number of Traditional Cogen Fuel / Technology Types
      PARAMETER(TC_FUELS=12)           ! Number of Traditional Cogen Fuel / Technology Types
      INTEGER*4 TC_SECTORS             ! Number of Demand Sectors with Cogen Generation
      PARAMETER(TC_SECTORS=5)          ! Number of Demand Sectors with Cogen Generation
      INTEGER*4 MX_ST_RPS              ! Maximum Number of State RPS Tranches
      PARAMETER(MX_ST_RPS=100)         ! Maximum Number of State RPS Tranches
      INTEGER*4 MX_ST_CODES            ! Maximum Number of States Codes
      PARAMETER(MX_ST_CODES=54)        ! Maximum Number of States Codes
      INTEGER*4 MX_TYPE                ! Number of Utility Types 1) IOU 2) COOP 3) MUNI 4) OTHR
      PARAMETER(MX_TYPE=10)            ! Number of Utility Types 1) IOU 2) COOP 3) MUNI 4) OTHR

      PARAMETER(MX_STO_INC=3)          ! Maximum Number of Storage Increments Allowed

      INTEGER*4 MX_GRDSRC              ! Maximum Number of Grid Resiliency Sources
      PARAMETER(MX_GRDSRC=6)           ! Maximum Number of Grid Resiliency Sources
