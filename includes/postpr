      COMMON /POSTPR2/ LINELOSS,CGRESPF,CGCOMPF,CGINDPF,CSHARE,ISHARE, &
                       RSHARE,IGENGN

      REAL LINELOSS
      REAL CGRESPF
      REAL CGCOMPF
      REAL CGINDPF
      REAL ISHARE(MNUMNR,MNUMCR)
      REAL CSHARE(MNUMNR,MNUMCR)
      REAL RSHARE(MNUMNR,MNUMCR)
      REAL IGENGN(MNUMNR)

      COMMON/CNSTR/CHKGEN,IRGEX,IRGIM,CHK$,PTHRESH1,PTHRESH2,CNSTRNTS,CNSTRNTS_EFD,CNSTRNTS_PREFIRM_EFD,CNSTRNTS_ECP

       REAL CHKGEN(MNUMNR+EFD_D_PROV,MNUMNR+EFD_D_PROV)
       REAL CHK$(MNUMNR+EFD_D_PROV,MNUMNR+EFD_D_PROV)
       REAL PTHRESH1(MNUMYR,MNUMNR+EFD_D_PROV,MNUMNR+EFD_D_PROV)
       REAL PTHRESH2(MNUMYR,MNUMNR+EFD_D_PROV,MNUMNR+EFD_D_PROV)
       REAL CNSTRNTS(ETT_D_MSP,MNUMYR,MNUMNR+EFD_D_PROV,MNUMNR+EFD_D_PROV)
       REAL CNSTRNTS_EFD(EFD_D_MSP,MNUMYR,MNUMNR+EFD_D_PROV,MNUMNR+EFD_D_PROV)
       REAL CNSTRNTS_PREFIRM_EFD(EFD_D_MSP,MNUMYR,MNUMNR+EFD_D_PROV,MNUMNR+EFD_D_PROV)
       REAL CNSTRNTS_ECP(EFD_D_MSP,MNUMYR,MNUMNR+EFD_D_PROV,MNUMNR+EFD_D_PROV)
       INTEGER IRGEX,IRGIM

       COMMON/CANSQZ/ ECANSQZ,CAN_CST_SCMULT,CAN_QTY_SCMULT

       REAL*8 ECANSQZ(EFD_D_PROV,MNUMYR)
       REAL*8 CAN_CST_SCMULT(MNUMYR)                 ! scenario multiplier for canada supply curve costs
       REAL*8 CAN_QTY_SCMULT(MNUMYR)                 ! scenario multiplier for canada supply curve quantities
