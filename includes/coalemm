!     $Header: m:/default/includes/RCS/coalemm,v 1.17 2017/06/16 19:31:09 pkc Exp $
!
!     Coal / EMM Module Data Transfers
!
      INTEGER NDRGG
      PARAMETER(NDRGG = 16)
      INTEGER NCLUT1
      PARAMETER(NCLUT1 = 38)
      INTEGER MX_UNITS
      PARAMETER(MX_UNITS = 1800)
      INTEGER MX_PRF
      PARAMETER(MX_PRF = 500)
      INTEGER MX_SUPPLY_CURVES
      PARAMETER(MX_SUPPLY_CURVES = 14)
      INTEGER MX_INTL_CURVES
 	   PARAMETER(MX_INTL_CURVES = 12)
      INTEGER MX_CPS_CTYPES
      PARAMETER(MX_CPS_CTYPES = 15)
      INTEGER NRANK
      PARAMETER(NRANK = MX_RANK)
      INTEGER NPLNT
      PARAMETER(NPLNT = ECP_D_DSP)
      INTEGER NSTEP
      PARAMETER(NSTEP = MX_ACI+1)
      INTEGER MAX_SO2
      PARAMETER(MAX_SO2 = 2)
      INTEGER EMMCLSZ
      PARAMETER(EMMCLSZ = MNUMYR * NDRGG  * NCLUT1 * 12          + &
                          NSTEP * NRANK * NCLUT1 * MNUMYR * 1    + &
                          NSTEP * NRANK * NCLUT1 * 3             + &
                          MX_ACSS * 2                            + &
                          MNUMYR                                 + &
                          MNUMYR                                 + & 
                          MNUMYR                                 + &
                          MNUMYR                                 + &                          
                          MNUMYR * MAX_SO2                       + &
                          6 * MX_UNITS                           + &
                          (MX_SUPPLY_CURVES + MX_INTL_CURVES) * MX_UNITS            + &
                          (MX_SUPPLY_CURVES + MX_INTL_CURVES) * MX_UNITS            + &
                          (MX_SUPPLY_CURVES + MX_INTL_CURVES) * MX_UNITS            + &
                          MX_UNITS                               + &
                          MX_UNITS                               + &
                          MX_UNITS                               + &  
                          MX_UNITS                               + &
                          MX_UNITS                               + &                
                          MX_UNITS                               + &                        
                          7 * 1000                               + &
                          NDRGG * 2                              + &
                          MX_PRF * MNUMYR                        + &
                          MX_PRF * MNUMYR                        + &
                          MX_PRF * MNUMYR                        + &
                          MX_PRF * MNUMYR                        + &
                          MX_UNITS * MNUMYR                      + &
                          MX_UNITS * MNUMYR                      + &
                          MX_UNITS * MNUMYR                      + &
                          NCOALTYP * NSREG                       + &
                          MX_CPS_CTYPES * NSREG                  + &
                          MX_SUPPLY_CURVES                       + &
                          MX_SUPPLY_CURVES                       + &
                          MX_SUPPLY_CURVES                       + &
                          ECP_D_CAP * NRANK                        + &
                          ECP_D_CAP * NRANK                        + &
                          ECP_D_CAP * NRANK                        + & 
                          ECP_D_CAP * NRANK                        + & 
                          ECP_D_CAP                                + &
                          ECP_D_CAP                                + &
                          ECP_D_CAP                                + &
                          NCLUT1 * NRANK                         + &
                          NCLUT1 * NRANK                         + &
                          NCLUT1 * NRANK                         + &
                          NCLUT1 * NRANK                         + &
                          NCLUT1 * NRANK                         + &
                          NCLUT1 * NRANK                         + &
                          10                                     + &
                          MX_UNITS                               + & ! E_PTP                          
                          2000                                   + & ! EG_RY
                          2000                                   + & ! EG_IGRP
                          1                                      + & ! EG_PLTS
                          2000                                   + & ! EG_RG
                          2000                                     ) ! EG_PTP



!
!     Information to Connect EMM Plant File and CMM Contract File via Coal Units
!
      CHARACTER*10 EMM_CL_UNITS(MX_UNITS)                 ! Plant ID and Unit ID Combination From EMM
      CHARACTER*10 CMM_CL_UNITS(MX_UNITS)                 ! Plant ID and Unit ID Combination From CMM
      REAL*4       R_CL_UNITS(5*MX_UNITS)                 ! Equivalence For Storage in Restart File

!
      EQUIVALENCE(R_CL_UNITS,EMM_CL_UNITS)
!
      REAL*4       CMM_CL_BTUS((MX_SUPPLY_CURVES + MX_INTL_CURVES),MX_UNITS)    ! Historical Coal Demands for Each Existing Coal Unit and Supply Curve (domestic & int'l)
      INTEGER*4    CMM_CONT_INDX((MX_SUPPLY_CURVES  + MX_INTL_CURVES),MX_UNITS)  ! Identify Contract Profile Associated with Each Coal Units and Supply Curve (domestic & int'l)
      INTEGER*4    CMM_TRANS_INDX((MX_SUPPLY_CURVES  + MX_INTL_CURVES),MX_UNITS) ! Identify Transportation Profile Associated with Each Coal Units and Supply Surve
      INTEGER*4    CMM_SDV_INDX(MX_UNITS)                    ! Identify Subbitminous Diversity Profile Associated with Each Coal Units
      INTEGER*4    CMM_LDV_INDX(MX_UNITS)                    ! Identify Lignite Diversity Profile Associated with Each Coal Units
      INTEGER*4    NEW_SDV_INDX(NDRGG)                       ! Identify Subbitminous Diversity Profile Associated with New or Unidentified Coal Units 
      INTEGER*4    NEW_LDV_INDX(NDRGG)                       ! Identify Lignite Diversity Profile Associated with New or Unidentified Coal Units
      REAL*4       CMM_CONT_PRF(MX_PRF,MNUMYR)               ! Identify Contract Profile Associated with Each Coal Units
      REAL*4       CMM_TRANS_PRF(MX_PRF,MNUMYR)              ! Identify Transportation Profile Associated with Each Coal Units
      REAL*4       CMM_SDV_PRF(MX_PRF,MNUMYR)                ! Identify Subbitminous Diversity Profile Associated with Each Coal Units
      REAL*4       CMM_LDV_PRF(MX_PRF,MNUMYR)                ! Identify Lignite Diversity Profile Associated with Each Coal Units
      INTEGER*4    EMM_CL_ECPT(MX_UNITS,MNUMYR)              ! ECP Type for Each Coal Unit in Each Forecast Year
      INTEGER*4    EMM_CL_CLRG(MX_UNITS)                     ! Coal Region for Each Coal Unit
      REAL*4       EMM_CL_BTUs(MX_UNITS,MNUMYR)              ! BTUs of Demand for Each Coal Unit in Each Forecast Year
      REAL*4       EMM_CL_CF(MX_UNITS,MNUMYR)                ! Capacity Factors for Each Coal Unit in Each Forecast Year
      INTEGER*4    NUM_CMM_UNITS                             ! Number of Coal Units
      INTEGER*4    NUM_CONT_PRF                              ! Number of Contract Profiles
      INTEGER*4    NUM_TRANS_PRF                             ! Number of Transportation Profiles
      INTEGER*4    NUM_SDV_PRF                               ! Number of Subbitminous Diversity Profiles
      INTEGER*4    NUM_LDV_PRF                               ! Number of Lignite Diversity Profiles
!
!     MAP CDS Types or CPS Coal Types and Supply Region to Supply Curve and viceversa
!
      INTEGER*4 NUM_SC                                       ! Number of Supply Curves
      INTEGER*4 MAP_CDS_SR_SC(NCOALTYP,NSREG)                ! Map CDS Coal Type and Supply Region to Supply Curve
      INTEGER*4 MAP_CPS_SR_SC(MX_CPS_CTYPES,NSREG)           ! Map CPS Coal Type and Supply Region to Supply Curve
      INTEGER*4 MAP_SC_SR(MX_SUPPLY_CURVES)                  ! Map Supply Curve to Supply Region
      INTEGER*4 MAP_SC_CDS(MX_SUPPLY_CURVES)                 ! Map Supply Curve to CDS Coal Type
      INTEGER*4 MAP_SC_CPS(MX_SUPPLY_CURVES)                 ! Map Supply Curve to CPS Coal Type

! Retrofits collected from ECP to pass to CTS
      REAL*4 E_RY(MX_UNITS)                  ! Year plant retrofitted in ECP
      REAL*4 E_IGRP(MX_UNITS)                ! W_IGRP index for plant retrofitted in ECP
      INTEGER*4 E_PLTS                       ! Number of retrofitted plants from ECP
      REAL*4 E_PTP(MX_UNITS)                 ! Plant type of coal retrofit

! Gas Retrofits collected from ECP to pass to CTS
      REAL*4 EG_RY(2000)                    ! Year plant retrofitted in ECP      
      REAL*4 EG_IGRP(2000)                  ! W_IGRP index for plant retrofitted in ECP
      REAL*4 EG_PLTS                        ! Number of retrofitted plants from ECP           
      REAL*4 EG_RG(2000)                    ! Fuel region of plant retrofitted in ECP
      REAL*4 EG_PTP(2000)                   ! Plant type of gas retrofit

! Retirements collected from ECP to pass to CTS
      REAL*4 R_RY(MX_UNITS)                  ! Year coal plant retired in ECP
      REAL*4 R_IGRP(MX_UNITS)                ! W_IGRP index for coal plant retired in ECP
      INTEGER*4 R_PLTS                        ! Number of retired coal plants from ECP     

! New CCS builds collected from ECP to pass to CTS
      REAL*4 N_RY(1000)                    ! Year plant built in ECP
      REAL*4 N_IGRP(1000)                  ! W_IGRP index for plant bult in ECP
      INTEGER*4 N_PLTS                        ! Number of new builds from ECP
      REAL*4 N_RG(1000)                    ! Region (NEMS or Fuel region?) of new build in ECP
      REAL*4 N_HRAT(1000)                      ! Heatrate of new CCS build
      REAL*4 N_CPTY(1000)                     ! Capacity  of new CCS build
      REAL*4 N_CFR(1000)                      ! Capacity Factor of new CCS build
      REAL*4 N_PTP(1000)                     ! Plant type of new CCS build     
      REAL*4 C_DR(MNUMYR)                    ! Discount Rate - Capture
      REAL*4 T_DR(MNUMYR)                    ! Discount Rate - Transport
      REAL*4 S_DR(MNUMYR)                    ! Discount Rate - Storage
      
      
!
!     Coal Demand at Coal Demand Regions by ECP Plant Type (Trillions of Btu)
!
      REAL QCLB1NR(NDRGG,MNUMYR) ! Demand for Coal at Unscb-BH -Any-      Coal Generators - ECP Code B1
      REAL QCLB2NR(NDRGG,MNUMYR) ! Demand for Coal at Unscb-BH -Any-SC    Coal Generators - ECP Code B2
      REAL QCLB3NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-BH -   -      Coal Generators - ECP Code B3
      REAL QCLB4NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-BH -   -SC    Coal Generators - ECP Code B4
      REAL QCLB5NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-BH -SCR-      Coal Generators - ECP Code B5
      REAL QCLB6NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-BH -SCR-SC    Coal Generators - ECP Code B6
      REAL QCLB7NR(NDRGG,MNUMYR) ! Demand for Coal at Dry  -BH -Any-      Coal Generators - ECP Code B7
      REAL QCLB8NR(NDRGG,MNUMYR) ! Demand for Coal at Dry  -BH -Any-SC    Coal Generators - ECP Code B8
      REAL QCLC1NR(NDRGG,MNUMYR) ! Demand for Coal at Unscb-CSE-Any-      Coal Generators - ECP Code C1
      REAL QCLC2NR(NDRGG,MNUMYR) ! Demand for Coal at Unscb-CSE-Any-   FF Coal Generators - ECP Code C2
      REAL QCLC3NR(NDRGG,MNUMYR) ! Demand for Coal at Unscb-CSE-Any-SC/FF Coal Generators - ECP Code C3
      REAL QCLC4NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-CSE-   -      Coal Generators - ECP Code C4
      REAL QCLC5NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-CSE-   -   FF Coal Generators - ECP Code C5
      REAL QCLC6NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-CSE-   -SC/FF Coal Generators - ECP Code C6
      REAL QCLC7NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-CSE-SCR-      Coal Generators - ECP Code C7
      REAL QCLC8NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-CSE-SCR-   FF Coal Generators - ECP Code C8
      REAL QCLC9NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-CSE-SCR-SC/FF Coal Generators - ECP Code C9
      REAL QCLCXNR(NDRGG,MNUMYR) ! Demand for Coal at D_scb-CSE-   -      Coal Generators - ECP Code CX
      REAL QCLCYNR(NDRGG,MNUMYR) ! Demand for Coal at D_scb-CSE-   -   FF Coal Generators - ECP Code CY
      REAL QCLCZNR(NDRGG,MNUMYR) ! Demand for Coal at D_scb-CSE-SCR-SC/FF Coal Generators - ECP Code CZ
      REAL QCLH1NR(NDRGG,MNUMYR) ! Demand for Coal at Unscb-O/N-Any-      Coal Generators - ECP Code H1
      REAL QCLH2NR(NDRGG,MNUMYR) ! Demand for Coal at Unscb-O/N-Any-   FF Coal Generators - ECP Code H2
      REAL QCLH3NR(NDRGG,MNUMYR) ! Demand for Coal at Unscb-O/N-Any-SC/FF Coal Generators - ECP Code H3
      REAL QCLH4NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-O/N-   -      Coal Generators - ECP Code H4
      REAL QCLH5NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-O/N-   -   FF Coal Generators - ECP Code H5
      REAL QCLH6NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-O/N-   -SC/FF Coal Generators - ECP Code H6
      REAL QCLH7NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-O/N-SCR-      Coal Generators - ECP Code H7
      REAL QCLH8NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-O/N-SCR-   FF Coal Generators - ECP Code H8
      REAL QCLH9NR(NDRGG,MNUMYR) ! Demand for Coal at W_scb-O/N-SCR-SC/FF Coal Generators - ECP Code H9
      REAL QCLHANR(NDRGG,MNUMYR) ! Demand for Coal at D_scb-O/N-Any-      Coal Generators - ECP Code HA
      REAL QCLHBNR(NDRGG,MNUMYR) ! Demand for Coal at D_scb-O/N-Any-   FF Coal Generators - ECP Code HB
      REAL QCLHCNR(NDRGG,MNUMYR) ! Demand for Coal at D_scb-O/N-Any-SC/FF Coal Generators - ECP Code HC
      REAL QCLPCNR(NDRGG,MNUMYR) ! Demand for Coal at New Pulverized Coal Generator       - ECP Code PC
      REAL QCLOCNR(NDRGG,MNUMYR) ! Demand for Coal at Other New Coal Generator            - ECP Code OC
      REAL QCLIGNR(NDRGG,MNUMYR) ! Demand for Coal at Integrated Gas Comb Cycle           - ECP Code IG
      REAL QCLI2NR(NDRGG,MNUMYR) ! Demand for Coal at IGCC with NG-Cofiring               - ECP Code I2
      REAL QCLPQNR(NDRGG,MNUMYR) ! Demand for Coal at IGCC with Partial Seequestration    - ECP Code PQ
      REAL QCLISNR(NDRGG,MNUMYR) ! Demand for Coal at IGCC w/Sequestration                - ECP Code IS
!
!     Percent Removal of SO2 at Coal Demand Regions by ECP Plant Type
!
      REAL RCLB1NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at Unscb-BH -Any-      Coal Generators - ECP Code B1
      REAL RCLB2NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at Unscb-BH -Any-SC    Coal Generators - ECP Code B2
      REAL RCLB3NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-BH -   -      Coal Generators - ECP Code B3
      REAL RCLB4NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-BH -   -SC    Coal Generators - ECP Code B4
      REAL RCLB5NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-BH -SCR-      Coal Generators - ECP Code B5
      REAL RCLB6NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-BH -SCR-SC    Coal Generators - ECP Code B6
      REAL RCLB7NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at Dry  -BH -Any-      Coal Generators - ECP Code B7
      REAL RCLB8NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at Dry  -BH -Any-SC    Coal Generators - ECP Code B8
      REAL RCLC1NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at Unscb-CSE-Any-      Coal Generators - ECP Code C1
      REAL RCLC2NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at Unscb-CSE-Any-   FF Coal Generators - ECP Code C2
      REAL RCLC3NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at Unscb-CSE-Any-SC/FF Coal Generators - ECP Code C3
      REAL RCLC4NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-CSE-   -      Coal Generators - ECP Code C4
      REAL RCLC5NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-CSE-   -   FF Coal Generators - ECP Code C5
      REAL RCLC6NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-CSE-   -SC/FF Coal Generators - ECP Code C6
      REAL RCLC7NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-CSE-SCR-      Coal Generators - ECP Code C7
      REAL RCLC8NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-CSE-SCR-   FF Coal Generators - ECP Code C8
      REAL RCLC9NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-CSE-SCR-SC/FF Coal Generators - ECP Code C9
      REAL RCLCXNR(NDRGG,MNUMYR) ! Percent Removal of SO2 at D_scb-CSE-   -      Coal Generators - ECP Code CX
      REAL RCLCYNR(NDRGG,MNUMYR) ! Percent Removal of SO2 at D_scb-CSE-   -   FF Coal Generators - ECP Code CY
      REAL RCLCZNR(NDRGG,MNUMYR) ! Percent Removal of SO2 at D_scb-CSE-SCR-SC/FF Coal Generators - ECP Code CZ
      REAL RCLH1NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at Unscb-O/N-Any-      Coal Generators - ECP Code H1
      REAL RCLH2NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at Unscb-O/N-Any-   FF Coal Generators - ECP Code H2
      REAL RCLH3NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at Unscb-O/N-Any-SC/FF Coal Generators - ECP Code H3
      REAL RCLH4NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-O/N-   -      Coal Generators - ECP Code H4
      REAL RCLH5NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-O/N-   -   FF Coal Generators - ECP Code H5
      REAL RCLH6NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-O/N-   -SC/FF Coal Generators - ECP Code H6
      REAL RCLH7NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-O/N-SCR-      Coal Generators - ECP Code H7
      REAL RCLH8NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-O/N-SCR-   FF Coal Generators - ECP Code H8
      REAL RCLH9NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at W_scb-O/N-SCR-SC/FF Coal Generators - ECP Code H9
      REAL RCLHANR(NDRGG,MNUMYR) ! Percent Removal of SO2 at D_scb-O/N-Any-      Coal Generators - ECP Code HA
      REAL RCLHBNR(NDRGG,MNUMYR) ! Percent Removal of SO2 at D_scb-O/N-Any-   FF Coal Generators - ECP Code HB
      REAL RCLHCNR(NDRGG,MNUMYR) ! Percent Removal of SO2 at D_scb-O/N-Any-SC/FF Coal Generators - ECP Code HC
      REAL RCLPCNR(NDRGG,MNUMYR) ! Percent Removal of SO2 at New Pulverized Coal Generator       - ECP Code PC
      REAL RCLOCNR(NDRGG,MNUMYR) ! Percent Removal of SO2 at Other New Coal Generator            - ECP Code OC
      REAL RCLIGNR(NDRGG,MNUMYR) ! Percent Removal of SO2 at Integrated Gas Comb Cycle           - ECP Code IG
      REAL RCLI2NR(NDRGG,MNUMYR) ! Percent Removal of SO2 at IGCC with NG-Cofiring               - ECP Code I2
      REAL RCLPQNR(NDRGG,MNUMYR) ! Percent Removal of SO2 at IGCC with Partial Seequestration    - ECP Code PQ
      REAL RCLISNR(NDRGG,MNUMYR) ! Percent Removal of SO2 at IGCC w/Sequestration                - ECP Code IS
!
!     Average SO2 Content of Coal at Coal Demand Regions by ECP Plant Type (lbs/MMBtu)
!
      REAL SCLB1NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to Unscb-BH -Any-      Coal Generators - ECP Code B1
      REAL SCLB2NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to Unscb-BH -Any-SC    Coal Generators - ECP Code B2
      REAL SCLB3NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-BH -   -      Coal Generators - ECP Code B3
      REAL SCLB4NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-BH -   -SC    Coal Generators - ECP Code B4
      REAL SCLB5NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-BH -SCR-      Coal Generators - ECP Code B5
      REAL SCLB6NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-BH -SCR-SC    Coal Generators - ECP Code B6
      REAL SCLB7NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to Dry  -BH -Any-      Coal Generators - ECP Code B7
      REAL SCLB8NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to Dry  -BH -Any-SC    Coal Generators - ECP Code B8
      REAL SCLC1NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to Unscb-CSE-Any-      Coal Generators - ECP Code C1
      REAL SCLC2NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to Unscb-CSE-Any-   FF Coal Generators - ECP Code C2
      REAL SCLC3NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to Unscb-CSE-Any-SC/FF Coal Generators - ECP Code C3
      REAL SCLC4NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-CSE-   -      Coal Generators - ECP Code C4
      REAL SCLC5NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-CSE-   -   FF Coal Generators - ECP Code C5
      REAL SCLC6NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-CSE-   -SC/FF Coal Generators - ECP Code C6
      REAL SCLC7NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-CSE-SCR-      Coal Generators - ECP Code C7
      REAL SCLC8NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-CSE-SCR-   FF Coal Generators - ECP Code C8
      REAL SCLC9NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-CSE-SCR-SC/FF Coal Generators - ECP Code C9
      REAL SCLCXNR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to D_scb-CSE-   -      Coal Generators - ECP Code CX
      REAL SCLCYNR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to D_scb-CSE-   -   FF Coal Generators - ECP Code CY
      REAL SCLCZNR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to D_scb-CSE-SCR-SC/FF Coal Generators - ECP Code CZ
      REAL SCLH1NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to Unscb-O/N-Any-      Coal Generators - ECP Code H1
      REAL SCLH2NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to Unscb-O/N-Any-   FF Coal Generators - ECP Code H2
      REAL SCLH3NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to Unscb-O/N-Any-SC/FF Coal Generators - ECP Code H3
      REAL SCLH4NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-O/N-   -      Coal Generators - ECP Code H4
      REAL SCLH5NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-O/N-   -   FF Coal Generators - ECP Code H5
      REAL SCLH6NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-O/N-   -SC/FF Coal Generators - ECP Code H6
      REAL SCLH7NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-O/N-SCR-      Coal Generators - ECP Code H7
      REAL SCLH8NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-O/N-SCR-   FF Coal Generators - ECP Code H8
      REAL SCLH9NR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to W_scb-O/N-SCR-SC/FF Coal Generators - ECP Code H9
      REAL SCLHANR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to D_scb-O/N-Any-      Coal Generators - ECP Code HA
      REAL SCLHBNR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to D_scb-O/N-Any-   FF Coal Generators - ECP Code HB
      REAL SCLHCNR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to D_scb-O/N-Any-SC/FF Coal Generators - ECP Code HC
      REAL SCLPCNR(NDRGG,MNUMYR) ! Average SO2 Content of Coal to New Pulverized Coal Generator       - ECP Code PC
