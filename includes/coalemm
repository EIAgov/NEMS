!     Mercury Data for Activated Carbon Supply Curve

      REAL    EMM_MEF(NSTEP,NRANK,NCLUT1)            ! Mercury Emission Factor by Plant Type, Coal Rank and Activated Carbon Step(1=>No ACI)
      REAL    ACI_CST(NSTEP,NRANK,NCLUT1)            ! Equipment Cost per Trillion Btu of Coal for Activated Carbon Injection
      REAL    ACI_QAC(NSTEP,NRANK,NCLUT1)            ! Activated Carbon Requirement per Trillion Btu of Coal for Activated Carbon Injection
      REAL    P_AC_SC(MX_ACSS)                       ! Activated Carbon Cost $ per pound for each supply step
      REAL    Q_AC_SC(MX_ACSS)                       ! Activated Carbon Supply pounds per supply step
      INTEGER N_AC_SC                                ! Number of Activated Carbon Supply Steps
      INTEGER ACI_OPT(NSTEP,NRANK,NCLUT1,MNUMYR)     ! Allowed ACI Options (0 => No , 1 => Yes)

!     Mercury Emission Factors and Parameters Required to Calculate Activated Carbon Requirements

      REAL    PLNT_EMF(ECP_D_CAP,NRANK)                ! Emission Modification Factor by Plant Type and Coal Rank
      REAL    MIN_EMF(ECP_D_CAP,NRANK)                 ! Minimum Achievable Emission Modification Factor using ACI
      REAL    ACI_OandM(ECP_D_CAP,NRANK)               ! Incremental O&M for ACI (87$/MWh)
      INTEGER ACI_STEPS(ECP_D_CAP,NRANK)               ! Number of ACI Steps to Create
      INTEGER HG_CHOICE(ECP_D_CAP)                     ! Choice of Input or Output Mercury Standard: 0=>None, 1=>Input Only, 2=>Output Only, 3=>Input or Output
      INTEGER HG_CLASS(ECP_D_CAP)                      ! Mecury Compliance Class : 0=>NA, 1=>Existing Coal, 2=>New Conventional Coal, 3=>New IGCC and Other Coal
