!  COGGLOBL COMMON CONTAINS DUPLICATE VARIABLES FOR ALL OF THE
!  NEMS GLOBAL VARIABLES USED IN OGSM

      INTEGER cMNOGRGN,cMNOGDTP,cMNOGCRO
      PARAMETER (cMNOGCRO=5)  ! Note:  should have same value as MNOGCRO in parametr 
      PARAMETER (cMNOGRGN=10) ! Note:  should have same value as MNOGRGN in ogsmout
      PARAMETER (cMNOGDTP=4)  ! Note:  should have same value as MNOGDTP in ogsmout

      REAL   cCFNGN(MNUMYR)   ! GJ/mcf
      REAL   cCFNGU(MNUMYR)   ! GJ/mcf
      
      REAL   cMC_PCWGDP(-2:MNUMYR)
      REAL   cMC_RLRMPUAANS(MNUMYR)
      REAL   cMC_CPI(MCANRG,MNUMYR)
      REAL   cMC_ECIWSP(MNUMYR)
      REAL   cMC_REALRMGBLUS(MNUMYR)
      REAL   cMC_RMPUAANS(MNUMYR)
      REAL   cMC_REXCUR(-2:MNUMYR) ! Canada- US exchange rate
      REAL   cMC_GDPMPU(-2:MNUMYR) ! US deflators

      REAL   cOGCNPPRD(MCANRG,7,MNUMYR)   
      REAL   cOGCNQPRD(MCANRG,7,MNUMYR)   
      REAL   cOGCNXPRD(MCANRG,7,MNUMYR)    
 
!     REAL   cOGPRRCAN(MCANRG,7,MNUMYR)  
 
      REAL   cOGQNGEXP(MNUMBX,MNUMYR)
      REAL   cOGQNGIMP(MNUMBX,MNUMYR)
      REAL   cOGPNGEXP(MNUMBX,MNUMYR)
      REAL   cOGPNGIMP(MNUMBX,MNUMYR)

      REAL   cOGTAXPREM(2,MNUMYR)

      REAL   cOGCNRESA(MCANRG,7,MNUMYR)    ! CANADIAN OIL & GAS RES ADDS
      REAL   cOGCNWELL(2,MCANRG,7,MNUMYR)  ! CANADIAN OIL & GAS WELLS
      REAL   cOGCNDISC(MCANRG,7,MNUMYR)    ! CANADIAN UNDISCOVERED AND INFERRED
      REAL   cOGCNBYPR(MCANRG,5,MNUMYR)    ! CANADIAN  OIL & GAS byproducts
      REAL   cOGCNEMIS(2,3,MNUMYR)         ! CANADIAN OIL & GAS Emissions
      REAL   cOGRESEOY(MCANRG,7,MNUMYR)    ! CANADIAN EOY Reserves
      REAL   cOGPCRWHP(MNUMYR)              ! CRUDE AVG WELLHEAD PRICE
      REAL   cOGQCRREP(cMNOGCRO,MNUMYR)     ! CRUDE PRODUCTION BY OIL CAT

      REAL   cPNGEL(MCANRG,MNUMYR)
      REAL   cQNGAS(MCANRG,MNUMYR)
      REAL   cRFDCRDP(MCANRG,MNUMYR,5)
      REAL   cRFQDCRD(MCANRG,MNUMYR)
      REAL   cRFQTDCRD(MCANRG+2,MNUMYR)
      REAL   cXDCRDWHP(MCANRG,MNXYR)
      REAL   cXOGWPRNG(MCANRG,MNXYR)
      REAL   cXRFQDCRD(MCANRG,MNUMYR)

      REAL   cIT_WOP(MNUMYR,3)

      COMMON /COGGLOBAL/cCFNGN,cCFNGU, &
         cMC_PCWGDP,cMC_RLRMPUAANS, &
         cOGCNPPRD,cOGCNQPRD,  &       
         cOGPNGEXP,cOGPNGIMP, &
!         cOGPRRCAN, &
         cOGQNGEXP,cOGQNGIMP, &
         cOGCNXPRD, &
         cOGTAXPREM, &
         cPNGEL,cQNGAS,cRFDCRDP,cRFQDCRD,cRFQTDCRD,cXDCRDWHP, &
         cXOGWPRNG,cXRFQDCRD, &
         cMC_CPI,cMC_ECIWSP,cMC_REALRMGBLUS,cMC_RMPUAANS,cIT_WOP, &
         cMC_REXCUR,cMC_GDPMPU, &
         cOGCNRESA, cOGCNWELL, cOGCNDISC, cOGCNBYPR,  &
         cOGCNEMIS, cOGRESEOY, cOGPCRWHP, cOGQCRREP
!         OGQUS2CN, OGPUS2CN, OGQCN2US, OGPCN2US
 
       REAL*4 cCAP_CEN(13,4,MNUMYR)
       REAL*4 cCAP_CEX(13,4,MNUMYR)
       REAL*4 cCAP_NEN(14,4,MNUMYR)
       REAL*4 cCAP_NEX(14,4,MNUMYR)
       REAL   cCFNGC(MNUMYR)

       REAL   cEQGCELGR(MCANRG,MNUMYR)
       REAL   cEQGFELGR(MCANRG,MNUMYR)
       REAL   cEQGIELGR(MCANRG,MNUMYR)
       REAL   cJMGCM(4,MNUMYR)
       REAL   cJNGTR(4,MNUMYR)
      
       REAL*4 cPGCELGR(MCANRG,MNUMYR)
       REAL   cPGFCM(MCANRG,MNUMYR)
       REAL   cPGFEL(MCANRG,MNUMYR)
       REAL*4 cPGFELGR(MCANRG,MNUMYR)
       REAL   cPGFIN(MCANRG,MNUMYR)
       REAL   cPGFRS(MCANRG,MNUMYR)
       REAL   cPGFTR(MCANRG,MNUMYR)
       REAL*4 cPGFTRFV(MCANRG,MNUMYR)
       REAL*4 cPGFTRPV(MCANRG,MNUMYR)
       REAL   cPGICM(MCANRG,MNUMYR)
       REAL   cPGIEL(MCANRG,MNUMYR)
       REAL*4 cPGIELGR(MCANRG,MNUMYR)
       REAL   cPGIIN(MCANRG,MNUMYR)
       REAL   cPGIRS(MCANRG,MNUMYR)
       REAL   cPGITR(MCANRG,MNUMYR)
       REAL   cPMGCM(MCANRG,MNUMYR)
       REAL   cPNGCM(MCANRG,MNUMYR)
       REAL   cPNGIN(MCANRG,MNUMYR)
       REAL   cPNGRS(MCANRG,MNUMYR)
       REAL   cPNGTR(MCANRG,MNUMYR)
       REAL*4 cPRNG_PADD(MCANRR,MNUMYR)
       REAL   cQGCELGR(MCANRG,MNUMYR)
       REAL   cQGFCM(MCANRG,MNUMYR)
       REAL   cQGFEL(MCANRG,MNUMYR)
       REAL   cQGFELGR(MCANRG,MNUMYR)
       REAL   cQGFIN(MCANRG,MNUMYR)
       REAL   cQGFRS(MCANRG,MNUMYR)
       REAL   cQGFTR(MCANRG,MNUMYR)
       REAL*4 cQGFTRFV(MCANRG,MNUMYR)
       REAL*4 cQGFTRPV(MCANRG,MNUMYR)
       REAL   cQGICM(MCANRG,MNUMYR)
       REAL   cQGIEL(MCANRG,MNUMYR)
       REAL   cQGIELGR(MCANRG,MNUMYR)
       REAL   cQGIIN(MCANRG,MNUMYR)
       REAL   cQGIRS(MCANRG,MNUMYR)
       REAL   cQGITR(MCANRG,MNUMYR)
       REAL   cQGPTR(MCANRG,MNUMYR)
       REAL   cOGWPTDM(20,MNUMYR)
       REAL   cQLPIN(MCANRG,MNUMYR)
       REAL   cQNGEL(MCANRG,MNUMYR)
       REAL*4 cSPGFELGR(MCANRG,MNUMYR,2)   ! NNGEM parameter in CN?
       REAL*4 cSPGIELGR(MCANRG,MNUMYR,2)
       REAL   cSQGFELGR(MCANRG,MNUMYR,2)
       REAL   cSQGIELGR(MCANRG,MNUMYR,2)
       REAL*4 cNGCAPS(12,6,MNUMYR)  !(9,6,MNUMYR) for Canada, kept 12 for consistency
       REAL*4 cNGFLOWS(12,6,MNUMYR)
       REAL*4 cNGPIPCAP(2,MNUMYR)
       REAL*4 cNGSTRCAP(32,MNUMYR)


!temp  from US pmmout, need for Canada, values must be in the mini restart
!!!    REAL  AKGTL_NGCNS(MNUMYR)      ! Natural gas consumed in GTL process (bcf)
!!!    REAL  AKGTLPRD(MNUMYR)         ! GTL produced in Alaska (mbcd)
!!!    REAL  AKGTLEXP(MNUMYR)         ! GTL exported from Alaska (mbcd)
       
       INTEGER*4 cNGARCMP(12,6)
       
       COMMON /CNGGLOBAL/cCAP_CEN,cCAP_CEX,cCAP_NEN,cCAP_NEX,cCFNGC, &
          cEQGCELGR,cEQGFELGR,cEQGIELGR,cJMGCM,cJNGTR, &
          cPGCELGR,cPGFCM,cPGFEL, &
          cPGFELGR,cPGFIN,cPGFRS,cPGFTR,cPGFTRFV,cPGFTRPV,cPGICM,cPGIEL, &
          cPGIELGR,cPGIIN,cPGIRS,cPGITR,cPMGCM,cPNGCM,cPNGIN,cPNGRS,cPNGTR, &
          cPRNG_PADD,cQGCELGR,cQGFCM,cQGFEL,cQGFELGR,cQGFIN,cQGFRS,cQGFTR, &
          cQGFTRFV,cQGFTRPV,cQGICM,cQGIEL,cQGIELGR,cQGIIN,cQGIRS,cQGITR, &
          cQGPTR,cOGWPTDM,cQLPIN,cQNGEL,cSPGFELGR,cSPGIELGR, &
          cNGARCMP,cNGCAPS,cNGFLOWS,cNGPIPCAP,cNGSTRCAP, &
          cSQGFELGR,cSQGIELGR

       REAL*4     cRFPSCO(MNUMYR)        ! cost of SCO, 87$/m3
       REAL*4     cRFPGPLC5(MNUMYR)      ! cost of C5+ (baed on DLL), 87$/m3
       REAL*4     cRFPBITBLN(MNUMYR)     ! cost of blended BIT (based on DHV), 87$/m3
       REAL*4     cOGQBITSCO(MNUMYR)     ! qty of syn crude produced by OGSM, MM m3
       REAL*4     cOGQBITC5(MNUMYR)      ! C5+ req for existing in-situ projects  
       REAL*4     cOGQBITBLN(MNUMYR)     ! qty of blended BIT produced by OGSM, MM m3

       COMMON /COGSM/cRFPSCO,cRFPGPLC5,cRFPBITBLN,cOGQBITSCO,cOGQBITC5,cOGQBITBLN
