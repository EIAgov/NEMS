! VARIABLES PASSED BETWEEN THE PTM AND ITM

      COMMON /NGPTMITM/ PTCURPCAP(TNODES,TNODES), PTCURPSTR(STNODES), &
         PTMAXPCAP(TNODES,TNODES),PTMAXPSTR(STNODES), &
         PTPKUTZ(TNODES,TNODES),PTOPUTZ(TNODES,TNODES),PTSTUTZ(STNODES), &
         INTRAREG_TAR(NETWORK,DNODES), PTPIPFLOW(TNODES,TNODES), &
         PTSTRFLOW(STNODES),  PTNETFLOW(NETWORK,TNODES,TNODES), &
         PTPIP_REV(TNODES,TNODES), PTSTR_REV(STNODES), &
         HPTPIPFLOW(ARCS+dnodes,FHISYR:LHISYR-1)

      REAL*4 PTCURPCAP     ! CURRENT BOY PIPELINE CAPACITY
      REAL*4 PTCURPSTR     ! CURRENT BOY STORAGE CAPACITY
      REAL*4 PTMAXPCAP     ! MAXIMUM PIPELINE CAPACITY
      REAL*4 PTMAXPSTR     ! MAXIMUM STORAGE CAPACITY
      REAL*4 INTRAREG_TAR  ! INTRAREGIONAL INTERSTATE TARIFFS
      REAL*4 PTPKUTZ       ! MAXIMUM PEAK PIPELINE UTILIZATION RATE
      REAL*4 PTOPUTZ       ! MAXIMUM OFFPEAK PIPELINE UTILIZATION RATE
      REAL*4 PTSTUTZ       ! MAXIMUM STORAGE UTILIZATION RATE
      REAL*4 HPTPIPFLOW    ! HIST. PIPELINE FLOWS IN CURRENT YEAR
      REAL*4 PTPIPFLOW     ! TOTAL PIPELINE FLOW (primary & secondary)
      REAL*4 PTSTRFLOW     ! STORAGE FLOW (net withdrawals)
      REAL*4 PTNETFLOW     ! PIPELINE FLOW BY NETWORK (primary & secondary)
      REAL*4 PTPIP_REV     ! ACTUAL PIPELINE REVENUE (primary arc only), MILLION 1987$
      REAL*4 PTSTR_REV     ! ACTUAL STORAGE REVENUE BY REGION, MILLION 1987$

