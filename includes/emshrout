!  $Header: N:/default/includes/RCS/emshrout,v 1.4 2000/09/01 14:29:24 XJ2 Exp $
!
!     EMSHROUT CONTAINS THE ADJUSTED CAPACITY ADDITIONS FROM THE
!     MARKET-SHARING ALGORITHM OF THE ECP
!
      COMMON /EMSOUT/ EPMSBLD,EPMSLIM                              ! REAL
!
      REAL*8    EPMSBLD(ECP_D_MXP,ECP_D_CAP,ECP_D_DGS,ECP_D_OWN,ECP_D_XPH) ! ADJ BUILDS
      REAL*8    EPMSLIM(MNUMNR,ECP_D_MXP,ECP_D_CAP,ECP_D_DGS,ECP_D_OWN,ECP_D_XPH) ! BLD LIM
