!   Equivalence for FILER used for data storage

      REAL EQ_MCOUT(MNUMYR+3       &  ! MC_JPGDP(-2:MNUMYR)
      +260*MNUMYR                  &  ! arrays dimensioned (mnumyr)
      +11*MNUMYR*MNUMCR            &  ! arrays dimensioned (mnumcr,mnumyr)
      +26*MNUMYR                   &  ! MC_VEHICLES
      +14*MNUMYR*MNUMCR            &  ! MC_COMMFLSP
      +MNUMCR*MNSICM*MNUMYR        &  ! MC_EMPNA
      +MNUMCR*MNSICM*MNUMYR        &  ! MC_REVIND
      +MNUMCR*MNSICNM*MNUMYR       &  ! MC_REVSER
      +1)                             ! MC_EV_SUCCESS

      EQUIVALENCE (EQ_MCOUT,MC_BOPCRNTAC)

!  Splitting out the variables referenced only by mac.f