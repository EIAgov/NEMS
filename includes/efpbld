!  $Header: N:/default/includes/RCS/efpbld,v 1.2 1999/12/19 20:13:19 DSA Exp $
! UEFP NEW BUILDS ARRAYS
      COMMON/EFPBLD/EBDITC,EBAFDC,EBBKVL,EBASVL,EBBCWP,EBPCAP, &
                    EBRCWP,EBPCST,EBSERP,EBNUM,EBPTYP,EBSYR, &
                    EBYCWP,EBABDE
      REAL*4 EBDITC(EFP_D_BLD,EFP_D_OWN) !DEFERRED INVEST TAX CREDITS
      REAL*4 EBAFDC(EFP_D_BLD,EFP_D_OWN) !AMORTIZATION OF DEFERRED TAX SAVINGS OF DEBT AFUDC
      REAL*4 EBBKVL(EFP_D_BLD,EFP_D_OWN) !BOOK VALUE OF BUILD
      REAL*4 EBASVL(EFP_D_BLD,EFP_D_OWN) !TAX BASIS OF BUILD
      REAL*4 EBBCWP(EFP_D_BLD,EFP_D_OWN) !BOOK VALUE OF CWIP FOR BUILD
      REAL*4 EBRCWP(EFP_D_BLD,EFP_D_OWN) !CWIP IN RATE BASE FOR BUILD
      INTEGER EBPTYP(EFP_D_BLD)        !PLANT TYPE OF BUILD
      INTEGER EBSYR(EFP_D_BLD)         !ON LINE YEAR OF BUILD
      REAL*4 EBPCST(EFP_D_BLD)         !CAPACITY COST ($/KW) OF BUILD (NO AFUDC)
      REAL*4 EBPCAP(EFP_D_BLD,EFP_D_OWN) !CAPACITY OF BUILD
      REAL*4 EBYCWP(EFP_D_BLD,EFP_D_OWN) !DIRECT CONSTRUCTION EXPENDITURE - CURRENT YEAR
      REAL*4 EBSERP(EFP_D_BLD)         !FRACTION OF FIRST YEAR BUILD IS IN SERVICE
      REAL*4 EBABDE(EFP_D_BLD,EFP_D_OWN) !ACCUMULATED BOOK DEPRECIATION IN FIRST YEAR
      INTEGER EBNUM                  !NUMBER OF BUILDS
