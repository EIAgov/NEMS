C                     ::: DCFLCMND 9-20-90 :::
C
C This is used by FLCMND and FLIPMAIN.  The applications system needs
C to take care in using this because there may be conflicts.
C
      CHARACTER*64  VERSN
      PARAMETER    (MAXCMD=50,MAXSW=20)
      CHARACTER*8   COMAND(MAXCMD),SWNAME(0:MAXSW)
      LOGICAL*1                    SWVALU(0:MAXSW)
      PARAMETER     (NRECAP=20)
      CHARACTER*128 CRECAP(NRECAP)
C
      COMMON/FLCSTR/CRECAP,VERSN,COMAND,SWNAME
      COMMON/FLCNUM/NCMND,NCFLIP,NCSYS,NSWSYS,NSWTCH,VINF,SWVALU
C
C             Dictionary
C             ~~~~~~~~~~
C       CRECAP.....Last (NRECAP) procedural commands executed
C       VERSN .....System version (from SYSIN)
C       COMAND.....Command names
C       SWNAME.....Switch names
C
C       NCMND......Number of commands (= NCFLIP+NCSYS)
C       NCFLIP.....Number of FLIP commands
C       NCSYS......Number of system commands
C       NSWSYS.....Number of FLIP switches
C       NSWTCH.....Number of system switches
C       VINF.......FLIP's infinity
C       SWVALU.....Switch values
C
      LOGICAL*1     SWECHO,SWERR,SWMSG
      EQUIVALENCE   (SWECHO,SWVALU(1)),(SWERR,SWVALU(2)),
     1              (SWMSG,SWVALU(3))
C
C               ::: END OF DCFLCMND :::
