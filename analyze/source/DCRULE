C DATES:  6-25-93...INCREASED SOME PARAM VALUES
C         7-05-93...EXPANDED VECTOR TO INCLUDE RANGE
C         4-14-94...INCREASED MAX STACK TO MAX COLS
C                   ADDED NTABS AND NTAB(10)
C
C DECLARATIONS FOR RULEBASE MODULE
C
C                  PARAMETER DICTIONARY
C                  ~~~~~~~~~~~~~~~~~~~~
C    PMXRLP = MAX NUMBER OF RULEBASE LINES IN LOOP
C    PMXRPC = MAX NUMBER OF CHARACTER PARAMS
C    PMXRPI = MAX NUMBER OF INTEGER   PARAMS
C    PMXRPS = MAX NUMBER OF SWITCH    PARAMS
C    PMXRPV = MAX NUMBER OF VALUE     PARAMS
C    PMXRST = MAX STACK SIZE
C    PMXRUP = MAX NUMBER OF USER PARAMS
C    PMXRUL = MAX NUMBER OF RULE FILES (LISTED IN RULBASE.RUL)
C
        PARAMETER (PMXRLP=99, PMXRPC=15, PMXRPI=25, PMXRPV=15,
     1         PMXRST=PMXCOL, PMXRUP=50, PMXRUL=30, PMXRPS=10)
CC                    :...4-24, Changed from 99
C   =================================================================
C
      CHARACTER*128 RULTXT
      CHARACTER*80  RULOOP(PMXRLP)
      CHARACTER*16  RUPVAL(PMXRUP),RBSTAK(PMXRST),VECNAM(-1:PMXROW)
      REAL                                        VECTOR(-1:PMXROW)
CC                                                       :...4-14-94
      CHARACTER*8   RULNAM(PMXRUL),RULINT(PMXRPI),RULCHR(PMXRPC),
     1              RULVAL(PMXRPV),RUPNAM(PMXRUP),RULSWK(PMXRPS)
C
      LOGICAL*1     SWLOOP,SWRDBG
      COMMON/RSWTCH/SWLOOP,SWRDBG
C
      COMMON/RCHRTX/RULTXT
      COMMON/RCHR08/RULNAM,RULINT,RULCHR,RULVAL,RUPNAM,RULSWK
      COMMON/RCHR16/RBSTAK,RUPVAL,VECNAM
      COMMON/RCHR80/RULOOP
      COMMON/RULNUM/VLOOK ,VECTOR,NVECTR,MAXRUL,MAXRUP,MAXRST,ILOOP,
     1              NRLOOP,NRULES,NRUINT,NRUCHR,NRUCOM,NRUPRM,NRUVAL,
     2              NRUSWK,ENDTXT,RULROW,RULCOL,ROWORD,COLORD,RSTCK1,
     X              RSTCK2,MARGIN,INDENT,RPAUSE,RPAUS0,NTABS,NTAB(10)
C                                                      =============
C                                                      ADDED 4-25-94
C                           ::: END DCRULE :::
