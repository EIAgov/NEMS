C                  ::: DCFLIP  7-29-95 :::
C
C Earlier dates deleted
C    4-29-93...increased length of SVALUE (from 32 to 64)
C    1-21-94...increased DICKEY to 32 (was 16)
C               "         "   & DICTXT length to 300 (was 200)
C
C DECLARATIONS FOR FLIP (SUPERSEDES ALL OTHER DC'S IN PREVIOUS VERSIONS)
C
C :::> STRING & CHARACTER DECLARATIONS...FOR R|I2C & STR MGT <:::
C
      PARAMETER     (MAXSTR=40)
C                           :...MAX # STRINGS (INCL. 9 SPECIAL ONES)
      CHARACTER*8   SNAME (MAXSTR)
      CHARACTER*64  SVALUE(MAXSTR)
      CHARACTER*1   CHRNUM(0:9),SDELIM,MASK,ASCII(0:36)
      EQUIVALENCE  (CHRNUM(0),ASCII(0))
      COMMON/FCSCHR/SVALUE,SNAME,SDELIM,MASK,ASCII
      COMMON/FCSNUM/NUMSTR
C
C :::> FILE UNITS + SCREEN PARAMS <:::
C
      LOGICAL*1     SWINCL
      COMMON/FIONUM/TTYIN,TTYOUT,INPFIL,OUTFIL,HLPFIL,INPUT,OUTPUT,
     1              INCFIL,INPLIN,FLNLEN,SCRWTH,SCRLEN,SCWCUR,
     2              EXENUM,SWINCL
C
      CHARACTER*64  EXEFIL
      CHARACTER*8   SYSNAM
      COMMON/FIOCHR/EXEFIL,SYSNAM
C
C FLIP DEBUG
      LOGICAL*1     SWFDBG
      COMMON/FLPDBG/FLPAUS,FPAUS0,SWFDBG
C
C RANDOMIZATION PARAMETERS...NOT NEEDED BY OTHER ROUTINES, EXCEPT
C  MAIN OF FLIP AND FRANDU
       COMMON/FCRNUM/RND0,RND1,RNDVAL
C
C      :::> DICtionary (? command) <:::
C
       PARAMETER (FMXDIC=300)
       CHARACTER*32  DICKEY(0:FMXDIC)
       CHARACTER*72  DICTXT(0:FMXDIC)
       COMMON/FCDCHR/DICTXT,DICKEY
       COMMON/FCDNUM/NUMDIC,MAXDIC
C
C      :::> SETUP (file prefixes and suffixes) <:::
C
       CHARACTER*8   SETKEY(30)
       CHARACTER*48  SETPRE(30)
       CHARACTER*4   SETSUF(30)
       COMMON/FSETDC/SETPRE,SETKEY,SETSUF
       COMMON/FSETN/ MXFSET,NFLSET,PRELEN,SUFLEN
C
C                      ::: END DCFLIP :::
