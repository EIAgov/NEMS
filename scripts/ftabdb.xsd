<?xml version="1.0" encoding="utf-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="FTABDB">
    <xs:complexType>
     <xs:element name="Study" maxOccurs="unbounded">
       <xs:complexType>
         <xs:sequence>
           <xs:element name="StudyID" type="xs:string"/>
           <xs:element name="LongTitle" type="xs:string"/>
           <xs:element name="Case" maxOccurs="unbounded">
             <xs:complexType>
               <xs:sequence>
                 <xs:element name="StudyID"         type="xs:string"/>
                 <xs:element name="CaseDescription" type="xs:string" minOccurs="0"/>
                 <xs:element name="ScenLabel"       type="xs:string" minOccurs="0"/>
                 <xs:element name="Scenario"        type="xs:string"/>
                 <xs:element name="Run"             maxOccurs="unbounded">
                   <xs:complexType>
                     <xs:sequence>
                        <xs:element name="StudyID"  type="xs:string"/>
                        <xs:element name="Scenario" type="xs:string"/>
                        <xs:element name="Datekey"  type="xs:string"/>
                        <xs:element name="ScenDate" type="xs:string"/>
                        <xs:element name="Table" maxOccurs="unbounded">
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="TabKey" type="xs:string"/>
                              <xs:element name="Scenario" type="xs:string"/>
                              <xs:element name="Datekey" type="xs:string"/>
                              <xs:element name="TableNumber" type="xs:string"/>
                              <xs:element name="Title" type="xs:string"/>
                              <xs:element name="SubTitle" type="xs:string"/>
                              <xs:element name="Stubhead" type="xs:string" minOccurs="0"/>
                              <xs:element name="Region" type="xs:string" minOccurs="0"/>
                              <xs:element name="RegionNum" type="xs:int" minOccurs="0"/>
                              <xs:element name="Row" type="RowType" maxOccurs="unbounded"/>
                              <xs:element name="Foot" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                  <xs:sequence>
                                    <xs:element name="TabKey" type="xs:string"/>
                                    <xs:element name="Scenario" type="xs:string"/>
                                    <xs:element name="Datekey" type="xs:string"/>
                                    <xs:element name="FootNum" type="xs:string"/>
                                    <xs:element name="FootNote" type="xs:string"/>
                                  </xs:sequence>
                                </xs:complexType>
                              </xs:element>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                     </xs:sequence>
                   </xs:complexType>
                 </xs:element>
               </xs:sequence>
             </xs:complexType>
           </xs:element>
         </xs:sequence>
       </xs:complexType>
     </xs:element>
     <xs:attribute name="version" type="xs:string"/>
     <xs:attribute name="author" type="xs:string"/>
   </xs:complexType>
  </xs:element>
  
